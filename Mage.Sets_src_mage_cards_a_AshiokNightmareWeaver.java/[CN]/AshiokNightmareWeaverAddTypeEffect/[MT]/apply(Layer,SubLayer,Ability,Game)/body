{
  Permanent creature=game.getPermanent(this.getTargetPointer().getFirst(game,source));
  if (creature != null) {
switch (layer) {
case TypeChangingEffects_4:
      if (sublayer == SubLayer.NA) {
        creature.getSubtype(game).add("Nightmare");
      }
    break;
}
return true;
}
 else {
this.used=true;
}
return false;
}
