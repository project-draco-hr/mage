{
  super(ownerId,28,"Master Healer",Rarity.RARE,new CardType[]{CardType.CREATURE},"{4}{W}");
  this.expansionSetCode="9ED";
  this.subtype.add("Human");
  this.subtype.add("Cleric");
  this.color.setWhite(true);
  this.power=new MageInt(1);
  this.toughness=new MageInt(4);
  Ability ability=new SimpleActivatedAbility(Constants.Zone.BATTLEFIELD,new PreventDamageTargetEffect(Constants.Duration.EndOfTurn,4),new TapSourceCost());
  ability.addTarget(new TargetCreatureOrPlayer());
  this.addAbility(ability);
}
