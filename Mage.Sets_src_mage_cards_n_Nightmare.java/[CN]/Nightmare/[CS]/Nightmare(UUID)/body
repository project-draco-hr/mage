{
  super(ownerId,164,"Nightmare",Rarity.RARE,new CardType[]{CardType.CREATURE},"{5}{B}");
  this.expansionSetCode="10E";
  this.subtype.add("Nightmare");
  this.subtype.add("Horse");
  this.power=new MageInt(0);
