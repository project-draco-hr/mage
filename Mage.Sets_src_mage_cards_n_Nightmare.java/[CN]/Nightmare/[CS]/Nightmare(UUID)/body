  this.addAbility(FlyingAbility.getInstance());
  this.addAbility(new SimpleStaticAbility(Zone.ALL,new SetPowerToughnessSourceEffect(new PermanentsOnBattlefieldCount(filter),Duration.EndOfGame)));
}
