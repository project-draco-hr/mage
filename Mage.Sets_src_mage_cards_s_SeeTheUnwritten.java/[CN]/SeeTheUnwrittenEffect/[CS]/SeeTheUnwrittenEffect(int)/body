{
  super(Outcome.DrawCard);
  this.numberOfCardsToPutIntoPlay=numberOfCardsToPutIntoPlay;
  this.staticText="Reveal the top eight cards of your library. You may put " + (numberOfCardsToPutIntoPlay == 1 ? "a creature card" : "two creature cards") + " from among them onto the battlefield. Put the rest into your graveyard";
}
