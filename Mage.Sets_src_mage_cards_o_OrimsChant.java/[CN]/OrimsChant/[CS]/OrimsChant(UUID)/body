{
  super(ownerId,11,"Orim's Chant",Rarity.RARE,new CardType[]{CardType.INSTANT},"{W}");
  this.expansionSetCode="PLS";
  this.addAbility(new KickerAbility("{W}"));
  this.getSpellAbility().addTarget(new TargetPlayer());
