{
  super(ownerId,setInfo,new CardType[]{CardType.LAND},"");
  this.addAbility(new EntersBattlefieldAbility(new AddCountersSourceEffect(CounterType.CHARGE.createInstance(1)),"with a charge counter on it"));
  this.addAbility(new ColorlessManaAbility());
  Ability ability=new AnyColorManaAbility();
  ability.addCost(new RemoveCountersSourceCost(CounterType.CHARGE.createInstance(1)));
  this.addAbility(ability);
}
