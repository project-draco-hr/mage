{
  super(ownerId,63,"Gnat Alley Creeper",Rarity.UNCOMMON,new CardType[]{CardType.CREATURE},"{2}{R}");
  this.expansionSetCode="DIS";
  this.subtype.add("Human");
  this.subtype.add("Rogue");
