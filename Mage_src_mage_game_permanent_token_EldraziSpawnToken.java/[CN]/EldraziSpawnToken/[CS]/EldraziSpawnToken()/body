{
  super("Eldrazi Spawn","0/1 colorless Eldrazi Spawn creature with \"Sacrifice this creature: Add {1} to your mana pool.\"");
  cardType.add(CardType.CREATURE);
  subtype.add("Eldrazi");
  subtype.add("Spawn");
  power=new MageInt(0);
  toughness=new MageInt(1);
  addAbility(new SimpleActivatedAbility(Zone.BATTLEFIELD,new ManaEffect(Mana.ColorlessMana),new SacrificeSourceCost()));
}
