{
  super(ownerId,140,"Setessan Tactics",Rarity.RARE,new CardType[]{CardType.INSTANT},"{1}{G}");
  this.expansionSetCode="JOU";
  this.color.setGreen(true);
  this.addAbility(new StriveAbility("{G}"));
  this.getSpellAbility().addTarget(new TargetCreaturePermanent(0,Integer.MAX_VALUE));
  Effect effect=new BoostTargetEffect(1,1,Duration.EndOfTurn);
  effect.setText("Until end of turn, any number of target creatures each get +1/+1");
  this.getSpellAbility().addEffect(effect);
  this.getSpellAbility().addEffect(new GainAbilityTargetEffect(new SimpleActivatedAbility(Zone.BATTLEFIELD,new FightTargetSourceEffect(),new TapSourceCost()),Duration.EndOfTurn,"and gain \"T: This creature fights another target creature"));
}
