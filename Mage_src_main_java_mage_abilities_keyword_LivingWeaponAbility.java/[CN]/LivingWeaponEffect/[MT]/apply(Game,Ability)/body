{
  Player controller=game.getPlayer(source.getControllerId());
  if (controller != null) {
    if (super.apply(game,source)) {
      Permanent p=game.getPermanent(this.getLastAddedTokenId());
      if (p != null) {
        p.addAttachment(source.getSourceId(),game);
        return true;
      }
    }
  }
  return false;
}
