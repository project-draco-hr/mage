{
  super(ownerId,setInfo,new CardType[]{CardType.ARTIFACT},"{7}");
  this.supertype.add("Legendary");
  SimpleActivatedAbility ability=new SimpleActivatedAbility(Zone.BATTLEFIELD,new ExileTargetEffect(),new ManaCostsImpl("{W}{U}{B}{R}{G}"));
  ability.addTarget(new TargetPermanent());
  this.addAbility(ability);
  this.addAbility(new PutIntoGraveFromAnywhereSourceAbility(new RevealAndShuffleIntoLibrarySourceEffect()));
}
