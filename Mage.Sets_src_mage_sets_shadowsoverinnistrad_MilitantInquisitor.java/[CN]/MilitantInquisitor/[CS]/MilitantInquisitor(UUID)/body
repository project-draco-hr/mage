{
  super(ownerId,26,"Militant Inquisitor",Rarity.COMMON,new CardType[]{CardType.CREATURE},"{2}{W}");
  this.expansionSetCode="SOI";
  this.subtype.add("Human");
  this.subtype.add("Cleric");
