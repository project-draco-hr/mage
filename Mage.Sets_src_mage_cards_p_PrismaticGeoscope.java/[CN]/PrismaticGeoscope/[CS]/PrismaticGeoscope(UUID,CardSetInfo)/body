{
  super(ownerId,setInfo,new CardType[]{CardType.ARTIFACT},"{5}");
  this.addAbility(new EntersBattlefieldTappedAbility());
  this.addAbility(new DynamicManaAbility(new Mana(0,0,0,0,0,0,1,0),new DomainValue(),new TapSourceCost(),"Add X mana in any combination of colors to your mana pool, where X is the number of basic land types among lands you control."));
}
