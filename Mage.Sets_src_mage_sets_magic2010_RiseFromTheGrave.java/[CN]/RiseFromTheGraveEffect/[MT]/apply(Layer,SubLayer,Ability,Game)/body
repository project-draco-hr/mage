{
  Permanent creature=game.getPermanent(source.getFirstTarget());
  if (creature != null) {
switch (layer) {
case TypeChangingEffects_4:
      if (sublayer == SubLayer.NA) {
        creature.getSubtype().add("Zombie");
      }
    break;
case ColorChangingEffects_5:
  if (sublayer == SubLayer.NA) {
    creature.getColor().setBlack(true);
  }
break;
}
return true;
}
return false;
}
