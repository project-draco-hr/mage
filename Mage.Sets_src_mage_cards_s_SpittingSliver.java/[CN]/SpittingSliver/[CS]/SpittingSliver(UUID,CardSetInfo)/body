{
  super(ownerId,setInfo,new CardType[]{CardType.CREATURE},"{4}{B}");
  this.subtype.add("Sliver");
  this.power=new MageInt(3);
  this.toughness=new MageInt(3);
  Ability gainedAbility=FirstStrikeAbility.getInstance();
  this.addAbility(new SimpleStaticAbility(Zone.BATTLEFIELD,new GainAbilityAllEffect(gainedAbility,Duration.WhileOnBattlefield,filter,"All Sliver creatures have first strike.")));
}
