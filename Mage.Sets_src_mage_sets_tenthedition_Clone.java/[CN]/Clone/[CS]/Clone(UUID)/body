{
  super(ownerId,73,"Clone",Rarity.RARE,new CardType[]{CardType.CREATURE},"{3}{U}");
  this.expansionSetCode="10E";
  this.color.setBlue(true);
  this.subtype.add("Shapeshifter");
  this.power=new MageInt(0);
  this.toughness=new MageInt(0);
  Ability ability=new SimpleStaticAbility(Zone.BATTLEFIELD,new EntersBattlefieldEffect(new CopyPermanentEffect(),"You may have {this} enter the battlefield as a copy of any creature on the battlefield",true));
  this.addAbility(ability);
}
