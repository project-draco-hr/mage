{
  super(ownerId,5,"Boonweaver Giant",Rarity.UNCOMMON,new CardType[]{CardType.CREATURE},"{6}{W}");
  this.expansionSetCode="M15";
  this.subtype.add("Giant");
  this.subtype.add("Monk");
