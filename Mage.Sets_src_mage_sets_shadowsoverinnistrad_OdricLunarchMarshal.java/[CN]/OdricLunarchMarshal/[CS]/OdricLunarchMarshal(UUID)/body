{
  super(ownerId,31,"Odric, Lunarch Marshal",Rarity.RARE,new CardType[]{CardType.CREATURE},"{3}{W}");
  this.expansionSetCode="SOI";
  this.supertype.add("Legendary");
  this.subtype.add("Human");
