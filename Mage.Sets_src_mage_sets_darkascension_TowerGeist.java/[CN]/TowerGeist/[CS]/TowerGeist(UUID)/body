{
  super(ownerId,53,"Tower Geist",Rarity.UNCOMMON,new CardType[]{CardType.CREATURE},"{3}{U}");
  this.expansionSetCode="DKA";
  this.subtype.add("Spirit");
  this.color.setBlue(true);
  this.power=new MageInt(2);
  this.toughness=new MageInt(2);
  this.addAbility(FlyingAbility.getInstance());
  this.addAbility(new EntersBattlefieldTriggeredAbility(new LookLibraryAndPickControllerEffect(new StaticValue(2),false,new StaticValue(1),new FilterCard(),Zone.GRAVEYARD,false,false)));
}
