{
  super(ownerId,setInfo,new CardType[]{CardType.SORCERY},"{1}{W}");
  this.getSpellAbility().addEffect(new ConditionalOneShotEffect(new CreateTokenEffect(new HumanToken(),5),new CreateTokenEffect(new HumanToken(),2),FatefulHourCondition.getInstance(),"Put two 1/1 white Human creature tokens onto the battlefield. If you have 5 or less life, put five of those tokens onto the battlefield instead"));
}
