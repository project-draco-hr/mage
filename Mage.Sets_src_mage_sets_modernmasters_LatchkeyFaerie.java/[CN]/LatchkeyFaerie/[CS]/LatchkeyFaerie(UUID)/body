{
  super(ownerId,50,"Latchkey Faerie",Rarity.COMMON,new CardType[]{CardType.CREATURE},"{3}{U}");
  this.expansionSetCode="MMA";
  this.subtype.add("Faerie");
  this.subtype.add("Rogue");
  this.color.setBlue(true);
  this.power=new MageInt(3);
  this.toughness=new MageInt(1);
  this.addAbility(FlyingAbility.getInstance());
  this.addAbility(new ProwlAbility(this,"{2}{U}"));
  EntersBattlefieldTriggeredAbility ability=new EntersBattlefieldTriggeredAbility(new DrawCardSourceControllerEffect(1),false);
  this.addAbility(new ConditionalTriggeredAbility(ability,ProwlCondition.getInstance(),"When {this} enters the battlefield, if its prowl cost was paid, draw a card."));
}
