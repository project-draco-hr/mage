{
  super(ownerId,51,"Xenograft",Rarity.RARE,new CardType[]{CardType.ENCHANTMENT},"{4}{U}");
  this.expansionSetCode="NPH";
  this.color.setBlue(true);
  this.addAbility(new AsEntersBattlefieldAbility(new XenograftEffect()));
