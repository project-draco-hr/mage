{
  super(ownerId,setInfo,new CardType[]{CardType.LAND},"");
  this.subtype.add("Urza's");
  this.subtype.add("Tower");
  Ability urzaManaAbility=new DynamicManaAbility(Mana.ColorlessMana(1),new UrzaTerrainValue(3),"Add {C} to your mana pool. If you control an Urza's Mine and an Urza's Power-Plant, add {C}{C}{C} to your mana pool instead");
  this.addAbility(urzaManaAbility);
}
