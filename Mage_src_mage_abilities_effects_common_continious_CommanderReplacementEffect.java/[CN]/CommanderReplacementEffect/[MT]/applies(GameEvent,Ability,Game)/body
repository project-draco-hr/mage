{
  if (((ZoneChangeEvent)event).getToZone() == Zone.GRAVEYARD || ((ZoneChangeEvent)event).getToZone() == Zone.EXILED || (alsoLibrary && ((ZoneChangeEvent)event).getToZone() == Zone.LIBRARY)) {
    if (commanderId != null) {
      if (((ZoneChangeEvent)event).getFromZone().equals(Zone.STACK)) {
        Spell spell=game.getStack().getSpell(event.getTargetId());
        if (spell != null) {
          if (commanderId.equals(spell.getSourceId())) {
            return true;
          }
        }
      }
      if (commanderId.equals(event.getTargetId())) {
        return true;
      }
    }
  }
  return false;
}
