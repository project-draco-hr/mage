{
  super(ownerId,11,"Warden of Geometries",Rarity.COMMON,new CardType[]{CardType.CREATURE},"{4}");
  this.expansionSetCode="OGW";
  this.subtype.add("Eldrazi");
  this.subtype.add("Drone");
