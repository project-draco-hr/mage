{
  if (event.getType() == GameEvent.EventType.ZONE_CHANGE) {
    ZoneChangeEvent zEvent=(ZoneChangeEvent)event;
    if (zEvent.getToZone() == Constants.Zone.GRAVEYARD) {
      if (game.getControllerId(event.getTargetId()) != this.getControllerId()) {
        return true;
      }
    }
  }
  return false;
}
