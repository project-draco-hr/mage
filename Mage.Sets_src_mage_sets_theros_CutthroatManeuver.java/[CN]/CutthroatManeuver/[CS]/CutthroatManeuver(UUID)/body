{
  super(ownerId,82,"Cutthroat Maneuver",Rarity.UNCOMMON,new CardType[]{CardType.INSTANT},"{3}{B}");
  this.expansionSetCode="THS";
  this.color.setBlack(true);
  Effect effect=new BoostTargetEffect(1,1,Duration.EndOfTurn);
  effect.setText("Up to two target creatures each get +2/+2");
  this.getSpellAbility().addEffect(effect);
  this.getSpellAbility().addEffect(new GainAbilityTargetEffect(LifelinkAbility.getInstance(),Duration.EndOfTurn,"and gain lifelink until end of turn"));
  this.getSpellAbility().addTarget(new TargetCreaturePermanent(0,2));
}
