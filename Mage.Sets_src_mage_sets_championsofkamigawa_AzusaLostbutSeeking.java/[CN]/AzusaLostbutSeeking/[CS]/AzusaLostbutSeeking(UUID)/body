  this.power=new MageInt(1);
  this.toughness=new MageInt(2);
  this.addAbility(new SimpleStaticAbility(Constants.Zone.BATTLEFIELD,new PlayAdditionalLandsControllerEffect(2,Constants.Duration.WhileOnBattlefield)));
}
