{
  super(ownerId,201,"Azusa, Lost but Seeking",Rarity.RARE,new CardType[]{CardType.CREATURE},"{2}{G}");
  this.expansionSetCode="CHK";
  this.supertype.add("Legendary");
  this.subtype.add("Human");
