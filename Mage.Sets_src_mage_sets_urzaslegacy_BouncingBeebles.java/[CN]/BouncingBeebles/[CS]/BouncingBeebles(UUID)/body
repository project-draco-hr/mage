{
  super(ownerId,28,"Bouncing Beebles",Rarity.COMMON,new CardType[]{CardType.CREATURE},"{2}{U}");
  this.expansionSetCode="ULG";
  this.subtype.add("Beeble");
  this.color.setBlue(true);
  this.power=new MageInt(2);
  this.toughness=new MageInt(2);
  Effect effect=new ConditionalContinousEffect(new UnblockableSourceEffect(),new DefendingPlayerControlsCondition(new FilterArtifactPermanent()),"{this} is unblockable as long as defending player controls an artifact");
  this.addAbility(new SimpleStaticAbility(Zone.BATTLEFIELD,effect));
}
