  this.toughness=new MageInt(4);
  this.addAbility(new SimpleActivatedAbility(Constants.Zone.BATTLEFIELD,new BoostSourceEffect(1,-1,Constants.Duration.EndOfTurn),new ManaCostsImpl("{U/R}")));
}
