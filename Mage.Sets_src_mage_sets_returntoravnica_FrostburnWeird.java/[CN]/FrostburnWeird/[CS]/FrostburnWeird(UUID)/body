{
  super(ownerId,215,"Frostburn Weird",Rarity.COMMON,new CardType[]{CardType.CREATURE},"{U/R}{U/R}");
  this.expansionSetCode="RTR";
  this.subtype.add("Weird");
  this.color.setBlue(true);
  this.color.setRed(true);
