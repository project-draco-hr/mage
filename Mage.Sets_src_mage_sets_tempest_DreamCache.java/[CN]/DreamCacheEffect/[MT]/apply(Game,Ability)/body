{
  Player player=game.getPlayer(source.getControllerId());
  if (player != null) {
    player.drawCards(3,game);
    boolean putOnTop=player.chooseUse(Outcome.Neutral,"Put cards on top?",game);
    putInLibrary(player,source,game,putOnTop);
    putInLibrary(player,source,game,putOnTop);
    return true;
  }
  return false;
}
