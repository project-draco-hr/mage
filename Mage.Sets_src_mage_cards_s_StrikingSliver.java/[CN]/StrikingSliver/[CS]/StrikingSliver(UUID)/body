{
  super(ownerId,157,"Striking Sliver",Rarity.COMMON,new CardType[]{CardType.CREATURE},"{R}");
  this.expansionSetCode="M14";
  this.subtype.add("Sliver");
  this.power=new MageInt(1);
  this.toughness=new MageInt(1);
  this.addAbility(new SimpleStaticAbility(Zone.BATTLEFIELD,new GainAbilityControlledEffect(FirstStrikeAbility.getInstance(),Duration.WhileOnBattlefield,new FilterCreaturePermanent("Sliver","Sliver creatures"))));
}
