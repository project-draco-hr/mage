{
  super(ownerId,setInfo,new CardType[]{CardType.CREATURE},"{5}{U}{U}");
  this.subtype.add("Kraken");
  this.power=new MageInt(4);
  this.toughness=new MageInt(8);
  this.addAbility(ShroudAbility.getInstance());
  this.addAbility(new SimpleStaticAbility(Zone.ALL,new ConditionalContinuousEffect(new BoostSourceEffect(4,8,Duration.EndOfGame),new CardsInAnyLibraryCondition(Condition.ComparisonType.LessThan,21),"{this} gets +4/+8 as long as a library has twenty or fewer cards in it")));
}
