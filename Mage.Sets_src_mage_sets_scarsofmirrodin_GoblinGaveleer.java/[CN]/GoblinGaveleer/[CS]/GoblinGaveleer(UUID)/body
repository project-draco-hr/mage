{
  super(ownerId,92,"Goblin Gaveleer",Rarity.COMMON,new CardType[]{CardType.CREATURE},"{R}");
  this.expansionSetCode="SOM";
  this.subtype.add("Goblin");
  this.subtype.add("Warrior");
  this.color.setRed(true);
  this.power=new MageInt(1);
  this.toughness=new MageInt(1);
  this.addAbility(new SimpleStaticAbility(Zone.BATTLEFIELD,new BoostSourceEffect(new EquipmentAttachedCount(2),new StaticValue(0),Duration.WhileOnBattlefield)));
}
