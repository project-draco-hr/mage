{
switch (mana) {
case B:
    if (testMana.getBlack() > 0)     return true;
case U:
  if (testMana.getBlue() > 0)   return true;
case R:
if (testMana.getRed() > 0) return true;
case W:
if (testMana.getWhite() > 0) return true;
case G:
if (testMana.getGreen() > 0) return true;
}
return testMana.count() > 0;
}
