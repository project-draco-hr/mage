{
  super(ownerId,127,"Sphinx Summoner",Rarity.RARE,new CardType[]{CardType.ARTIFACT,CardType.CREATURE},"{3}{U}{B}");
  this.expansionSetCode="CON";
  this.subtype.add("Sphinx");
  this.power=new MageInt(3);
