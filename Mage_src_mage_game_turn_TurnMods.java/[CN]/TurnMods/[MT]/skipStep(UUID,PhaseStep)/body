{
  ListIterator<TurnMod> it=this.listIterator(this.size());
  while (it.hasPrevious()) {
    TurnMod turnMod=it.previous();
    if (turnMod.getSkipStep() != null && turnMod.getPlayerId().equals(playerId) && turnMod.getSkipStep() == step) {
      it.remove();
      return true;
    }
  }
  return false;
}
