{
  super(ownerId,189,"Predatory Sliver",Rarity.COMMON,new CardType[]{CardType.CREATURE},"{1}{G}");
  this.expansionSetCode="M14";
  this.subtype.add("Sliver");
  this.power=new MageInt(1);
  this.toughness=new MageInt(1);
  this.addAbility(new SimpleStaticAbility(Zone.BATTLEFIELD,new BoostControlledEffect(1,1,Duration.WhileInGraveyard,new FilterCreaturePermanent("Sliver","Sliver creatures"))));
}
