{
  Spell spell=game.getStack().getSpell(targetPointer.getFirst(game,source));
  if (spell != null) {
    int cmc=spell.getConvertedManaCost();
    if (cmc > 0) {
      return new CreateTokenEffect(new EndrekSahrMasterBreederThrullToken(),cmc).apply(game,source);
    }
    return true;
  }
  return false;
}
