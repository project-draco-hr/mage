{
  super(ownerId,121,"Phyrexian Juggernaut",Rarity.UNCOMMON,new CardType[]{CardType.ARTIFACT,CardType.CREATURE},"{6}");
  this.expansionSetCode="MBS";
  this.subtype.add("Juggernaut");
  this.power=new MageInt(5);
