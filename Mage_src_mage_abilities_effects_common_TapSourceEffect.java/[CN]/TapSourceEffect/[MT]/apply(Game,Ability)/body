{
  Permanent permanent=game.getPermanent(source.getSourceId());
  if (permanent == null) {
    permanent=(Permanent)getValue(EntersBattlefieldEffect.ENTERING_PERMANENT);
  }
  if (permanent != null) {
    if (withoutTrigger) {
      permanent.setTapped(true);
    }
 else {
      permanent.tap(game);
    }
    return true;
  }
  return false;
}
