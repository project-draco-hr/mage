  this.toughness=new MageInt(3);
  this.addAbility(new ConditionalActivatedAbility(Zone.BATTLEFIELD,new BoostSourceEffect(1,1,Duration.EndOfTurn),new ManaCostsImpl("{1}{B}"),new DeliriumCondition()));
}
