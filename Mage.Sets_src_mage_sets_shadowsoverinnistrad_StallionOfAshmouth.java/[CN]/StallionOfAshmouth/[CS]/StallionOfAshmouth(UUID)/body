{
  super(ownerId,136,"Stallion of Ashmouth",Rarity.COMMON,new CardType[]{CardType.CREATURE},"{3}{B}");
  this.expansionSetCode="SOI";
  this.subtype.add("Nightmare");
  this.subtype.add("Horse");
