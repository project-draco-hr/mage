{
  super(ownerId,128,"Battle Sliver",Rarity.UNCOMMON,new CardType[]{CardType.CREATURE},"{4}{R}");
  this.expansionSetCode="M14";
  this.subtype.add("Sliver");
  this.color.setRed(true);
  this.power=new MageInt(3);
  this.toughness=new MageInt(3);
  this.addAbility(new SimpleStaticAbility(Zone.BATTLEFIELD,new BoostControlledEffect(2,0,Duration.WhileOnBattlefield,new FilterCreaturePermanent("Sliver","Sliver creatures"))));
}
