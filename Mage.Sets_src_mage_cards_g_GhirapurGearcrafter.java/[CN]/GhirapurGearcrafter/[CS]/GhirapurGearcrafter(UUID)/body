{
  super(ownerId,149,"Ghirapur Gearcrafter",Rarity.COMMON,new CardType[]{CardType.CREATURE},"{2}{R}");
  this.expansionSetCode="ORI";
  this.subtype.add("Human");
  this.subtype.add("Artificer");
