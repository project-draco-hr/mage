{
  int maxLife=0;
  for (  UUID pid : game.getPlayerList()) {
    Player p=game.getPlayer(pid);
    if (p != null) {
      if (maxLife < p.getLife()) {
        maxLife=p.getLife();
      }
    }
  }
  for (  UUID pid : game.getPlayerList()) {
    Player p=game.getPlayer(pid);
    if (p != null) {
      p.setLife(maxLife,game);
    }
  }
  return true;
}
