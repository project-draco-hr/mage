{
  int val=CardUtil.getColorIdentitySortValue(o1.getOriginal().getManaCost(),o1.getOriginal().getColor(),o1.getOriginal().getRules()) - CardUtil.getColorIdentitySortValue(o2.getOriginal().getManaCost(),o2.getOriginal().getColor(),o2.getOriginal().getRules());
  if (val == 0) {
    return o1.getOriginal().getName().compareTo(o2.getOriginal().getName());
  }
 else {
    return val;
  }
}
