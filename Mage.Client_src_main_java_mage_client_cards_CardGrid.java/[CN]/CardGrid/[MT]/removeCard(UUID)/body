{
  for (  Component comp : getComponents()) {
    if (comp instanceof Card) {
      if (((Card)comp).getCardId().equals(cardId)) {
        remove(comp);
      }
 else       if (comp instanceof MageCard) {
        if (((MageCard)comp).getOriginal().getId().equals(cardId)) {
          remove(comp);
        }
      }
    }
  }
}
