{
  if (controllers.containsKey(tableId)) {
    controllers.get(tableId).leaveTable(userId);
    if (isTableOwner(tableId,userId)) {
      if (getTable(tableId).getState().equals(TableState.WAITING) || getTable(tableId).getState().equals(TableState.STARTING)) {
        removeTable(tableId);
      }
    }
  }
}
