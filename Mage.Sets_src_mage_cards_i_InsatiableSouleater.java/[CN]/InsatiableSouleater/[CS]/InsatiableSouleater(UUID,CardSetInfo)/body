{
  super(ownerId,setInfo,new CardType[]{CardType.ARTIFACT,CardType.CREATURE},"{4}");
  this.subtype.add("Beast");
  this.power=new MageInt(5);
  this.toughness=new MageInt(1);
  this.addAbility(new SimpleActivatedAbility(Zone.BATTLEFIELD,new GainAbilitySourceEffect(TrampleAbility.getInstance(),Duration.EndOfTurn),new PhyrexianManaCost(ColoredManaSymbol.G)));
}
