{
  super(ownerId,266,"Giant Growth",Rarity.COMMON,new CardType[]{CardType.INSTANT},"{G}");
  this.expansionSetCode="10E";
  this.color.setGreen(true);
  this.getSpellAbility().addTarget(new TargetCreaturePermanent());
