{
  super(ownerId,7,"Hada Freeblade",Rarity.UNCOMMON,new CardType[]{CardType.CREATURE},"{W}");
  this.expansionSetCode="WWK";
  this.subtype.add("Human");
  this.subtype.add("Soldier");
