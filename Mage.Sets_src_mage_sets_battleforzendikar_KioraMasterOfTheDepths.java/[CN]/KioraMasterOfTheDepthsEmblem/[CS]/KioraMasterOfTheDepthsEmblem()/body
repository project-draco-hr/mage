{
  this.setName("EMBLEM: Kiora, Master of the Depths");
  Ability ability=new EntersBattlefieldControlledTriggeredAbility(Zone.COMMAND,new KioraFightEffect(),filter,true,SetTargetPointer.PERMANENT,"Whenever a creature enters the battlefield under your control, you may have it fight target creature.");
  ability.addTarget(new TargetCreaturePermanent());
  this.getAbilities().add(ability);
}
