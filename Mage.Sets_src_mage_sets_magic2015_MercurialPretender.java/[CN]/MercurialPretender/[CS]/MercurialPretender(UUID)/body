{
  super(ownerId,68,"Mercurial Pretender",Rarity.RARE,new CardType[]{CardType.CREATURE},"{4}{U}");
  this.expansionSetCode="M15";
  this.subtype.add("Shapeshifter");
  this.color.setBlue(true);
  this.power=new MageInt(0);
  this.toughness=new MageInt(0);
  Effect effect=new CopyPermanentEffect(new FilterControlledCreaturePermanent(),new AbilityApplier(new SimpleActivatedAbility(Zone.BATTLEFIELD,new ReturnToHandSourceEffect(true),new ManaCostsImpl("{2}{U}{U}"))));
  effect.setText(effectText);
  this.addAbility(new EntersBattlefieldAbility(effect,true));
}
