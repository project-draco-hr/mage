{
  super(ownerId,14,"Affa Protector",Rarity.COMMON,new CardType[]{CardType.CREATURE},"{2}{W}");
  this.expansionSetCode="OGW";
  this.subtype.add("Human");
  this.subtype.add("Soldier");
  this.subtype.add("Ally");
