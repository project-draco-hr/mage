{
  StringBuilder sb=new StringBuilder();
  if (attachmentType == Constants.AttachmentType.AURA)   sb.append("Enchanted");
 else   if (attachmentType == Constants.AttachmentType.EQUIPMENT)   sb.append("Equipped");
  sb.append(" creature becomes ").append(addedSubtype).append(" in addition to its other types");
  staticText=sb.toString();
}
