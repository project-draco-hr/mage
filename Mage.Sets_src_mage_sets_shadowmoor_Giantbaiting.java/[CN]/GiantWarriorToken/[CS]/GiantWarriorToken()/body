{
  super("Giant Warrior","4/4 red and green Giant Warrior creature token with haste");
  cardType.add(CardType.CREATURE);
  color.setRed(true);
  color.setGreen(true);
  subtype.add("Giant");
  subtype.add("Warrior");
  power=new MageInt(4);
  toughness=new MageInt(4);
  this.addAbility(HasteAbility.getInstance());
}
