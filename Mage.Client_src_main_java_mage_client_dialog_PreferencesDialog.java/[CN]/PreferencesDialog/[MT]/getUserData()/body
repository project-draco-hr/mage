{
  if (selectedAvatarId == 0) {
    getSelectedAvatar();
  }
  return new UserData(UserGroup.PLAYER,PreferencesDialog.selectedAvatarId,PreferencesDialog.getCachedValue(PreferencesDialog.KEY_SHOW_ABILITY_PICKER_FORCED,"true").equals("true"),PreferencesDialog.getCachedValue(PreferencesDialog.KEY_GAME_ALLOW_REQUEST_SHOW_HAND_CARDS,"true").equals("true"),PreferencesDialog.getCachedValue(PreferencesDialog.KEY_GAME_CONFIRM_EMPTY_MANA_POOL,"true").equals("true"),getUserSkipPrioritySteps(),MageFrame.getPreferences().get(KEY_CONNECT_FLAG,"world"),PreferencesDialog.getCachedValue(PreferencesDialog.KEY_GAME_ASK_MOVE_TO_GRAVE_ORDER,"false").equals("true"),PreferencesDialog.getCachedValue(PreferencesDialog.KEY_GAME_MANA_AUTOPAYMENT,"true").equals("true"),PreferencesDialog.getCachedValue(PreferencesDialog.KEY_GAME_MANA_AUTOPAYMENT_ONLY_ONE,"true").equals("true"),PreferencesDialog.getCachedValue(PreferencesDialog.KEY_PASS_PRIORITY_CAST,"true").equals("true"),PreferencesDialog.getCachedValue(PreferencesDialog.KEY_PASS_PRIORITY_ACTIVATION,"true").equals("true"),PreferencesDialog.getCachedValue(PreferencesDialog.KEY_AUTO_ORDER_TRIGGER,"true").equals("true"),PreferencesDialog.getCachedValue(PreferencesDialog.KEY_USE_FIRST_MANA_ABILITY,"false").equals("true"));
}
