{
  try {
    selectedId=Integer.valueOf(MageFrame.getPreferences().get(KEY_AVATAR,String.valueOf(DEFAULT_AVATAR_ID)));
  }
 catch (  NumberFormatException n) {
    selectedId=DEFAULT_AVATAR_ID;
  }
 finally {
    if (!availableAvatars.contains(selectedId)) {
      selectedId=DEFAULT_AVATAR_ID;
    }
  }
  return selectedId;
}
