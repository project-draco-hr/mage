{
  boolean result=true;
  int targetsSet=0;
  for (  Player player : game.getPlayers().values()) {
    if (player.getName().equals(target)) {
      ability.getTargets().get(0).addTarget(player.getId(),ability,game);
      targetsSet++;
      break;
    }
  }
  if (targetsSet < 1) {
    result=false;
  }
  return result;
}
