{
  if (!choices.isEmpty()) {
    for (    String choose2 : choices) {
      for (      String choose1 : choice.getChoices()) {
        if (choose1.equals(choose2)) {
          choice.setChoice(choose2);
          choices.remove(choose2);
          return true;
        }
      }
    }
  }
  return super.choose(outcome,choice,game);
}
