{
  if (!choices.isEmpty()) {
    for (    String choice : choices) {
      for (int index=0; index < rEffects.size(); index++) {
        if (choice.equals(rEffects.get(index))) {
          choices.remove(choice);
          return index;
        }
      }
    }
  }
  return super.chooseReplacementEffect(rEffects,game);
}
