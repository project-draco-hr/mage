{
  if (groups.length > 2 && groups[2].startsWith("spellOnStack=")) {
    String spellOnStack=groups[2].substring(13);
    for (    StackObject stackObject : game.getStack()) {
      if (stackObject.getStackAbility().toString().indexOf(spellOnStack) >= 0) {
        return true;
      }
    }
    return false;
  }
  return true;
}
