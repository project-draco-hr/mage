{
  UUID ownerId=game.getOwnerId(source.getSourceId());
  if (ownerId == null) {
    return false;
  }
  Player owner=game.getPlayer(ownerId);
  if (owner != null) {
    owner.moveCards(owner.getGraveyard(),null,Zone.LIBRARY,source,game);
    owner.shuffleLibrary(game);
    return true;
  }
  return false;
}
