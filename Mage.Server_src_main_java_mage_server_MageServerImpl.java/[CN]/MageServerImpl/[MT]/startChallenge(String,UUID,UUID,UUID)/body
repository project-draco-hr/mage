{
  if (SessionManager.getInstance().isValidSession(sessionId)) {
    try {
      rmiExecutor.execute(new Runnable(){
        @Override public void run(){
          TableManager.getInstance().startChallenge(sessionId,roomId,tableId,challengeId);
        }
      }
);
    }
 catch (    Exception ex) {
      handleException(ex);
    }
  }
}
