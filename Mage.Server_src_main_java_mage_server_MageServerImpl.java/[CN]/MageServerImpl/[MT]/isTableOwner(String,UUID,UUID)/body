{
  try {
    Session session=SessionManager.getInstance().getSession(sessionId);
    if (session != null) {
      UUID userId=SessionManager.getInstance().getSession(sessionId).getUserId();
      return TableManager.getInstance().isTableOwner(tableId,userId);
    }
  }
 catch (  Exception ex) {
    handleException(ex);
  }
  return false;
}
