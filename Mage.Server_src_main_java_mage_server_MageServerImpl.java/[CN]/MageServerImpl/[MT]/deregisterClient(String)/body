{
  try {
    rmiExecutor.execute(new Runnable(){
      @Override public void run(){
        Session session=SessionManager.getInstance().getSession(sessionId);
        if (session != null) {
          session.kill();
          logger.info("Client deregistered ...");
        }
      }
    }
);
  }
 catch (  Exception ex) {
    handleException(ex);
  }
}
