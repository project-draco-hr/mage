{
  try {
    if (SessionManager.getInstance().isValidSession(sessionId)) {
      UUID userId=SessionManager.getInstance().getSession(sessionId).getUserId();
      return GamesRoomManager.getInstance().getRoom(roomId).watchTable(userId,tableId);
    }
  }
 catch (  Exception ex) {
    handleException(ex);
  }
  return false;
}
