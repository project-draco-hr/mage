{
  if (SessionManager.getInstance().isValidSession(sessionId)) {
    try {
      rmiExecutor.execute(new Runnable(){
        @Override public void run(){
          GamesRoomManager.getInstance().getRoom(roomId).leaveTable(sessionId,tableId);
        }
      }
);
    }
 catch (    Exception ex) {
      handleException(ex);
    }
  }
}
