{
  if (SessionManager.getInstance().isValidSession(sessionId)) {
    try {
      callExecutor.execute(new Runnable(){
        @Override public void run(){
          User user=SessionManager.getInstance().getUser(sessionId);
          user.sendPlayerBoolean(gameId,data);
        }
      }
);
    }
 catch (    Exception ex) {
      handleException(ex);
    }
  }
}
