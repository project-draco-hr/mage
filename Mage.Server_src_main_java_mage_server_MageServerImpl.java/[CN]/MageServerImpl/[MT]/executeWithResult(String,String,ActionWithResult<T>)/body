{
  if (SessionManager.getInstance().isValidSession(sessionId)) {
    try {
      return action.execute();
    }
 catch (    Exception ex) {
      handleException(ex);
    }
  }
 else {
    LogServiceImpl.instance.log(LogKeys.KEY_NOT_VALID_SESSION,actionName,sessionId);
  }
  return action.negativeResult();
}
