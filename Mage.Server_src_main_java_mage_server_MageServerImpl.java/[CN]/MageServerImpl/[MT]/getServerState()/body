{
  try {
    return new ServerState(GameFactory.getInstance().getGameTypes(),TournamentFactory.getInstance().getTournamentTypes(),PlayerFactory.getInstance().getPlayerTypes().toArray(new String[PlayerFactory.getInstance().getPlayerTypes().size()]),DeckValidatorFactory.getInstance().getDeckTypes().toArray(new String[DeckValidatorFactory.getInstance().getDeckTypes().size()]),testMode,Main.getVersion());
  }
 catch (  Exception ex) {
    handleException(ex);
  }
  return null;
}
