{
  super(ownerId,43,"Merfolk Traders",Rarity.COMMON,new CardType[]{CardType.CREATURE},"{1}{U}");
  this.expansionSetCode="WTH";
  this.subtype.add("Merfolk");
  this.power=new MageInt(1);
  this.toughness=new MageInt(2);
