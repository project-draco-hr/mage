{
  super(ownerId,13,"Gustrider Exuberant",Rarity.COMMON,new CardType[]{CardType.CREATURE},"{2}{W}");
  this.expansionSetCode="ALA";
  this.subtype.add("Human");
  this.subtype.add("Wizard");
  this.power=new MageInt(1);
  this.toughness=new MageInt(2);
  this.addAbility(FlyingAbility.getInstance());
  this.addAbility(new SimpleActivatedAbility(Zone.BATTLEFIELD,new GainAbilityControlledEffect(FlyingAbility.getInstance(),Duration.EndOfTurn,filter),new SacrificeSourceCost()));
}
