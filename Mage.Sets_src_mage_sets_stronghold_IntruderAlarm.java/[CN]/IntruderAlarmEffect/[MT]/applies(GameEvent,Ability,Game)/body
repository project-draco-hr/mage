{
  Permanent creature=game.getPermanent(event.getTargetId());
  if (game.getTurn().getStepType() == PhaseStep.UNTAP && event.getType() == EventType.UNTAP && creature != null && creature.getCardType().contains(CardType.CREATURE) && creature.getControllerId() == event.getPlayerId()) {
    return true;
  }
  return false;
}
