{
  Card card=game.getCard(sourceId);
  if (card != null && card.getZoneChangeCounter(game) <= zoneChangeCounter + 1) {
    for (    OptionalAdditionalCost cost : kickerCosts) {
      if (cost.isActivated()) {
        return true;
      }
    }
  }
 else {
    this.resetKicker();
  }
  return false;
}
