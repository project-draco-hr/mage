{
  super(ownerId,30,"Moorland Inquisitor",Rarity.COMMON,new CardType[]{CardType.CREATURE},"{1}{W}");
  this.expansionSetCode="AVR";
  this.subtype.add("Human");
  this.subtype.add("Soldier");
  this.color.setWhite(true);
  this.power=new MageInt(2);
  this.toughness=new MageInt(2);
  this.addAbility(new SimpleActivatedAbility(Constants.Zone.BATTLEFIELD,new GainAbilitySourceEffect(FirstStrikeAbility.getInstance(),Constants.Duration.EndOfTurn),new ManaCostsImpl("{2}{W}")));
}
