  this.addAbility(new SimpleActivatedAbility(Zone.BATTLEFIELD,new GainAbilitySourceEffect(DeathtouchAbility.getInstance(),Duration.EndOfTurn),new ColoredManaCost(ColoredManaSymbol.B)));
}
