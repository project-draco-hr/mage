{
  super(ownerId,16,"Mardu Hateblade",Rarity.COMMON,new CardType[]{CardType.CREATURE},"{W}");
  this.expansionSetCode="KTK";
  this.subtype.add("Human");
  this.subtype.add("Warrior");
