{
  super(ownerId,12,"Breaching Leviathan",Rarity.RARE,new CardType[]{CardType.CREATURE},"{7}{U}{U}");
  this.expansionSetCode="C14";
  this.subtype.add("Leviathan");
  this.color.setBlue(true);
  this.power=new MageInt(9);
  this.toughness=new MageInt(9);
  Ability ability=new EntersBattlefieldTriggeredAbility(new ConditionalOneShotEffect(new BreachingLeviathanEffect(),new CastFromHandCondition(),"if you cast it from your hand, tap all nonblue creatures. Those creatures don't untap during their controllers' next untap steps"));
  this.addAbility(ability,new CastFromHandWatcher());
}
