{
  super(ownerId,91,"Graf Rats",Rarity.COMMON,new CardType[]{CardType.CREATURE},"{1}{B}");
  this.expansionSetCode="EMN";
  this.subtype.add("Rat");
  this.power=new MageInt(2);
  this.toughness=new MageInt(1);
  this.addAbility(new ConditionalTriggeredAbility(new BeginningOfCombatTriggeredAbility(new MeldEffect("Midnight Scavengers",new ChitteringHost(ownerId)),TargetController.YOU,false),new MeldCondition("Midnight Scavengers"),"At the beginning of combat on your turn, if you both own and control {this} and a creature named Midnight Scavengers, exile them, then meld them into Chittering Host."));
}
