{
  super(ownerId,177,"Groundshaker Sliver",Rarity.COMMON,new CardType[]{CardType.CREATURE},"{6}{G}");
  this.expansionSetCode="M14";
  this.subtype.add("Sliver");
  this.power=new MageInt(5);
  this.toughness=new MageInt(5);
  this.addAbility(new SimpleStaticAbility(Zone.BATTLEFIELD,new GainAbilityControlledEffect(TrampleAbility.getInstance(),Duration.WhileOnBattlefield,new FilterCreaturePermanent("Sliver","Sliver creatures"))));
}
