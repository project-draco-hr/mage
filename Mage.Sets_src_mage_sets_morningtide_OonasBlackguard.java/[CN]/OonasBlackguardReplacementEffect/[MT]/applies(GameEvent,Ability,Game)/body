{
  if (event.getType() == GameEvent.EventType.ENTERS_THE_BATTLEFIELD) {
    Permanent creature=game.getPermanent(event.getTargetId());
    if (creature != null && creature.getControllerId().equals(source.getControllerId()) && creature.getCardType().contains(CardType.CREATURE) && creature.getSubtype().contains("Rogue") && !event.getTargetId().equals(source.getSourceId())) {
      return true;
    }
  }
  return false;
}
