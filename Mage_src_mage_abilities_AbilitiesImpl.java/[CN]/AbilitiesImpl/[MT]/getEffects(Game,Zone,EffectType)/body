{
  Map<Effect,Ability> effects=new HashMap<Effect,Ability>();
  for (  T ability : this) {
    if (ability instanceof StaticAbility && ability.getZone().match(zone)) {
      for (      Effect effect : ability.getEffects(game,effectType)) {
        effects.put(effect,ability);
      }
    }
  }
  return effects;
}
