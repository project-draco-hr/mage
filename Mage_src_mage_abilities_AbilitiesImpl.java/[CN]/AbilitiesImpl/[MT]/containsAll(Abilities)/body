{
  if (this.size() < abilities.size())   return false;
  for (  Ability ability : abilities) {
    boolean found=false;
    for (    Ability test : this) {
      if (ability.getRule().equals(test.getRule())) {
        found=true;
        break;
      }
      if (!found)       return false;
    }
  }
  return true;
}
