{
  if (this.size() < abilities.size())   return false;
  for (  T ability : abilities) {
    boolean found=false;
    for (    T test : this) {
      if (ability.getId().equals(test.getId()) || ability.getRule().equals(test.getRule())) {
        found=true;
        break;
      }
    }
    if (!found)     return false;
  }
  return true;
}
