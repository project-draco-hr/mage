{
  List<String> abilities=new ArrayList<>();
  for (  T ability : this) {
    if (ability.toString() != null) {
      abilities.add(ability.toString());
    }
  }
  Collections.sort(abilities);
  StringBuilder sb=new StringBuilder();
  for (  String s : abilities) {
    sb.append(s);
  }
  return sb.toString();
}
