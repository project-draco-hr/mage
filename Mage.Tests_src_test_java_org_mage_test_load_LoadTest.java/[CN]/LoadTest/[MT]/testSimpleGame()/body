{
  final DeckCardLists deckList=createDeck();
  for (int i=0; i < EXECUTION_COUNT_PLAY_GAME; i++) {
    final int j=i;
    Thread t=new Thread(new Runnable(){
      @Override public void run(){
        try {
          testSimpleGame0(deckList,j);
        }
 catch (        InterruptedException e) {
          e.printStackTrace();
        }
      }
    }
);
    t.start();
    t.join();
  }
}
