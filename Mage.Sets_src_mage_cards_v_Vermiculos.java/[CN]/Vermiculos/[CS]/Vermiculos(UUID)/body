{
  super(ownerId,80,"Vermiculos",Rarity.RARE,new CardType[]{CardType.CREATURE},"{4}{B}");
  this.expansionSetCode="MRD";
  this.subtype.add("Horror");
  this.power=new MageInt(1);
  this.toughness=new MageInt(1);
