{
  super(ownerId,179,"Mayael the Anima",Rarity.MYTHIC,new CardType[]{CardType.CREATURE},"{R}{G}{W}");
  this.expansionSetCode="ALA";
  this.supertype.add("Legendary");
  this.subtype.add("Elf");
  this.subtype.add("Shaman");
  this.power=new MageInt(2);
  this.toughness=new MageInt(3);
  SimpleActivatedAbility ability=new SimpleActivatedAbility(Zone.BATTLEFIELD,new LookLibraryAndPickControllerEffect(5,1,filter,false,false,Zone.BATTLEFIELD,true),new ManaCostsImpl("{3}{R}{G}{W}"));
  ability.addCost(new TapSourceCost());
  this.addAbility(ability);
}
