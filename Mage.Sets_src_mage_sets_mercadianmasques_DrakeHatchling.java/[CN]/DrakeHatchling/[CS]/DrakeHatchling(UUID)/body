  this.addAbility(FlyingAbility.getInstance());
  this.addAbility(new LimitedTimesPerTurnActivatedAbility(Zone.BATTLEFIELD,new BoostSourceEffect(1,0,Duration.EndOfTurn),new ManaCostsImpl("{U}")));
}
