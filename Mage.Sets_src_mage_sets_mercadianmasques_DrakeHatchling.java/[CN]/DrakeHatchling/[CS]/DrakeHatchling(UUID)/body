{
  super(ownerId,76,"Drake Hatchling",Rarity.COMMON,new CardType[]{CardType.CREATURE},"{2}{U}");
  this.expansionSetCode="MMQ";
  this.subtype.add("Drake");
  this.power=new MageInt(1);
