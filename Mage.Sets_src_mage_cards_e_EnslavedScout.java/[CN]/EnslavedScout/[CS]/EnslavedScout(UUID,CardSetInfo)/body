{
  super(ownerId,setInfo,new CardType[]{CardType.CREATURE},"{2}{R}");
  this.subtype.add("Goblin");
  this.subtype.add("Scout");
  this.power=new MageInt(2);
  this.toughness=new MageInt(2);
  this.addAbility(new SimpleActivatedAbility(Zone.BATTLEFIELD,new GainAbilitySourceEffect(new MountainwalkAbility(false),Duration.EndOfTurn),new GenericManaCost(2)));
}
