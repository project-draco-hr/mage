{
  super(ownerId,38,"Steelform Sliver",Rarity.UNCOMMON,new CardType[]{CardType.CREATURE},"{2}{W}");
  this.expansionSetCode="M14";
  this.subtype.add("Sliver");
  this.power=new MageInt(2);
  this.toughness=new MageInt(2);
  this.addAbility(new SimpleStaticAbility(Zone.BATTLEFIELD,new BoostControlledEffect(0,1,Duration.WhileOnBattlefield,new FilterCreaturePermanent("Sliver","Sliver creatures"),false)));
}
