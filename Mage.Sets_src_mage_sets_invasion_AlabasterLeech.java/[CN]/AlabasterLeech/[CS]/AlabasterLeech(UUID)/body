{
  super(ownerId,1,"Alabaster Leech",Rarity.RARE,new CardType[]{CardType.CREATURE},"{W}");
  this.expansionSetCode="INV";
  this.subtype.add("Leech");
  this.power=new MageInt(1);
  this.toughness=new MageInt(3);
