{
  super(ownerId,27,"Taj-Nar Swordsmith",Rarity.UNCOMMON,new CardType[]{CardType.CREATURE},"{3}{W}");
  this.expansionSetCode="MRD";
  this.subtype.add("Cat");
  this.subtype.add("Soldier");
