{
  StringBuilder sb=new StringBuilder();
switch (manaType) {
case BLACK:
    sb.append("black");
  break;
case GREEN:
sb.append("green");
break;
case RED:
sb.append("red");
break;
case BLUE:
sb.append("blue");
break;
case WHITE:
sb.append("white");
break;
case COLORLESS:
sb.append("colorless");
break;
}
sb.append(" mana in your mana pool");
return sb.toString();
}
