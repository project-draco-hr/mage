{
  super(ownerId,setInfo,new CardType[]{CardType.ARTIFACT},"{4}");
  SimpleActivatedAbility ability=new SimpleActivatedAbility(Zone.BATTLEFIELD,new PutPermanentOnBattlefieldEffect(new FilterCreatureCard("a creature card")),new ManaCostsImpl("{4}"));
  ability.addCost(new TapSourceCost());
  this.addAbility(ability);
}
