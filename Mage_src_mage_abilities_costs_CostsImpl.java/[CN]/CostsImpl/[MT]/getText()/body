{
  if (this.size() == 0) {
    return "";
  }
  StringBuilder sbText=new StringBuilder();
  for (  T cost : this) {
    String text=cost.getText();
    sbText.append(Character.toUpperCase(text.charAt(0))).append(text.substring(1)).append(", ");
  }
  sbText.setLength(sbText.length() - 2);
  return sbText.toString();
}
