{
  super(ownerId,21,"Radiant's Dragoons",Rarity.UNCOMMON,new CardType[]{CardType.CREATURE},"{3}{W}");
  this.expansionSetCode="ULG";
  this.subtype.add("Human");
  this.subtype.add("Soldier");
