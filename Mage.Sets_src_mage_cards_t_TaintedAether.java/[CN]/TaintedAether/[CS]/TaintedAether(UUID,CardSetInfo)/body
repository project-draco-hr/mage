{
  super(ownerId,setInfo,new CardType[]{CardType.ENCHANTMENT},"{2}{B}{B}");
  this.addAbility(new EntersBattlefieldAllTriggeredAbility(Zone.BATTLEFIELD,new SacrificeEffect(filter,1,""),new FilterCreaturePermanent(),false,SetTargetPointer.PLAYER,"Whenever a creature enters the battlefield, its controller sacrifices a creature or land"));
}
