{
  super(ownerId,143,"Respite",Rarity.COMMON,new CardType[]{CardType.INSTANT},"{1}{G}");
  this.expansionSetCode="TMP";
  this.getSpellAbility().addEffect(new PreventAllDamageByAllPermanentsEffect(Duration.EndOfTurn,true));
