{
  super(ownerId,7,"Cradle of Vitality",Rarity.RARE,new CardType[]{CardType.ENCHANTMENT},"{3}{W}");
  this.expansionSetCode="ALA";
  this.color.setWhite(true);
  Ability ability=new CradleOfVitalityGainLifeTriggeredAbility();
