{
  this.faceDown=state.faceDown;
  if (state.info != null) {
    info=new HashMap<>();
    info.putAll(state.info);
  }
  counters=state.counters.copy();
  if (state.abilities != null) {
    abilities=new AbilitiesImpl<>();
    for (    Ability ability : state.abilities) {
      abilities.add(ability.copy());
    }
  }
}
