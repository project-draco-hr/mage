{
  super(ownerId,333,"Mantis Engine",Rarity.UNCOMMON,new CardType[]{CardType.ARTIFACT,CardType.CREATURE},"{5}");
  this.expansionSetCode="10E";
  this.subtype.add("Insect");
  this.power=new MageInt(3);
  this.toughness=new MageInt(3);
  this.addAbility(new SimpleActivatedAbility(Constants.Zone.BATTLEFIELD,new GainAbilitySourceEffect(FlyingAbility.getInstance(),Constants.Duration.EndOfTurn),new GenericManaCost(2)));
  this.addAbility(new SimpleActivatedAbility(Constants.Zone.BATTLEFIELD,new GainAbilitySourceEffect(FirstStrikeAbility.getInstance(),Constants.Duration.EndOfTurn),new GenericManaCost(2)));
}
