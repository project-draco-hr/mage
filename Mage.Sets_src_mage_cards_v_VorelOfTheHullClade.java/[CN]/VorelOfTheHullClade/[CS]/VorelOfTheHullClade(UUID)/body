{
  super(ownerId,115,"Vorel of the Hull Clade",Rarity.RARE,new CardType[]{CardType.CREATURE},"{1}{G}{U}");
  this.expansionSetCode="DGM";
  this.supertype.add("Legendary");
  this.subtype.add("Human");
  this.subtype.add("Merfolk");
