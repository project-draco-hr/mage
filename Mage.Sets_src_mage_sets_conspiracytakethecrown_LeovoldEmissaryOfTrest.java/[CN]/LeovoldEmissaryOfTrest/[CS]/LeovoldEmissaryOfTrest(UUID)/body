{
  super(ownerId,77,"Leovold, Emissary of Trest",Rarity.MYTHIC,new CardType[]{CardType.CREATURE},"{B}{G}{U}");
  this.expansionSetCode="CN2";
  this.supertype.add("Legendary");
  this.subtype.add("Elf");
  this.subtype.add("Advisor");
