{
  super(ownerId,setInfo,new CardType[]{CardType.ENCHANTMENT},"{2}{G}{G}");
  this.addAbility(new EntersBattlefieldAllTriggeredAbility(Zone.BATTLEFIELD,new CreateTokenTargetEffect(new SnakeToken()),new FilterLandPermanent(),false,SetTargetPointer.PLAYER,"Whenever a land enters the battlefield, its controller creates a 1/1 green Snake creature token."));
}
