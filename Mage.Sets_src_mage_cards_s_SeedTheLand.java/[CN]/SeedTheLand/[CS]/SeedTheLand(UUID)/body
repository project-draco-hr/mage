{
  super(ownerId,146,"Seed the Land",Rarity.RARE,new CardType[]{CardType.ENCHANTMENT},"{2}{G}{G}");
  this.expansionSetCode="SOK";
  this.addAbility(new EntersBattlefieldAllTriggeredAbility(Zone.BATTLEFIELD,new CreateTokenTargetEffect(new SnakeToken()),new FilterLandPermanent(),false,SetTargetPointer.PLAYER,"Whenever a land enters the battlefield, its controller puts a 1/1 green Snake creature token onto the battlefield."));
}
