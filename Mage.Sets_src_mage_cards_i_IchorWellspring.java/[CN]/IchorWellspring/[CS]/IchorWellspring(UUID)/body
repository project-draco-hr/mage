{
  super(ownerId,110,"Ichor Wellspring",Rarity.COMMON,new CardType[]{CardType.ARTIFACT},"{2}");
  this.expansionSetCode="MBS";
  this.addAbility(new EntersBattlefieldTriggeredAbility(new DrawCardSourceControllerEffect(1)));
