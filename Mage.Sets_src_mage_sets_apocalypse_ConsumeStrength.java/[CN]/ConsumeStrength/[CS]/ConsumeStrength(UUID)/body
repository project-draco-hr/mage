{
  super(ownerId,93,"Consume Strength",Rarity.COMMON,new CardType[]{CardType.INSTANT},"{1}{B}{G}");
  this.expansionSetCode="APC";
  this.color.setGreen(true);
  this.color.setBlack(true);
