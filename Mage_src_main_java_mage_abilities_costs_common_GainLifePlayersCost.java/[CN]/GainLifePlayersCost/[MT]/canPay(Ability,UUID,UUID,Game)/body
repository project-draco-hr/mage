{
  Player controller=game.getPlayer(controllerId);
  if (controller != null) {
    for (    UUID playerId : controller.getInRange()) {
      if (!playerId.equals(controllerId)) {
        Player player=game.getPlayer(playerId);
        if (player != null && !player.isCanGainLife()) {
          return false;
        }
      }
    }
  }
  return true;
}
