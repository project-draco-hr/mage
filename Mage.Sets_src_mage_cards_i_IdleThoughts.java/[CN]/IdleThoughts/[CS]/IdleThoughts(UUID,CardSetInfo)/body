{
  super(ownerId,setInfo,new CardType[]{CardType.ENCHANTMENT},"{3}{U}");
  this.addAbility(new SimpleActivatedAbility(Zone.BATTLEFIELD,new ConditionalOneShotEffect(new DrawCardSourceControllerEffect(2),new CardsInHandCondition(),"Draw a card if you have no cards in hand"),new ManaCostsImpl("{2}")));
}
