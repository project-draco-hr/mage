{
  super(ownerId,8,"Not of This World",Rarity.UNCOMMON,new CardType[]{CardType.INSTANT},"{7}");
  this.expansionSetCode="ROE";
  this.supertype.add("Tribal");
  this.subtype.add("Eldrazi");
  this.getSpellAbility().addTarget(new TargetSpellTargetingControlledPermanent(new FilterSpell("a spell or ability that targets a creature you control with power 7 or greater.")));
  this.getSpellAbility().addEffect(new CounterTargetEffect());
}
