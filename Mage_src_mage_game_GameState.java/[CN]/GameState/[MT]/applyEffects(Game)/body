{
  Logger.getLogger(GameState.class).debug("Apply Effects turn: " + game.getTurnNum() + " - Step: "+ (game.getStep() == null ? "null" : game.getStep().getType().toString()));
  for (  Player player : players.values()) {
    player.reset();
  }
  battlefield.reset(game);
  combat.reset();
  this.reset();
  effects.apply(game);
}
