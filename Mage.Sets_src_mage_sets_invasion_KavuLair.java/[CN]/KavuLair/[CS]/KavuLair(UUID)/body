{
  super(ownerId,193,"Kavu Lair",Rarity.RARE,new CardType[]{CardType.ENCHANTMENT},"{2}{G}");
  this.expansionSetCode="INV";
  this.addAbility(new EntersBattlefieldAllTriggeredAbility(Zone.BATTLEFIELD,new DrawCardTargetEffect(1),filter,false,SetTargetPointer.PLAYER,"Whenever a creature with power 4 or greater enters the battlefield, its controller draws a card."));
}
