{
  super(ownerId,26,"Mind Funeral",Rarity.UNCOMMON,new CardType[]{CardType.SORCERY},"{1}{U}{B}");
  this.expansionSetCode="ARB";
  this.color.setBlue(true);
  this.color.setBlack(true);
