{
  super("Goblin Rogue","1/1 black Goblin Rogue creature tokens, and those tokens gain haste until end of turn");
  cardType.add(CardType.CREATURE);
  color.setBlack(true);
  subtype.add("Goblin");
  subtype.add("Rogue");
  power.setValue(1);
  toughness.setValue(1);
  Ability ability=new SimpleStaticAbility(Zone.BATTLEFIELD,new GainAbilitySourceEffect(HasteAbility.getInstance(),Duration.EndOfTurn));
  ability.setRuleVisible(false);
  this.addAbility(ability);
}
