{
  super(ownerId,213,"Rogue Kavu",Rarity.COMMON,new CardType[]{CardType.CREATURE},"{1}{R}");
  this.expansionSetCode="9ED";
  this.subtype.add("Kavu");
  this.power=new MageInt(1);
