{
  super(Outcome.DrawCard);
  this.cardsToDraw=cardsToDraw;
  this.cardsToDiscard=cardsToDiscard;
  staticText=new StringBuilder("Target player draws ").append(cardsToDraw == 1 ? "a" : CardUtil.numberToText(cardsToDraw)).append(" card").append(cardsToDraw == 1 ? " " : "s").append(", then discards ").append(cardsToDiscard == 1 ? "a" : CardUtil.numberToText(cardsToDiscard)).append(" card").append(cardsToDiscard == 1 ? " " : "s").toString();
}
