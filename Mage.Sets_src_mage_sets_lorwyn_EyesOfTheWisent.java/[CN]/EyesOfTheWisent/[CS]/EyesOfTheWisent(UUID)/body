{
  super(ownerId,210,"Eyes of the Wisent",Rarity.RARE,new CardType[]{CardType.TRIBAL,CardType.ENCHANTMENT},"{1}{G}");
  this.expansionSetCode="LRW";
  this.subtype.add("Elemental");
  this.addAbility(new ConditionalTriggeredAbility(new SpellCastOpponentTriggeredAbility(new CreateTokenEffect(new EyesOfTheWisentElementalToken()),filter,true),new MyTurnCondition(),"Whenever an opponent casts a blue spell during your turn, you may put a 4/4 green Elemental creature token onto the battlefield.",true));
}
