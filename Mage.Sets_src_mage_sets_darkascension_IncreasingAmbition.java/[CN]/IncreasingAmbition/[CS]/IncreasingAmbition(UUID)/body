{
  super(ownerId,69,"Increasing Ambition",Rarity.RARE,new CardType[]{CardType.SORCERY},"{4}{B}");
  this.expansionSetCode="DKA";
  this.color.setBlack(true);
  this.getSpellAbility().addEffect(new IncreasingAmbitionEffect());
