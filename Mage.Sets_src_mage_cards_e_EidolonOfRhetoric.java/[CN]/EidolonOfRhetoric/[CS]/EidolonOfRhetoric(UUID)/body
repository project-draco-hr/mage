{
  super(ownerId,10,"Eidolon of Rhetoric",Rarity.UNCOMMON,new CardType[]{CardType.ENCHANTMENT,CardType.CREATURE},"{2}{W}");
  this.expansionSetCode="JOU";
  this.subtype.add("Spirit");
