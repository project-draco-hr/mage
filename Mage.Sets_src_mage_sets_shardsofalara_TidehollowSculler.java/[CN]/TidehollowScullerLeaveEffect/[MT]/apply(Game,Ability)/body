{
  ExileZone exZone=game.getExile().getExileZone(CardUtil.getCardExileZoneId(game,source));
  if (exZone != null) {
    for (    Card card : exZone.getCards(game)) {
      if (card != null) {
        card.moveToZone(Zone.HAND,source.getId(),game,false);
      }
    }
    return true;
  }
  return false;
}
