  this.addAbility(new GreenManaAbility());
  this.addAbility(new AsEntersBattlefieldAbility(new TapSourceUnlessPaysEffect(new PayLifeCost(2)),"you may pay 2 life. If you don't, {this} enters the battlefield tapped"));
}
