{
  super("Sand Warrior","1/1 Sand Warrior creature tokens that are red, green, and white");
  cardType.add(CardType.CREATURE);
  color.setRed(true);
  color.setGreen(true);
  color.setWhite(true);
  subtype.add("Sand");
  subtype.add("Warrior");
  power=new MageInt(1);
  toughness=new MageInt(1);
}
