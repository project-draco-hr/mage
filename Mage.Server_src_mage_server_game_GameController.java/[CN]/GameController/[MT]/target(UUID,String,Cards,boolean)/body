{
  if (gameSessions.containsKey(playerId)) {
    if (cards != null)     gameSessions.get(playerId).target(question,new CardsView(cards.getCards(game)),required,getGameView(playerId));
 else     gameSessions.get(playerId).target(question,new CardsView(),required,getGameView(playerId));
  }
  informOthers(playerId);
}
