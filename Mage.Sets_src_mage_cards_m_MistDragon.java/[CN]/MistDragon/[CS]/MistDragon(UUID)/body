{
  super(ownerId,79,"Mist Dragon",Rarity.RARE,new CardType[]{CardType.CREATURE},"{4}{U}{U}");
  this.expansionSetCode="MIR";
  this.subtype.add("Dragon");
  this.power=new MageInt(4);
  this.toughness=new MageInt(4);
  this.addAbility(new SimpleActivatedAbility(Zone.BATTLEFIELD,new GainAbilitySourceEffect(FlyingAbility.getInstance(),Duration.WhileOnBattlefield),new GenericManaCost(0)));
  this.addAbility(new SimpleActivatedAbility(Zone.BATTLEFIELD,new LoseAbilitySourceEffect(FlyingAbility.getInstance(),Duration.WhileOnBattlefield),new GenericManaCost(0)));
  this.addAbility(new SimpleActivatedAbility(Zone.BATTLEFIELD,new PhaseOutSourceEffect(),new ManaCostsImpl("{3}{U}{U}")));
}
