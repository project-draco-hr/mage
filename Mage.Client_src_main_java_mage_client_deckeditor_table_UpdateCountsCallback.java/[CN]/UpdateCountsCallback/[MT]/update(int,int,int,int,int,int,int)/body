{
  if (this.lblCount != null)   this.lblCount.setText(Integer.toString(count));
  if (this.lblCreatureCount != null)   this.lblCreatureCount.setText(Integer.toString(creatures));
  if (this.lblLandCount != null)   this.lblLandCount.setText(Integer.toString(lands));
  if (this.lblSorceryCount != null)   this.lblSorceryCount.setText(Integer.toString(sorceries));
  if (this.lblInstantCount != null)   this.lblInstantCount.setText(Integer.toString(instants));
  if (this.lblEnchantmentCount != null)   this.lblEnchantmentCount.setText(Integer.toString(enchantments));
  if (this.lblArtifactCount != null)   this.lblArtifactCount.setText(Integer.toString(artifacts));
}
