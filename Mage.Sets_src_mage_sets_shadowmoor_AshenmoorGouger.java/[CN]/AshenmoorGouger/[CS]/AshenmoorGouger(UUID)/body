{
  super(ownerId,180,"Ashenmoor Gouger",Rarity.UNCOMMON,new CardType[]{CardType.CREATURE},"{B/R}{B/R}{B/R}");
  this.expansionSetCode="SHM";
  this.subtype.add("Elemental");
