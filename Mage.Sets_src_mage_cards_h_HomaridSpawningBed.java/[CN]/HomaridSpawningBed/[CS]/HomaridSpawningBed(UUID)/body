{
  super(ownerId,43,"Homarid Spawning Bed",Rarity.UNCOMMON,new CardType[]{CardType.ENCHANTMENT},"{U}{U}");
  this.expansionSetCode="FEM";
  Ability ability=new SimpleActivatedAbility(Zone.BATTLEFIELD,new CreateTokenEffect(new CamaridToken(),new SacrificeCostConvertedMana("creature")),new ManaCostsImpl("{1}{U}{U}"));
  ability.addCost(new SacrificeTargetCost(new TargetControlledCreaturePermanent(filter)));
  this.addAbility(ability);
}
