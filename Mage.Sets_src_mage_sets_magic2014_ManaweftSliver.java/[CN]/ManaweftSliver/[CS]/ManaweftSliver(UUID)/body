{
  super(ownerId,184,"Manaweft Sliver",Rarity.UNCOMMON,new CardType[]{CardType.CREATURE},"{1}{G}");
  this.expansionSetCode="M14";
  this.subtype.add("Sliver");
  this.color.setGreen(true);
  this.power=new MageInt(1);
  this.toughness=new MageInt(1);
  this.addAbility(new SimpleStaticAbility(Zone.BATTLEFIELD,new GainAbilityAllEffect(new AnyColorManaAbility(),Duration.WhileOnBattlefield,filter)));
}
