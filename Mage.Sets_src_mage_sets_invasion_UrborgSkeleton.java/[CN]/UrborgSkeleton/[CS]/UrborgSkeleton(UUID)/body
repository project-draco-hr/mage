{
  super(ownerId,134,"Urborg Skeleton",Rarity.COMMON,new CardType[]{CardType.CREATURE},"{B}");
  this.expansionSetCode="INV";
  this.subtype.add("Skeleton");
  this.color.setBlack(true);
  this.power=new MageInt(0);
  this.toughness=new MageInt(1);
  this.addAbility(new KickerAbility(new KickerManaCost("{3}")));
  this.addAbility(new SimpleActivatedAbility(Zone.BATTLEFIELD,new RegenerateSourceEffect(),new ManaCostsImpl("{B}")));
  Ability ability=new EntersBattlefieldAbility(new AddCountersSourceEffect(CounterType.P1P1.createInstance(1)),KickedCondition.getInstance(),false,staticText,"");
  this.addAbility(ability);
}
