{
switch (arg1) {
case 0:
    return players[arg0].getFlagName();
case 1:
  return players[arg0].getUserName();
case 2:
return players[arg0].getMatchHistory();
case 3:
return players[arg0].getTourneyHistory();
case 4:
return players[arg0].getInfoGames();
case 5:
return players[arg0].getInfoPing();
}
return "";
}
