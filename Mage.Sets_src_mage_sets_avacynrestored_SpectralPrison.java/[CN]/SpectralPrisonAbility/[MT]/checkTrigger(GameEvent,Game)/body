{
  if (event.getType() == GameEvent.EventType.TARGETED) {
    Permanent enchantment=game.getPermanent(sourceId);
    if (enchantment != null && enchantment.getAttachedTo() != null) {
      if (event.getTargetId().equals(enchantment.getAttachedTo())) {
        return true;
      }
    }
  }
  return false;
}
