{
  super(ownerId,205,"Elvish Eulogist",Rarity.COMMON,new CardType[]{CardType.CREATURE},"{G}");
  this.expansionSetCode="LRW";
  this.subtype.add("Elf");
  this.subtype.add("Shaman");
