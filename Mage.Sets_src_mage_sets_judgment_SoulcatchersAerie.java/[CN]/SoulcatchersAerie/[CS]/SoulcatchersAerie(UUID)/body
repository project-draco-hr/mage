{
  super(ownerId,25,"Soulcatchers' Aerie",Rarity.UNCOMMON,new CardType[]{CardType.ENCHANTMENT},"{1}{W}");
  this.expansionSetCode="JUD";
  this.addAbility(new PutIntoGraveFromBattlefieldAllTriggeredAbility(new AddCountersSourceEffect(CounterType.FEATHER.createInstance()),false,filter,false,true));
  this.addAbility(new SimpleStaticAbility(Zone.BATTLEFIELD,new BoostAllEffect(new CountersCount(CounterType.FEATHER),new CountersCount(CounterType.FEATHER),Duration.WhileOnBattlefield,filter2,false,"Bird creatures get +1/+1 for each feather counter on {this}.")));
}
