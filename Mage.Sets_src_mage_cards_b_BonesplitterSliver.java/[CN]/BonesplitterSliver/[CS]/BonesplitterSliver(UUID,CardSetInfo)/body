{
  super(ownerId,setInfo,new CardType[]{CardType.CREATURE},"{3}{R}");
  this.subtype.add("Sliver");
  this.power=new MageInt(2);
  this.toughness=new MageInt(2);
  this.addAbility(new SimpleStaticAbility(Zone.BATTLEFIELD,new BoostAllEffect(2,0,Duration.WhileOnBattlefield,new FilterCreaturePermanent("Sliver","Sliver creatures"),false)));
}
