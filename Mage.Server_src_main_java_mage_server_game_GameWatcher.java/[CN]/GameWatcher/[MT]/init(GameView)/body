{
  if (!killed) {
    Session session=SessionManager.getInstance().getSession(sessionId);
    if (session != null) {
      try {
        session.fireCallback(new ClientCallback("gameInit",gameId,gameView));
        return true;
      }
 catch (      CallbackException ex) {
        logger.fatal("Unable to start watching ",ex);
        return false;
      }
    }
  }
  return false;
}
