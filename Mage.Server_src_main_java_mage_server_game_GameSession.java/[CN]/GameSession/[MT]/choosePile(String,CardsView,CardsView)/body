{
  if (!killed) {
    setupTimeout();
    User user=UserManager.getInstance().getUser(userId);
    if (user != null) {
      List<CardsView> piles=new ArrayList<CardsView>();
      piles.add(pile1);
      piles.add(pile2);
      user.fireCallback(new ClientCallback("gameChoosePile",game.getId(),new GameClientMessage(message,pile1,pile2)));
    }
  }
}
