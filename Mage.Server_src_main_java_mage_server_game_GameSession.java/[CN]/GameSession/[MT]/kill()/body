{
  if (game != null) {
    if (game.getPlayer(playerId).isInGame()) {
      logger.debug("QUIT game playerId: " + playerId + " gameId: "+ game.getId());
      game.quit(playerId);
    }
  }
 else {
    logger.error("game object missing   playerId: " + (playerId == null ? "[null]" : playerId));
  }
}
