{
  super(ownerId,setInfo,new CardType[]{CardType.CREATURE},"{G}{G}{G}");
  this.supertype.add("Legendary");
  this.subtype.add("Centaur");
  this.subtype.add("Druid");
  this.power=new MageInt(2);
  this.toughness=new MageInt(2);
  this.addAbility(new SimpleManaAbility(Zone.BATTLEFIELD,new BasicManaEffect(Mana.GreenMana(1)),new TapTargetCost(new TargetControlledCreaturePermanent(1,1,filter,true))));
}
