{
  super(ownerId,214,"Quicksilver Amulet",Rarity.RARE,new CardType[]{CardType.ARTIFACT},"{4}");
  this.expansionSetCode="M12";
  SimpleActivatedAbility ability=new SimpleActivatedAbility(Zone.BATTLEFIELD,new PutCreatureOnBattlefieldEffect(),new ManaCostsImpl("{4}"));
  ability.addCost(new TapSourceCost());
  ability.addTarget(new TargetCardInHand(new FilterCreatureCard()));
  this.addAbility(ability);
}
