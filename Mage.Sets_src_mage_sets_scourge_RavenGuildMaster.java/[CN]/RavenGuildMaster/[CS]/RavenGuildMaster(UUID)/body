{
  super(ownerId,47,"Raven Guild Master",Rarity.RARE,new CardType[]{CardType.CREATURE},"{1}{U}{U}");
  this.expansionSetCode="SCG";
  this.subtype.add("Human");
  this.subtype.add("Wizard");
  this.subtype.add("Mutant");
