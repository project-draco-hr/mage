{
  Player target=game.getPlayer(source.getFirstTarget());
  if (target != null) {
    for (    Permanent p : game.getBattlefield().getAllActivePermanents(new FilterCreaturePermanent(),target.getId())) {
      p.addCounters(CounterType.M1M1.createInstance(),game);
    }
    return true;
  }
  return false;
}
