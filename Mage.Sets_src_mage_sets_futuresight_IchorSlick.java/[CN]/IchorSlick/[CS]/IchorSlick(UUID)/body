{
  super(ownerId,68,"Ichor Slick",Rarity.COMMON,new CardType[]{CardType.SORCERY},"{2}{B}");
  this.expansionSetCode="FUT";
  this.color.setBlack(true);
  this.getSpellAbility().addEffect(new BoostTargetEffect(-3,-3,Constants.Duration.EndOfTurn));
  this.getSpellAbility().addTarget(new TargetCreaturePermanent());
  this.addAbility(new CyclingAbility(new ManaCostsImpl("{2}")));
  this.addAbility(new MadnessAbility(this,new ManaCostsImpl("{3}{B}")));
}
