{
  super(ownerId,123,"Skyshroud Elite",Rarity.UNCOMMON,new CardType[]{CardType.CREATURE},"{G}");
  this.expansionSetCode="EXO";
  this.subtype.add("Elf");
  this.power=new MageInt(1);
  this.toughness=new MageInt(1);
  this.addAbility(new SimpleStaticAbility(Zone.BATTLEFIELD,new ConditionalContinuousEffect(new BoostSourceEffect(1,2,Duration.WhileOnBattlefield),new OpponentControlsPermanentCondition(filter),"{this} gets +1/+2 as long as an opponent controls a nonbasic land")));
}
