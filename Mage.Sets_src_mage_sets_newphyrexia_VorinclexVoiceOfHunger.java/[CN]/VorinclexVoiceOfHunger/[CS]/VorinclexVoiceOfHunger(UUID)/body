{
  super(ownerId,127,"Vorinclex, Voice of Hunger",Rarity.MYTHIC,new CardType[]{CardType.CREATURE},"{6}{G}{G}");
  this.expansionSetCode="NPH";
  this.supertype.add("Legendary");
  this.subtype.add("Praetor");
  this.color.setGreen(true);
  this.power=new MageInt(7);
  this.toughness=new MageInt(6);
  this.addAbility(TrampleAbility.getInstance());
  this.addAbility(new TapForManaAllTriggeredManaAbility(new AddManaOfAnyColorTargetCanProduceEffect(),new FilterControlledLandPermanent("you tap a land"),SetTargetPointer.PERMANENT));
  this.addAbility(new VorinclexTriggeredAbility2());
}
