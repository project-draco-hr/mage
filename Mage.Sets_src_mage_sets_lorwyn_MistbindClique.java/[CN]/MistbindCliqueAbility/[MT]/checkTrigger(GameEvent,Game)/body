{
  if (event.getSourceId() != null && event.getSourceId().equals(getSourceId()) && !event.getSourceId().equals(event.getTargetId())) {
    ZoneChangeEvent zEvent=(ZoneChangeEvent)event;
    if (zEvent.getTarget() != null && zEvent.getTarget().hasSubtype("Faerie",game)) {
      return true;
    }
  }
  return false;
}
