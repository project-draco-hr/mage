{
  super(ownerId,103,"Time Spiral",Rarity.RARE,new CardType[]{CardType.SORCERY},"{4}{U}{U}");
  this.expansionSetCode="USG";
  this.color.setBlue(true);
  this.getSpellAbility().addEffect(ExileSpellEffect.getInstance());
