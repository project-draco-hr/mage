{
  super(ownerId,150,"Cartel Aristocrat",Rarity.UNCOMMON,new CardType[]{CardType.CREATURE},"{W}{B}");
  this.expansionSetCode="GTC";
  this.subtype.add("Human");
  this.subtype.add("Advisor");
  this.power=new MageInt(2);
  this.toughness=new MageInt(2);
  Ability ability=new SimpleActivatedAbility(Zone.BATTLEFIELD,new CartelAristocratEffect(),new SacrificeTargetCost(new TargetControlledPermanent(filter)));
  this.addAbility(ability);
}
