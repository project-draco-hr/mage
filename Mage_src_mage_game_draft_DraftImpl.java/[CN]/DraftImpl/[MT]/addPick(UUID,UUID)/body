{
  DraftPlayer player=players.get(playerId);
  if (player.isPicking()) {
    for (    Card card : player.booster) {
      if (card.getId().equals(cardId)) {
        player.addPick(card);
        player.booster.remove(card);
        break;
      }
    }
synchronized (this) {
      this.notifyAll();
    }
  }
  return !player.isPicking();
}
