{
  Card card=game.getCard(source.getSourceId());
  if (card != null) {
switch (game.getZone(card.getId())) {
case BATTLEFIELD:
      if (card != null) {
        return card.moveToZone(Zone.HAND,source.getId(),game,false);
      }
    break;
case GRAVEYARD:
  if (card != null) {
    return card.moveToZone(Zone.HAND,source.getId(),game,true);
  }
break;
}
}
return false;
}
