{
  String expected=(String)ref.get("manaCost");
  String cost=join(card.getManaCost().getSymbols());
  if ("".equals(cost)) {
    cost=null;
  }
  if (cost != null) {
    cost=cost.replaceAll("P\\}","/P}");
  }
  if (!Objects.equals(cost,expected)) {
    System.out.println(cost + " != " + expected+ " for "+ card);
  }
}
