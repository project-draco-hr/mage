{
  Player controller=game.getPlayer(source.getControllerId());
  if (controller != null) {
    Card card=game.getCard(getTargetPointer().getFirst(game,source));
    if (card != null) {
      controller.moveCards(card,Zone.EXILED,Zone.GRAVEYARD,source,game);
    }
    return true;
  }
  return false;
}
