{
  if (event.getType() == GameEvent.EventType.ZONE_CHANGE && ((ZoneChangeEvent)event).getToZone() == Zone.GRAVEYARD && ((ZoneChangeEvent)event).getFromZone() == Zone.BATTLEFIELD) {
    Permanent permanent=game.getPermanent(event.getTargetId());
    if (permanent == null) {
      permanent=(Permanent)game.getLastKnownInformation(event.getTargetId(),Zone.BATTLEFIELD);
    }
    if (permanent != null && permanent.getCardType().contains(CardType.LAND)) {
      return true;
    }
  }
  return false;
}
