{
  super(ownerId,setInfo,new CardType[]{CardType.CREATURE},"{2}{U}");
  this.subtype.add("Sliver");
  this.power=new MageInt(1);
  this.toughness=new MageInt(1);
  this.addAbility(new SimpleStaticAbility(Zone.BATTLEFIELD,new GainAbilityAllEffect(ShadowAbility.getInstance(),Duration.WhileOnBattlefield,filter,"All Sliver creatures have shadow.")));
}
