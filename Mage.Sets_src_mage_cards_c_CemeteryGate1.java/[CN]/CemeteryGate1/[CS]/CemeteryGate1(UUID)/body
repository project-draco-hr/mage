{
  super(ownerId,4,"Cemetery Gate",Rarity.COMMON,new CardType[]{CardType.CREATURE},"{2}{B}");
  this.expansionSetCode="HML";
  this.subtype.add("Wall");
  this.power=new MageInt(0);
  this.toughness=new MageInt(5);
