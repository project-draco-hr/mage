{
  super(ownerId,276,"Game Trail",Rarity.RARE,new CardType[]{CardType.LAND},"");
  this.expansionSetCode="SOI";
  this.addAbility(new AsEntersBattlefieldAbility(new TapSourceUnlessPaysEffect(new RevealTargetFromHandCost(new TargetCardInHand(filter))),"you may reveal a Mountain or Forest card from your hand. If you don't, {this} enters the battlefield tapped"));
  this.addAbility(new RedManaAbility());
  this.addAbility(new GreenManaAbility());
}
