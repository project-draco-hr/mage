{
  if (this.black < mana.getBlack())   return false;
  if (this.blue < mana.getBlue())   return false;
  if (this.white < mana.getWhite())   return false;
  if (this.red < mana.getRed())   return false;
  if (this.green < mana.getGreen())   return false;
  if (this.colorless < mana.getColorless())   return false;
  return true;
}
