{
  if (ability == null || conditionalMana.size() == 0) {
    return 0;
  }
  boolean hasWhite=false;
  for (  ConditionalMana mana : conditionalMana) {
    if (mana.getWhite() > 0) {
      hasWhite=true;
      break;
    }
  }
  if (!hasWhite)   return 0;
  for (  ConditionalMana mana : conditionalMana) {
    if (mana.getWhite() > 0 && mana.apply(ability,game)) {
      return mana.getWhite();
    }
  }
  return 0;
}
