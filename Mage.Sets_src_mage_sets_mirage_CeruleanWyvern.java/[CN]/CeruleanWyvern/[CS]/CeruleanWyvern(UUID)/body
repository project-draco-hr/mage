{
  super(ownerId,57,"Cerulean Wyvern",Rarity.UNCOMMON,new CardType[]{CardType.CREATURE},"{4}{U}");
  this.expansionSetCode="MIR";
  this.subtype.add("Drake");
  this.power=new MageInt(3);
