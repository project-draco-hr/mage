{
  super(ownerId,setInfo,new CardType[]{CardType.CREATURE},"{4}{G}");
  this.subtype.add("Incarnation");
  this.power=new MageInt(4);
  this.toughness=new MageInt(4);
  Ability ability=new BeginningOfUpkeepTriggeredAbility(Zone.GRAVEYARD,new DoIfCostPaid(new ReturnFromGraveyardToHandTargetEffect(),new ManaCostsImpl("{2}{G}")),TargetController.YOU,false,false);
  ability.addTarget(new TargetCardInYourGraveyard(new FilterCreatureCard("creature card from your graveyard")));
  this.addAbility(ability);
}
