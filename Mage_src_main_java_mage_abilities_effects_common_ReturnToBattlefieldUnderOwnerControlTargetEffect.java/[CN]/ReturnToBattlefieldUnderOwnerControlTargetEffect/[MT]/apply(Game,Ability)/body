{
  Player controller=game.getPlayer(source.getControllerId());
  if (controller != null) {
    Card card=game.getCard(targetPointer.getFirst(game,source));
    if (card != null) {
      controller.moveCards(card,Zone.BATTLEFIELD,source,game,tapped,false,true,null);
    }
    return true;
  }
  return false;
}
