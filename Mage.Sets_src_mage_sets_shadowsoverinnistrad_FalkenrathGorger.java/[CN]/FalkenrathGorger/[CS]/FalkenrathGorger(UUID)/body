{
  super(ownerId,155,"Falkenrath Gorger",Rarity.RARE,new CardType[]{CardType.CREATURE},"{R}");
  this.expansionSetCode="SOI";
  this.subtype.add("Vampire");
  this.subtype.add("Berserker");
