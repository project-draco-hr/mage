{
  super(ownerId,119,"Ulcerate",Rarity.UNCOMMON,new CardType[]{CardType.INSTANT},"{B}");
  this.expansionSetCode="M15";
  this.getSpellAbility().addTarget(new TargetCreaturePermanent());
