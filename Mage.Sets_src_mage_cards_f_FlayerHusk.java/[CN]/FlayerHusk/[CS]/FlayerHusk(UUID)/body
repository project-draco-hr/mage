{
  super(ownerId,107,"Flayer Husk",Rarity.COMMON,new CardType[]{CardType.ARTIFACT},"{1}");
  this.expansionSetCode="MBS";
  this.subtype.add("Equipment");
  this.addAbility(new LivingWeaponAbility());
