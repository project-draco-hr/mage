{
  if (mana.black > 0 && this.black > 0) {
    return true;
  }
  if (mana.blue > 0 && this.blue > 0) {
    return true;
  }
  if (mana.red > 0 && this.red > 0) {
    return true;
  }
  if (mana.white > 0 && this.white > 0) {
    return true;
  }
  if (mana.green > 0 && this.green > 0) {
    return true;
  }
  if (mana.colorless > 0 && this.count() > 0) {
    return true;
  }
  return false;
}
