{
  if (filter == null) {
    return count();
  }
  int count=0;
  if (filter.isBlack()) {
    count+=black;
  }
  if (filter.isBlue()) {
    count+=blue;
  }
  if (filter.isWhite()) {
    count+=white;
  }
  if (filter.isGreen()) {
    count+=green;
  }
  if (filter.isRed()) {
    count+=red;
  }
  if (filter.isGeneric()) {
    count+=generic;
  }
  if (filter.isColorless()) {
    count+=colorless;
  }
  return count;
}
