{
  if (mana.black > 0 && this.black > 0) {
    return true;
  }
 else   if (mana.blue > 0 && this.blue > 0) {
    return true;
  }
 else   if (mana.red > 0 && this.red > 0) {
    return true;
  }
 else   if (mana.white > 0 && this.white > 0) {
    return true;
  }
 else   if (mana.green > 0 && this.green > 0) {
    return true;
  }
  return false;
}
