{
  super("Rune-Tail's Essence","");
  supertype.add("Legendary");
  cardType.add(CardType.ENCHANTMENT);
  color.setWhite(true);
  this.addAbility(new SimpleStaticAbility(Zone.BATTLEFIELD,new PreventAllDamageToAllEffect(Duration.WhileOnBattlefield,new FilterControlledCreatureInPlay("creatures you control"))));
}
