{
  super(ownerId,setInfo,new CardType[]{CardType.ENCHANTMENT},"{3}{G}{G}");
  Ability ability=new SimpleActivatedAbility(Zone.BATTLEFIELD,new SearchLibraryPutInPlayEffect(new TargetCardInLibrary(filter),true,Outcome.PutLandInPlay),new ManaCostsImpl("{1}"));
  ability.addCost(new SacrificeTargetCost(new TargetControlledCreaturePermanent()));
  this.addAbility(ability);
}
