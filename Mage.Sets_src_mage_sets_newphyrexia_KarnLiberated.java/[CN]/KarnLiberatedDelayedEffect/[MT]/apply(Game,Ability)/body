{
  ExileZone exile=game.getExile().getExileZone(exileId);
  if (exile != null) {
    Cards cards=new CardsImpl();
    cards.addAll(exile);
    for (    Card card : cards.getCards(game)) {
      card.putOntoBattlefield(game,Zone.EXILED,source.getSourceId(),source.getControllerId());
    }
    return true;
  }
  return false;
}
