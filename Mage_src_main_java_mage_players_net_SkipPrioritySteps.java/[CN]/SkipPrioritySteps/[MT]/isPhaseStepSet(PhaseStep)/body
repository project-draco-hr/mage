{
switch (phaseStep) {
case UPKEEP:
    return isUpkeep();
case DRAW:
  return isDraw();
case PRECOMBAT_MAIN:
return isMain1();
case BEGIN_COMBAT:
return isBeforeCombat();
case END_COMBAT:
return isEndOfCombat();
case POSTCOMBAT_MAIN:
return isMain2();
case END_TURN:
return isEndOfTurn();
default :
return true;
}
}
