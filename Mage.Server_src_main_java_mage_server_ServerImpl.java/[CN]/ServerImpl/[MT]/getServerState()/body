{
  try {
    return new ServerState(GameFactory.getInstance().getGameTypes(),TournamentFactory.getInstance().getTournamentTypes(),PlayerFactory.getInstance().getPlayerTypes().toArray(new String[0]),DeckValidatorFactory.getInstance().getDeckTypes().toArray(new String[0]),testMode);
  }
 catch (  Exception ex) {
    handleException(ex);
  }
  return null;
}
