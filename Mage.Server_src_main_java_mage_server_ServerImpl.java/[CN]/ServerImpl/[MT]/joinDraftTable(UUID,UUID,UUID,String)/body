{
  try {
    boolean ret=GamesRoomManager.getInstance().getRoom(roomId).joinDraftTable(sessionId,tableId,name);
    logger.info("Session " + sessionId + " joined table "+ tableId);
    return ret;
  }
 catch (  Exception ex) {
    if (ex instanceof GameException)     throw (GameException)ex;
    handleException(ex);
  }
  return false;
}
