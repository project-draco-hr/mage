{
  try {
    TableView table=GamesRoomManager.getInstance().getRoom(roomId).createTable(sessionId,gameType,deckType,playerTypes,attackOption,range);
    logger.info("Table " + table.getTableId() + " created");
    return table;
  }
 catch (  Exception ex) {
    handleException(ex);
  }
  return null;
}
