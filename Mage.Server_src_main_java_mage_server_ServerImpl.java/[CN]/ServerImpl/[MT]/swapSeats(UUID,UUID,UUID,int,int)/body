{
  if (SessionManager.getInstance().isValidSession(sessionId)) {
    try {
      rmiExecutor.execute(new Runnable(){
        @Override public void run(){
          TableManager.getInstance().swapSeats(tableId,sessionId,seatNum1,seatNum2);
        }
      }
);
    }
 catch (    Exception ex) {
      handleException(ex);
    }
  }
}
