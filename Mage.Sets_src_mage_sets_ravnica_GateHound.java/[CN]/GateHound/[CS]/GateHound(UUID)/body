{
  super(ownerId,19,"Gate Hound",Rarity.COMMON,new CardType[]{CardType.CREATURE},"{2}{W}");
  this.expansionSetCode="RAV";
  this.subtype.add("Hound");
  this.color.setWhite(true);
  this.power=new MageInt(1);
  this.toughness=new MageInt(1);
  this.addAbility(new SimpleStaticAbility(Zone.BATTLEFIELD,new ConditionalContinuousEffect(new GainAbilityControlledEffect(VigilanceAbility.getInstance(),Duration.WhileOnBattlefield,new FilterCreaturePermanent()),new EnchantedCondition(),"Creatures you control have vigilance as long as {this} is enchanted")));
}
