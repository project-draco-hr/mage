{
  super("Dragon","5/5 red Dragon creature token with flying");
  cardType.add(CardType.CREATURE);
  color.setWhite(true);
  subtype.add("Dragon");
  power=new MageInt(5);
  toughness=new MageInt(5);
  addAbility(FlyingAbility.getInstance());
  availableImageSetCodes.addAll(Arrays.asList("WWK","10E"));
}
