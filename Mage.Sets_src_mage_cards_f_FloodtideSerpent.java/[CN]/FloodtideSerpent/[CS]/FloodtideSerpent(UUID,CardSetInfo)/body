{
  super(ownerId,setInfo,new CardType[]{CardType.CREATURE},"{4}{U}");
  this.subtype.add("Serpent");
  this.power=new MageInt(4);
  this.toughness=new MageInt(4);
  this.addAbility(new SimpleStaticAbility(Zone.BATTLEFIELD,new CantAttackBlockUnlessPaysSourceEffect(new ReturnToHandChosenControlledPermanentCost(new TargetControlledPermanent(filter)),PayCostToAttackBlockEffectImpl.RestrictType.ATTACK)));
}
