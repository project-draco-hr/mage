{
  List<String> keysToRemove=new ArrayList<String>();
  for (  Map.Entry<String,List<UUID>> entry : sources.entrySet()) {
    if (entry.getValue().contains(sourceId)) {
      keysToRemove.add(entry.getKey());
    }
  }
  for (  String key : keysToRemove) {
    sources.remove(key);
  }
  return keysToRemove;
}
