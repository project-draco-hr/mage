{
  super(ownerId,setInfo,new CardType[]{CardType.LAND},"");
  this.addAbility(new ColorlessManaAbility());
  this.addAbility(new SimpleActivatedAbility(Zone.BATTLEFIELD,new AddContinuousEffectToGame(new CastAsThoughItHadFlashAllEffect(Duration.EndOfTurn,filter)),new CompositeCost(new ManaCostsImpl("{G}{U}"),new TapSourceCost(),"{G}{U}, {T}")));
}
