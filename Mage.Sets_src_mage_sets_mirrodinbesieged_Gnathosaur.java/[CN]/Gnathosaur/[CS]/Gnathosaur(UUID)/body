{
  super(ownerId,63,"Gnathosaur",Rarity.COMMON,new CardType[]{CardType.CREATURE},"{4}{R}{R}");
  this.expansionSetCode="MBS";
  this.subtype.add("Lizard");
  this.color.setRed(true);
  this.power=new MageInt(5);
  this.toughness=new MageInt(4);
  this.addAbility(new SimpleActivatedAbility(Constants.Zone.BATTLEFIELD,new GainAbilityTargetEffect(TrampleAbility.getInstance(),Constants.Duration.EndOfTurn),new SacrificeTargetCost(new TargetControlledPermanent(filter))));
}
