{
  super(ownerId,176,"Circle of Elders",Rarity.UNCOMMON,new CardType[]{CardType.CREATURE},"{2}{G}{G}");
  this.expansionSetCode="DTK";
  this.subtype.add("Human");
  this.subtype.add("Shaman");
  this.power=new MageInt(2);
  this.toughness=new MageInt(4);
  this.addAbility(VigilanceAbility.getInstance());
  Ability ability=new ActivateIfConditionManaAbility(Zone.BATTLEFIELD,new BasicManaEffect(new Mana(0,0,0,0,0,3,0)),new TapSourceCost(),FormidableCondition.getInstance());
  ability.setAbilityWord(AbilityWord.FORMIDABLE);
  this.addAbility(ability);
}
