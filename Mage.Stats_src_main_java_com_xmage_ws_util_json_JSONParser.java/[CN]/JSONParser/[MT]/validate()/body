{
  if (this.json == null) {
    throw new JSONValidationException("JSON is null");
  }
  try {
    this.root=(JSONObject)JSONValue.parse(this.json);
    if (this.root == null) {
      throw new JSONValidationException("Root json is null");
    }
  }
 catch (  Exception e) {
    throw new JSONValidationException("JSON is not valid",e);
  }
}
