{
  super(ownerId,130,"Village Survivors",Rarity.UNCOMMON,new CardType[]{CardType.CREATURE},"{4}{G}");
  this.expansionSetCode="DKA";
  this.subtype.add("Human");
  this.color.setGreen(true);
  this.power=new MageInt(4);
  this.toughness=new MageInt(5);
  this.addAbility(VigilanceAbility.getInstance());
  this.addAbility(new SimpleStaticAbility(Constants.Zone.BATTLEFIELD,new ConditionalContinousEffect(new GainAbilityControlledEffect(VigilanceAbility.getInstance(),Constants.Duration.WhileOnBattlefield),MorbidCondition.getInstance(),"Fateful hour - As long as you have 5 or less life, other creatures you control have vigilance")));
}
