{
  super(ownerId,81,"Tolarian Emissary",Rarity.UNCOMMON,new CardType[]{CardType.CREATURE},"{2}{U}");
  this.expansionSetCode="INV";
  this.subtype.add("Human");
  this.subtype.add("Wizard");
  this.power=new MageInt(1);
  this.toughness=new MageInt(2);
  this.addAbility(new KickerAbility("{1}{W}"));
  this.addAbility(FlyingAbility.getInstance());
  TriggeredAbility ability=new EntersBattlefieldTriggeredAbility(new DestroyTargetEffect());
  ability.addTarget(new TargetEnchantmentPermanent());
  this.addAbility(new ConditionalTriggeredAbility(ability,KickedCondition.getInstance(),"When {this} enters the battlefield, if it was kicked, destroy target enchantment."));
}
