{
  super(ownerId,14,"Leonin Arbiter",Rarity.RARE,new CardType[]{CardType.CREATURE},"{1}{W}");
  this.expansionSetCode="SOM";
  this.subtype.add("Cat");
  this.subtype.add("Cleric");
