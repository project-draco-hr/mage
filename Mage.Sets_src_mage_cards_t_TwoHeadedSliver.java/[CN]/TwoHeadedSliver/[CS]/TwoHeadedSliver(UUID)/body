{
  super(ownerId,183,"Two-Headed Sliver",Rarity.COMMON,new CardType[]{CardType.CREATURE},"{1}{R}");
  this.expansionSetCode="TSP";
  this.subtype.add("Sliver");
  this.power=new MageInt(1);
  this.toughness=new MageInt(1);
  this.addAbility(new SimpleStaticAbility(Zone.BATTLEFIELD,new GainAbilityAllEffect(new MenaceAbility(),Duration.WhileOnBattlefield,filter,"All Sliver creatures have menace. (They can't be blocked except by two or more creatures.)")));
}
