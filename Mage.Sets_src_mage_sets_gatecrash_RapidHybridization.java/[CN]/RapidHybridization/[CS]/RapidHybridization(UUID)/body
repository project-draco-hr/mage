{
  super(ownerId,44,"Rapid Hybridization",Rarity.UNCOMMON,new CardType[]{CardType.INSTANT},"{U}");
  this.expansionSetCode="GTC";
  this.getSpellAbility().addTarget(new TargetCreaturePermanent());
