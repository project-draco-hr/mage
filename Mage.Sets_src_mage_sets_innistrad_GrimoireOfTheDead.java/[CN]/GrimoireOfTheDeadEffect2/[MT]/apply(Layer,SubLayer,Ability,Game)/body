{
  Permanent permanent=game.getPermanent(targetId);
  if (permanent != null) {
switch (layer) {
case ColorChangingEffects_5:
      if (sublayer == SubLayer.NA) {
        permanent.getColor(game).setBlack(true);
      }
    break;
case TypeChangingEffects_4:
  if (sublayer == SubLayer.NA) {
    permanent.getSubtype(game).add("Zombie");
  }
break;
}
return true;
}
return false;
}
