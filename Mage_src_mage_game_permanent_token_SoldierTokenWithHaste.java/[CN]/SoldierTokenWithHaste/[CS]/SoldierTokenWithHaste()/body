{
  super("Soldier","1/1 red and white Soldier creature token with haste");
  cardType.add(CardType.CREATURE);
  color.setWhite(true);
  color.setRed(true);
  subtype.add("Soldier");
  power=new MageInt(1);
  toughness=new MageInt(1);
  addAbility(HasteAbility.getInstance());
  setOriginalExpansionSetCode("GTC");
}
