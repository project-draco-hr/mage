{
  super(Zone.BATTLEFIELD,new ConditionalOneShotEffect(new AddCountersSourceEffect(CounterType.P1P1.createInstance(monstrosityValue)),new InvertCondition(MonstrousCondition.getInstance()),""),new ManaCostsImpl(manaString));
  this.addEffect(new ConditionalOneShotEffect(new BecomeMonstrousSourceEffect(),new InvertCondition(MonstrousCondition.getInstance()),""));
  this.monstrosityValue=monstrosityValue;
}
