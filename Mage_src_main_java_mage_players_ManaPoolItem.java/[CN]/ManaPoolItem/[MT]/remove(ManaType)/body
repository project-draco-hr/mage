{
  int oldCount=count();
switch (manaType) {
case BLACK:
    if (black > 0) {
      black--;
    }
  break;
case BLUE:
if (blue > 0) {
  blue--;
}
break;
case GREEN:
if (green > 0) {
green--;
}
break;
case RED:
if (red > 0) {
red--;
}
break;
case WHITE:
if (white > 0) {
white--;
}
break;
case COLORLESS:
if (colorless > 0) {
colorless--;
}
break;
}
if (stock == oldCount && oldCount > count()) {
stock--;
}
}
