{
  if (black > 0) {
    return ManaType.BLACK;
  }
 else   if (blue > 0) {
    return ManaType.BLUE;
  }
 else   if (green > 0) {
    return ManaType.GREEN;
  }
 else   if (red > 0) {
    return ManaType.RED;
  }
 else   if (white > 0) {
    return ManaType.WHITE;
  }
 else   if (colorless > 0) {
    return ManaType.COLORLESS;
  }
  return null;
}
