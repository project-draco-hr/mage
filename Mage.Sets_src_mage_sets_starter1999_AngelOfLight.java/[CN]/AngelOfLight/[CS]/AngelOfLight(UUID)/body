{
  super(ownerId,1,"Angel of Light",Rarity.UNCOMMON,new CardType[]{CardType.CREATURE},"{4}{W}");
  this.expansionSetCode="S99";
  this.subtype.add("Angel");
  this.power=new MageInt(3);
  this.toughness=new MageInt(3);
