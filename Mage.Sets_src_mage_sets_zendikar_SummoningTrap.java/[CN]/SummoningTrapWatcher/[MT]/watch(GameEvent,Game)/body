{
  if (condition == true)   return;
  if (event.getType() == EventType.COUNTERED && game.getStack().getStackObject(event.getTargetId()).getCardType().contains(CardType.CREATURE) && game.getOpponents(controllerId).contains(event.getPlayerId()))   Logger.getAnonymousLogger().info(game.getCard(event.getTargetId()).toString());
  condition=true;
}
