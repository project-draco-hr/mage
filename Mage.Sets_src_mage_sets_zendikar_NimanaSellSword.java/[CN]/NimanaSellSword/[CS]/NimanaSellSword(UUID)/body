{
  super(ownerId,106,"Nimana Sell-Sword",Rarity.COMMON,new CardType[]{CardType.CREATURE},"{3}{B}");
  this.expansionSetCode="ZEN";
  this.subtype.add("Human");
  this.subtype.add("Warrior");
  this.subtype.add("Ally");
