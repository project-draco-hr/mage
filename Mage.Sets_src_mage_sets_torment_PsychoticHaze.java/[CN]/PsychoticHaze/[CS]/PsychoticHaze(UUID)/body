{
  super(ownerId,76,"Psychotic Haze",Rarity.COMMON,new CardType[]{CardType.INSTANT},"{2}{B}{B}");
  this.expansionSetCode="TOR";
  this.color.setBlack(true);
  this.getSpellAbility().addEffect(new DamageEverythingEffect(1));
