{
  super(ownerId,92,"Progenitor Mimic",Rarity.MYTHIC,new CardType[]{CardType.CREATURE},"{4}{G}{U}");
  this.expansionSetCode="DGM";
  this.subtype.add("Shapeshifter");
  this.color.setBlue(true);
  this.color.setGreen(true);
  this.power=new MageInt(0);
  this.toughness=new MageInt(0);
  this.addAbility(new SimpleStaticAbility(Zone.BATTLEFIELD,new EntersBattlefieldEffect(new CopyPermanentEffect(new ProgenitorMimicApplyToPermanent()),"You may have {this} enter the battlefield as a copy of any creature on the battlefield except it gains \"At the beginning of your upkeep, if this creature isn't a token, put a token onto the battlefield that's a copy of this creature.\"",true)));
}
