{
  Ability ability=new ConditionalTriggeredAbility(new BeginningOfUpkeepTriggeredAbility(new ProgenitorMimicCopyEffect(),TargetController.YOU,false),new SourceMatchesFilterCondition(filter),"At the beginning of your upkeep, if this creature isn't a token, put a token onto the battlefield that's a copy of this creature.");
  permanent.addAbility(ability,game);
  return true;
}
