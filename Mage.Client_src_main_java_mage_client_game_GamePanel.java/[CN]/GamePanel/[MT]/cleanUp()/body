{
  this.gameChatPanel.disconnect();
  this.players.clear();
  logger.debug("players clear.");
  this.pnlBattlefield.removeAll();
  combat.hideDialog();
  pickNumber.hideDialog();
  for (  ExileZoneDialog exile : exiles.values()) {
    exile.hideDialog();
  }
  for (  ShowCardsDialog reveal : revealed.values()) {
    reveal.hideDialog();
  }
}
