{
  return original.replace("\u2014","-").replace("\u00E9","e").replace("\u00C6","AE").replace("\u00E6","ae").replace("\u00C1","A").replace("\u00E1","a").replace("\u00C2","A").replace("\u00E2","a").replace("\u00D6","O").replace("\u00F6","o").replace("\u00DB","U").replace("\u00FB","u").replace("\u00DC","U").replace("\u00FC","u");
}
