{
  for (Iterator<T> i=this.iterator(); i.hasNext(); ) {
    T entry=i.next();
    if (entry.equals(effect)) {
      HashSet<Ability> abilities=effectAbilityMap.get(effect.getId());
      if (!abilities.isEmpty()) {
        abilities.remove(ability);
      }
      if (abilities.isEmpty()) {
        i.remove();
      }
    }
  }
}
