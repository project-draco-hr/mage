{
  List<Card> foundCards=new ArrayList<Card>();
  for (  Card card : cards) {
    if (name.equalsIgnoreCase(card.getName())) {
      foundCards.add(card);
    }
  }
  if (foundCards.size() > 0) {
    Card newCard=foundCards.get(rnd.nextInt(foundCards.size())).copy();
    newCard.assignNewId();
    return newCard;
  }
  return null;
}
