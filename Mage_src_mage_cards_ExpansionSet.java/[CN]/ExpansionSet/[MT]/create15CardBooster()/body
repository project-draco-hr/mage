{
  List<Card> theBooster=this.createBooster();
  List<CardInfo> commons=getCardsByRarity(Rarity.COMMON);
  int iterations=0;
  while (15 > theBooster.size() && !commons.isEmpty()) {
    addToBooster(theBooster,commons);
    iterations++;
    if (iterations > 14) {
      break;
    }
  }
  while (theBooster.size() > 15) {
    theBooster.remove(0);
  }
  return theBooster;
}
