{
  super(ownerId,174,"Lazav, Dimir Mastermind",Rarity.MYTHIC,new CardType[]{CardType.CREATURE},"{U}{U}{B}{B}");
  this.expansionSetCode="GTC";
  this.supertype.add("Legendary");
  this.subtype.add("Shapeshifter");
  this.power=new MageInt(3);
  this.toughness=new MageInt(3);
  this.addAbility(HexproofAbility.getInstance());
  this.addAbility(new PutCardIntoGraveFromAnywhereAllTriggeredAbility(new LazavDimirEffect(),true,new FilterCreatureCard("a creature card"),TargetController.OPPONENT,SetTargetPointer.CARD));
}
