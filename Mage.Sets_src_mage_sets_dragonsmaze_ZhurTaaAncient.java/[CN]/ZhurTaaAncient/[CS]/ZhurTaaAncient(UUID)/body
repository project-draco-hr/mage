{
  super(ownerId,119,"Zhur-Taa Ancient",Rarity.RARE,new CardType[]{CardType.CREATURE},"{3}{R}{G}");
  this.expansionSetCode="DGM";
  this.subtype.add("Beast");
  this.color.setRed(true);
  this.color.setGreen(true);
  this.power=new MageInt(7);
  this.toughness=new MageInt(5);
  this.addAbility(new TapForManaAllTriggeredManaAbility(new AddManaOfAnyColorTargetCanProduceEffect(),new FilterLandPermanent("a player taps a land"),SetTargetPointer.PERMANENT));
}
