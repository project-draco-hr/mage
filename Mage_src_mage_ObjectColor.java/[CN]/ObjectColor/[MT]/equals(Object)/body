{
  if (this == color)   return true;
  if (!(color instanceof ObjectColor))   return false;
  ObjectColor test=(ObjectColor)color;
  if (test.white != this.white)   return false;
  if (test.blue != this.blue)   return false;
  if (test.black != this.black)   return false;
  if (test.red != this.red)   return false;
  if (test.green != this.green)   return false;
  return true;
}
