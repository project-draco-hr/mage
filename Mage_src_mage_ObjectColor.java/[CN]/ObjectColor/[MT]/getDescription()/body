{
  StringBuilder sb=new StringBuilder();
  if (getColorCount() > 1) {
    return "multicolored";
  }
 else {
    if (white)     return "white";
    if (blue)     return "blue";
    if (black)     return "black";
    if (red)     return "red";
    if (green)     return "green";
  }
  return "colorless";
}
