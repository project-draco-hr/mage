{
  List<ObjectColor> colors=new ArrayList<>();
  if (this.isWhite()) {
    colors.add(ObjectColor.WHITE);
  }
  if (this.isBlue()) {
    colors.add(ObjectColor.BLUE);
  }
  if (this.isBlack()) {
    colors.add(ObjectColor.BLACK);
  }
  if (this.isRed()) {
    colors.add(ObjectColor.RED);
  }
  if (this.isGreen()) {
    colors.add(ObjectColor.GREEN);
  }
  return colors;
}
