{
  super(ownerId,121,"Shivan Wumpus",Rarity.RARE,new CardType[]{CardType.CREATURE},"{3}{R}");
  this.expansionSetCode="PLC";
  this.subtype.add("Beast");
  this.power=new MageInt(6);
  this.toughness=new MageInt(6);
  this.addAbility(TrampleAbility.getInstance());
  this.addAbility(new EntersBattlefieldTriggeredAbility(new DoIfAnyPlayerPaysEffect(new PutOnLibrarySourceEffect(true),new SacrificeTargetCost(new TargetControlledPermanent(new FilterControlledLandPermanent("a land"))),"Sacrifice a land to return {this} to top of its owners library?"),false));
}
