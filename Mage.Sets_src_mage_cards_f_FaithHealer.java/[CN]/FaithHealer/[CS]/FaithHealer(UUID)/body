{
  super(ownerId,14,"Faith Healer",Rarity.RARE,new CardType[]{CardType.CREATURE},"{1}{W}");
  this.expansionSetCode="USG";
  this.subtype.add("Human");
  this.subtype.add("Cleric");
  this.power=new MageInt(1);
  this.toughness=new MageInt(1);
  this.addAbility(new SimpleActivatedAbility(Zone.BATTLEFIELD,new GainLifeEffect(new SacrificeCostConvertedMana("enchantment")),new SacrificeTargetCost(new TargetControlledPermanent(new FilterControlledEnchantmentPermanent()))));
}
