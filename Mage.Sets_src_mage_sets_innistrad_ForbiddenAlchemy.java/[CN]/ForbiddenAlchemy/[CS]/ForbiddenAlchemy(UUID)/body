{
  super(ownerId,55,"Forbidden Alchemy",Rarity.COMMON,new CardType[]{CardType.INSTANT},"{2}{U}");
  this.expansionSetCode="ISD";
  this.color.setBlue(true);
  this.getSpellAbility().addEffect(new ForbiddenAlchemyEffect());
