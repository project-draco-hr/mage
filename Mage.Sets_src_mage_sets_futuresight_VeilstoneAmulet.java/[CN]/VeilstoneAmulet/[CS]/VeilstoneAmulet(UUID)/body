{
  super(ownerId,166,"Veilstone Amulet",Rarity.RARE,new CardType[]{CardType.ARTIFACT},"{3}");
  this.expansionSetCode="FUT";
  this.addAbility(new SpellCastControllerTriggeredAbility(new VeilstoneAmuletEffect(),new FilterSpell(),false,"Whenever you cast a spell, creatures you control can't be the targets of spells or abilities your opponents control this turn."));
}
