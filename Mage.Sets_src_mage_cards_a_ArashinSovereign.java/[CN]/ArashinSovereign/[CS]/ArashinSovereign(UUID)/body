{
  super(ownerId,212,"Arashin Sovereign",Rarity.RARE,new CardType[]{CardType.CREATURE},"{5}{G}{W}");
  this.expansionSetCode="DTK";
  this.subtype.add("Dragon");
  this.power=new MageInt(6);
