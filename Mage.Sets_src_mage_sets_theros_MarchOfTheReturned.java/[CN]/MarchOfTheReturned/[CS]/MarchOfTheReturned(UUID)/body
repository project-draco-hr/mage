{
  super(ownerId,96,"March of the Returned",Rarity.COMMON,new CardType[]{CardType.SORCERY},"{3}{B}");
  this.expansionSetCode="THS";
  this.color.setBlack(true);
  Effect effect=new ReturnFromGraveyardToHandTargetEffect();
  effect.setText("Return up to two target creature cards from your graveyard to your hand");
  this.getSpellAbility().addEffect(effect);
  this.getSpellAbility().addTarget(new TargetCardInYourGraveyard(0,2,new FilterCreatureCard()));
}
