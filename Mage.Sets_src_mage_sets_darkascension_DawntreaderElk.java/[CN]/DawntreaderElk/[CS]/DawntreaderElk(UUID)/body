{
  super(ownerId,111,"Dawntreader Elk",Rarity.COMMON,new CardType[]{CardType.CREATURE},"{1}{G}");
  this.expansionSetCode="DKA";
  this.subtype.add("Elk");
  this.color.setGreen(true);
  this.power=new MageInt(2);
  this.toughness=new MageInt(2);
  SimpleActivatedAbility ability=new SimpleActivatedAbility(Zone.BATTLEFIELD,new SearchLibraryPutInPlayEffect(new TargetCardInLibrary(new FilterBasicLandCard()),true),new ColoredManaCost(ColoredManaSymbol.G));
  ability.addCost(new SacrificeSourceCost());
  this.addAbility(ability);
}
