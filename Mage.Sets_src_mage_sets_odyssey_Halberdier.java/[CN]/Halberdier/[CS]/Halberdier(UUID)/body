{
  super(ownerId,196,"Halberdier",Rarity.COMMON,new CardType[]{CardType.CREATURE},"{3}{R}");
  this.expansionSetCode="ODY";
  this.subtype.add("Human");
  this.subtype.add("Barbarian");
