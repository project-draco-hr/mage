{
  super(ownerId,120,"Gloryscale Viashino",Rarity.UNCOMMON,new CardType[]{CardType.CREATURE},"{1}{R}{G}{W}");
  this.expansionSetCode="ARB";
  this.subtype.add("Viashino");
