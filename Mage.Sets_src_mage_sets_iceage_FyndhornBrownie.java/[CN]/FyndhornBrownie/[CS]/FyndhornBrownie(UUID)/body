{
  super(ownerId,130,"Fyndhorn Brownie",Rarity.COMMON,new CardType[]{CardType.CREATURE},"{2}{G}");
  this.expansionSetCode="ICE";
  this.subtype.add("Ouphe");
  this.color.setGreen(true);
  this.power=new MageInt(1);
  this.toughness=new MageInt(1);
  Ability ability=new SimpleActivatedAbility(Zone.BATTLEFIELD,new UntapTargetEffect(),new ManaCostsImpl("{2}{G}"));
  ability.addCost(new TapSourceCost());
  ability.addTarget(new TargetCreaturePermanent(true));
  this.addAbility(ability);
}
