{
  if (zone != null) {
    if (object.getZone().match(zone) == notZone)     return notFilter;
  }
  if (outcomes.size() > 0) {
    if (!compOutcome.compare(outcomes,object.getEffects().getOutcomes(),scopeOutcome,notOutcome))     return notFilter;
  }
  if (types.size() > 0) {
    if (types.contains(object.getAbilityType()) == notType)     return notFilter;
  }
  return !notFilter;
}
