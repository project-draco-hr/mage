{
  super(ownerId,248,"Dack's Duplicate",Rarity.RARE,new CardType[]{CardType.CREATURE},"{2}{U}{R}");
  this.expansionSetCode="VMA";
  this.subtype.add("Shapeshifter");
  this.color.setRed(true);
  this.color.setBlue(true);
  this.power=new MageInt(0);
  this.toughness=new MageInt(0);
  this.addAbility(new SimpleStaticAbility(Zone.BATTLEFIELD,new EntersBattlefieldEffect(new CopyPermanentEffect(new DacksDuplicateApplyToPermanent()),"You may have {this} enter the battlefield as a copy of any creature on the battlefield except it gains haste and dethrone",true)));
}
