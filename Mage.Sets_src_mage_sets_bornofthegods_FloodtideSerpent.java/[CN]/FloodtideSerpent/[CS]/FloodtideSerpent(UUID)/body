{
  super(ownerId,41,"Floodtide Serpent",Rarity.COMMON,new CardType[]{CardType.CREATURE},"{4}{U}");
  this.expansionSetCode="BNG";
  this.subtype.add("Serpent");
  this.power=new MageInt(4);
  this.toughness=new MageInt(4);
  this.addAbility(new SimpleStaticAbility(Zone.BATTLEFIELD,new CantAttackBlockUnlessPaysSourceEffect(new ReturnToHandTargetPermanentCost(new TargetControlledPermanent(filter)),PayCostToAttackBlockEffectImpl.RestrictType.ATTACK)));
}
