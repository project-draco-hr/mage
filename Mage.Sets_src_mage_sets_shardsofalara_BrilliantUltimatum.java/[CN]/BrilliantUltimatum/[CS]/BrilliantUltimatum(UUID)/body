{
  super(ownerId,159,"Brilliant Ultimatum",Rarity.RARE,new CardType[]{CardType.SORCERY},"{B}{B}");
  this.expansionSetCode="ALA";
  this.color.setBlue(true);
  this.color.setBlack(true);
