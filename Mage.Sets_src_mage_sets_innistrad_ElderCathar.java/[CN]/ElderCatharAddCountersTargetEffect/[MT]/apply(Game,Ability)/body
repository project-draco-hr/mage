{
  Permanent permanent=game.getPermanent(targetPointer.getFirst(game,source));
  if (permanent != null) {
    if (counter != null) {
      if (permanent.hasSubtype("Human")) {
        permanent.addCounters(counter2.copy(),game);
      }
 else {
        permanent.addCounters(counter.copy(),game);
      }
      return true;
    }
  }
  return false;
}
