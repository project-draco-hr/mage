{
  super(ownerId,179,"Wasp Lancer",Rarity.UNCOMMON,new CardType[]{CardType.CREATURE},"{U/B}{U/B}{U/B}");
  this.expansionSetCode="SHM";
  this.subtype.add("Faerie");
  this.subtype.add("Soldier");
