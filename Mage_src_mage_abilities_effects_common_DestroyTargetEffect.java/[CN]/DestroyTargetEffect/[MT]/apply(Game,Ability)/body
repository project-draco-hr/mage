{
  int affectedTargets=0;
  for (  UUID permanentId : source.getTargets().get(0).getTargets()) {
    Permanent permanent=game.getPermanent(permanentId);
    if (permanent != null) {
      permanent.destroy(source.getId(),game,noRegen);
      affectedTargets++;
    }
  }
  return affectedTargets > 0;
}
