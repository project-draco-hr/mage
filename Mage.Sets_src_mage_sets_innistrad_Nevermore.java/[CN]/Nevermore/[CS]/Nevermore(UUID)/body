{
  super(ownerId,25,"Nevermore",Rarity.RARE,new CardType[]{CardType.ENCHANTMENT},"{1}{W}{W}");
  this.expansionSetCode="ISD";
  this.color.setWhite(true);
  this.addAbility(new AsEntersBattlefieldAbility(new NevermoreEffect1()));
