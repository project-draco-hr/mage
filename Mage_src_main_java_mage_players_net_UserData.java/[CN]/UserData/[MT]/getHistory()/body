{
  if (UserGroup.COMPUTER.equals(this.groupId)) {
    return "";
  }
  return "Matches: " + this.matchHistory + " ("+ this.matchQuitRatio+ "%), Tourneys: "+ this.tourneyHistory+ " ("+ this.tourneyQuitRatio+ "%)"+ ", Constructed Rating: "+ getConstructedRating()+ ", Limited Rating: "+ getLimitedRating();
}
