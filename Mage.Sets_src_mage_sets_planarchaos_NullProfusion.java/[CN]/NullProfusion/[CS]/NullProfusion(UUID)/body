{
  super(ownerId,89,"Null Profusion",Rarity.RARE,new CardType[]{CardType.ENCHANTMENT},"{4}{B}{B}");
  this.expansionSetCode="PLC";
  this.addAbility(new SimpleStaticAbility(Zone.BATTLEFIELD,new SkipDrawStepEffect()));
