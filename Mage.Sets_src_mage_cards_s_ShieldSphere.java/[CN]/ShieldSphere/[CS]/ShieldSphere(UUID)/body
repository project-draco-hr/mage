{
  super(ownerId,172,"Shield Sphere",Rarity.UNCOMMON,new CardType[]{CardType.ARTIFACT,CardType.CREATURE},"{0}");
  this.expansionSetCode="ALL";
  this.subtype.add("Wall");
  this.power=new MageInt(0);
