{
  TableView tableView=view.get(0);
  if (count == 0) {
    count=getPlayersCount(tableView);
  }
 else {
    int current=getPlayersCount(tableView);
    if (current != count) {
      count=current;
      if (count > 0) {
        MageTray.getInstance().blink();
      }
    }
  }
  dialog.update(tableView);
}
