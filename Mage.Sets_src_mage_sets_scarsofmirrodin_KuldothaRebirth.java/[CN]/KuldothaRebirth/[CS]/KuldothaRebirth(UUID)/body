{
  super(ownerId,96,"Kuldotha Rebirth",Rarity.COMMON,new CardType[]{CardType.SORCERY},"{R}");
  this.expansionSetCode="SOM";
  this.color.setRed(true);
  this.getSpellAbility().addEffect(new CreateTokenEffect(new GoblinToken(),3));
