{
  super(ownerId,192,"Ivory Guardians",Rarity.UNCOMMON,new CardType[]{CardType.CREATURE},"{4}{W}{W}");
  this.expansionSetCode="LEG";
  this.subtype.add("Giant");
  this.subtype.add("Cleric");
