{
  super(ownerId,52,"Diminish",Rarity.COMMON,new CardType[]{CardType.INSTANT},"{U}");
  this.expansionSetCode="M11";
  this.color.setBlue(true);
  this.getSpellAbility().addTarget(new TargetCreaturePermanent(true));
