{
  if (this == obj) {
    return true;
  }
  if (obj == null) {
    return false;
  }
  if (getClass() != obj.getClass()) {
    return false;
  }
  final TargetAddress other=(TargetAddress)obj;
  if (this.spellAbilityIndex != other.spellAbilityIndex) {
    return false;
  }
  if (this.targetIndex != other.targetIndex) {
    return false;
  }
  return Objects.equals(this.mode,other.mode);
}
