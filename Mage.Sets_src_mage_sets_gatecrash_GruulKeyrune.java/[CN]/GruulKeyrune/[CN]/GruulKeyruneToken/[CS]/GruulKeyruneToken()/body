{
  super("Beast","a 3/2 red and green Beast artifact creature with trample");
  cardType.add(CardType.ARTIFACT);
  cardType.add(CardType.CREATURE);
  color.setRed(true);
  color.setGreen(true);
  subtype.add("Beast");
  power=new MageInt(3);
  toughness=new MageInt(2);
  this.addAbility(TrampleAbility.getInstance());
}
