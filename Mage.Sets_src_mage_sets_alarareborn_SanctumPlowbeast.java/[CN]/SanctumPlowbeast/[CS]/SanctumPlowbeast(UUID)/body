{
  super(ownerId,10,"Sanctum Plowbeast",Rarity.COMMON,new CardType[]{CardType.ARTIFACT,CardType.CREATURE},"{4}{W}{U}");
  this.expansionSetCode="ARB";
  this.subtype.add("Beast");
