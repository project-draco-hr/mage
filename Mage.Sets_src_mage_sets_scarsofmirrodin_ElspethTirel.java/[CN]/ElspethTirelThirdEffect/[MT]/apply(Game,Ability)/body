{
  for (  Permanent perm : game.getBattlefield().getActivePermanents(source.getControllerId(),game)) {
    if (!perm.getId().equals(source.getSourceId()) && !(perm instanceof PermanentToken) && !(perm.getCardType().contains(CardType.LAND)))     perm.destroy(source.getId(),game,false);
  }
  return true;
}
