{
  this.bigCard=bigCard;
  this.gameId=gameId;
  cards.clear();
  for (  CardView card : showCards) {
    Card cardImg=new Card(card,bigCard,Config.dimensions,gameId);
    cardImg.update(card);
    cardImg.addMouseListener(this);
    cards.add(cardImg);
  }
  drawCards();
  this.setVisible(true);
}
