{
  boolean result=true;
  if (checkIfClause(game)) {
    for (    Effect effect : getEffects()) {
      if (effect instanceof OneShotEffect) {
        if (!(effect instanceof PostResolveEffect)) {
          result&=effect.apply(game,this);
          game.applyEffects();
        }
      }
 else {
        game.addEffect((ContinuousEffect)effect,this);
      }
    }
  }
  return result;
}
