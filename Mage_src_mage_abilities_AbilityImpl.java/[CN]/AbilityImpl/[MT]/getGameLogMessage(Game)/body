{
  if (game.isSimulation()) {
    return "";
  }
  MageObject object=game.getObject(this.sourceId);
  if (object == null) {
    logger.warn("Could get no object: " + this.toString());
  }
  return new StringBuilder(" activates: ").append(object != null ? this.formatRule(modes.getText(),object.getName()) : modes.getText()).append(" from ").append(getMessageText(game)).toString();
}
