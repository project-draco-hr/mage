{
  super(ownerId,117,"Hound of the Farbogs",Rarity.COMMON,new CardType[]{CardType.CREATURE},"{4}{B}");
  this.expansionSetCode="SOI";
  this.subtype.add("Zombie");
  this.subtype.add("Hound");
  this.power=new MageInt(5);
  this.toughness=new MageInt(3);
  this.addAbility(new SimpleStaticAbility(Zone.BATTLEFIELD,new ConditionalContinuousEffect(new GainAbilitySourceEffect(new MenaceAbility(),Duration.WhileOnBattlefield),new DeliriumCondition(),rule)));
}
