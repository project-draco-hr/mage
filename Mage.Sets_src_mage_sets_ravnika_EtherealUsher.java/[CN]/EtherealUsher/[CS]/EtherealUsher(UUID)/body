{
  super(ownerId,47,"Ethereal Usher",Rarity.UNCOMMON,new CardType[]{CardType.CREATURE},"{5}{U}");
  this.expansionSetCode="RAV";
  this.subtype.add("Spirit");
  this.color.setBlue(true);
  this.power=new MageInt(2);
  this.toughness=new MageInt(3);
  Ability ability=new SimpleActivatedAbility(Zone.BATTLEFIELD,new GainAbilityTargetEffect(UnblockableAbility.getInstance(),Duration.EndOfTurn),new ManaCostsImpl("{U}"));
  ability.addCost(new TapSourceCost());
  ability.addTarget(new TargetCreaturePermanent());
  this.addAbility(ability);
  this.addAbility(new TransmuteAbility("{1}{U}{U}"));
}
