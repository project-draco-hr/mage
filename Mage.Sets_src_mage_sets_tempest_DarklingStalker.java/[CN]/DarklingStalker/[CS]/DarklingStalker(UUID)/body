{
  super(ownerId,13,"Darkling Stalker",Rarity.COMMON,new CardType[]{CardType.CREATURE},"{3}{B}");
  this.expansionSetCode="TMP";
  this.subtype.add("Shade");
  this.subtype.add("Spirit");
  this.color.setBlack(true);
  this.power=new MageInt(1);
  this.toughness=new MageInt(1);
  this.addAbility(new SimpleActivatedAbility(Constants.Zone.BATTLEFIELD,new RegenerateSourceEffect(),new ColoredManaCost(Constants.ColoredManaSymbol.B)));
  this.addAbility(new SimpleActivatedAbility(Constants.Zone.BATTLEFIELD,new BoostSourceEffect(1,1,Constants.Duration.EndOfTurn),new ColoredManaCost(Constants.ColoredManaSymbol.B)));
}
