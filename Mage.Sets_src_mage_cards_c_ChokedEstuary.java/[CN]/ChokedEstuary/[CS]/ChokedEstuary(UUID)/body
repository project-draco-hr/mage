{
  super(ownerId,270,"Choked Estuary",Rarity.RARE,new CardType[]{CardType.LAND},"");
  this.expansionSetCode="SOI";
  this.addAbility(new AsEntersBattlefieldAbility(new TapSourceUnlessPaysEffect(new RevealTargetFromHandCost(new TargetCardInHand(filter))),"you may reveal an Island or Swamp card from your hand. If you don't, {this} enters the battlefield tapped"));
  this.addAbility(new BlueManaAbility());
  this.addAbility(new BlackManaAbility());
}
