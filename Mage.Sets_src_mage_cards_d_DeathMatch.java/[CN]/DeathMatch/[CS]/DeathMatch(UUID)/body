{
  super(ownerId,136,"Death Match",Rarity.RARE,new CardType[]{CardType.ENCHANTMENT},"{3}{B}");
  this.expansionSetCode="ONS";
  Ability ability=new EntersBattlefieldAllTriggeredAbility(Zone.BATTLEFIELD,new DeathMatchEffect(),new FilterCreaturePermanent(),false,SetTargetPointer.PLAYER,"");
  ability.addTarget(new TargetCreaturePermanent());
  this.addAbility(ability);
  originalId=ability.getOriginalId();
}
