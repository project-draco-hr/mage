{
  if (!images.containsKey(ref)) {
    try {
      if (Config.useResource)       images.put(ref,ImageIO.read(ImageHelper.class.getResourceAsStream(ref)));
 else       images.put(ref,ImageIO.read(new File(ref)));
    }
 catch (    Exception e) {
      return null;
    }
  }
  return images.get(ref);
}
