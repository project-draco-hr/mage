{
  InputStream stream;
  stream=UI.class.getResourceAsStream(path);
  if (stream == null) {
    throw new IllegalArgumentException("Couldn't find image in resources: " + path);
  }
  try {
    ImageIO.setUseCache(false);
    BufferedImage image=ImageIO.read(stream);
    return image;
  }
 catch (  IOException e) {
    e.printStackTrace();
  }
  return null;
}
