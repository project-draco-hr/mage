{
  super(ownerId,42,"Minamo's Meddling",Rarity.COMMON,new CardType[]{CardType.INSTANT},"{2}{U}{U}");
  this.expansionSetCode="BOK";
  this.color.setBlue(true);
  this.getSpellAbility().addTarget(new TargetSpell(new FilterSpell()));
