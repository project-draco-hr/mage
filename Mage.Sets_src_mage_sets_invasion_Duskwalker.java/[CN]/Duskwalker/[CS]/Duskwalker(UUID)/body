{
  super(ownerId,104,"Duskwalker",Rarity.COMMON,new CardType[]{CardType.CREATURE},"{B}");
  this.expansionSetCode="INV";
  this.subtype.add("Human");
  this.subtype.add("Minion");
  this.power=new MageInt(1);
  this.toughness=new MageInt(1);
  this.addAbility(new KickerAbility("{3}{B}"));
  Ability ability=new EntersBattlefieldAbility(new AddCountersSourceEffect(CounterType.P1P1.createInstance(2)),KickedCondition.getInstance(),true,"If {this} was kicked, it enters the battlefield with two +1/+1 counters on it and with fear.","");
  ability.addEffect(new GainAbilitySourceEffect(FearAbility.getInstance(),Duration.WhileOnBattlefield));
  this.addAbility(ability);
}
