{
  super(ownerId,setInfo,new CardType[]{CardType.INSTANT},"{3}{G}");
  Effect effect=new ConditionalOneShotEffect(new CreateTokenEffect(new WolfToken(),3),new CreateTokenEffect(new WolfToken(),1),new MorbidCondition(),"Create a 2/2 green Wolf creature token. <br/><br/><i>Morbid</i> - Create three 2/2 green Wolf creature tokens instead if a creature died this turn.");
  this.getSpellAbility().addEffect(effect);
}
