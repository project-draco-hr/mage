{
  if (event.getSourceId() != null && event.getSourceId().equals(getSourceId()) && !event.getSourceId().equals(event.getTargetId())) {
    Permanent sacrificed=game.getPermanentOrLKIBattlefield(event.getTargetId());
    if (sacrificed != null && sacrificed.hasSubtype("Faerie",game)) {
      return true;
    }
  }
  return false;
}
