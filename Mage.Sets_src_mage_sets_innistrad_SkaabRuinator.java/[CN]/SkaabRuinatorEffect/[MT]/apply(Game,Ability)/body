{
  Card target=(Card)game.getObject(targetPointer.getFirst(game,source));
  if (target != null) {
    Player controller=game.getPlayer(target.getOwnerId());
    if (controller != null) {
      return target.cast(game,Zone.GRAVEYARD,target.getSpellAbility(),controller.getId());
    }
  }
  return false;
}
