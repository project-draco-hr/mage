{
  Permanent permanent=(Permanent)game.getLastKnownInformation(target,Zone.BATTLEFIELD);
  if (permanent != null) {
    Player player=game.getPlayer(permanent.getControllerId());
    if (player != null) {
      player.damage(3,source.getSourceId(),game,false,true);
      return true;
    }
  }
  return false;
}
