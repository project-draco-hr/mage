{
  Player player=game.getPlayer(source.getControllerId());
  if (player == null) {
    return false;
  }
  Permanent permanent=game.getPermanent(source.getSourceId());
  if (permanent != null) {
    permanent.moveToZone(Zone.LIBRARY,id,game,false);
    player.shuffleLibrary(game);
    return true;
  }
  return false;
}
