{
  super(ownerId,116,"Phyrexian Infiltrator",Rarity.RARE,new CardType[]{CardType.CREATURE},"{2}{B}");
  this.expansionSetCode="INV";
  this.subtype.add("Minion");
  this.power=new MageInt(2);
  this.toughness=new MageInt(2);
  Ability ability=new SimpleActivatedAbility(Zone.BATTLEFIELD,new ExchangeControlTargetEffect(Duration.EndOfGame,"Exchange control of {this} and target creature",true),new ManaCostsImpl("{2}{U}{U}"));
  ability.addTarget(new TargetCreaturePermanent());
  this.addAbility(ability);
}
