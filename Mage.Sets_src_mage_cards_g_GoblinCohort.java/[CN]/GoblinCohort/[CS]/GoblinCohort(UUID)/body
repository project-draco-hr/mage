{
  super(ownerId,106,"Goblin Cohort",Rarity.COMMON,new CardType[]{CardType.CREATURE},"{R}");
  this.expansionSetCode="BOK";
  this.subtype.add("Goblin");
  this.subtype.add("Warrior");
