{
  super(ownerId,3,"Banishment Decree",Rarity.COMMON,new CardType[]{CardType.INSTANT},"{3}{W}{W}");
  this.expansionSetCode="MBS";
  this.color.setWhite(true);
  this.getSpellAbility().addEffect(new PutOnLibraryTargetEffect(true));
