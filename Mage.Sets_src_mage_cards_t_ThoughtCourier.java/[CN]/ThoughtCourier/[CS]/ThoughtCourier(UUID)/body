{
  super(ownerId,104,"Thought Courier",Rarity.UNCOMMON,new CardType[]{CardType.CREATURE},"{1}{U}");
  this.expansionSetCode="9ED";
  this.subtype.add("Human");
  this.subtype.add("Wizard");
