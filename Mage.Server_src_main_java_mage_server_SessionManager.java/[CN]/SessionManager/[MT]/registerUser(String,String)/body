{
  Session session=sessions.get(sessionId);
  if (session != null) {
    String returnMessage=session.registerUser(userName);
    if (returnMessage == null) {
      LogServiceImpl.instance.log(LogKeys.KEY_USER_CONNECTED,userName,session.getHost(),sessionId);
      logger.info(new StringBuilder("User: ").append(userName).append(" userId: ").append(session.getUserId()).append(" connected from: ").append(session.getHost()).append(" sessionId: ").append(sessionId));
      return true;
    }
 else {
      logger.info(new StringBuilder("User not registered - ").append(returnMessage));
    }
  }
  return false;
}
