{
  super(ownerId,34,"Veteran Armorer",Rarity.COMMON,new CardType[]{CardType.CREATURE},"{1}{W}");
  this.expansionSetCode="RAV";
  this.subtype.add("Human");
  this.subtype.add("Soldier");
