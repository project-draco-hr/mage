{
  Thread.setDefaultUncaughtExceptionHandler(new Thread.UncaughtExceptionHandler(){
    public void uncaughtException(    Thread t,    Throwable e){
      logger.fatal(null,e);
    }
  }
);
  SwingUtilities.invokeLater(new Runnable(){
