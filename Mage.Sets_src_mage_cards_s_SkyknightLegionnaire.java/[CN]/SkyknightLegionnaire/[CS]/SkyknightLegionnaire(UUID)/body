{
  super(ownerId,232,"Skyknight Legionnaire",Rarity.COMMON,new CardType[]{CardType.CREATURE},"{1}{R}{W}");
  this.expansionSetCode="RAV";
  this.subtype.add("Human");
  this.subtype.add("Knight");
  this.power=new MageInt(2);
