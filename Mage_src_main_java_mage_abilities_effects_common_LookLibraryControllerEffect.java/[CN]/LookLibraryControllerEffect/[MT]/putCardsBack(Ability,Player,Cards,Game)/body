{
switch (targetZoneLookedCards) {
case LIBRARY:
    if (putOnTop) {
      player.putCardsOnTopOfLibrary(cards,game,source,true);
    }
 else {
      player.putCardsOnBottomOfLibrary(cards,game,source,true);
    }
  break;
case GRAVEYARD:
player.moveCards(cards,Zone.GRAVEYARD,source,game);
break;
default :
}
}
