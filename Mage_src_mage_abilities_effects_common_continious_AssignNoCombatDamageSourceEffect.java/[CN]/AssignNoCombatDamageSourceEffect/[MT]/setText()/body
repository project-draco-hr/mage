{
  StringBuilder sb=new StringBuilder("{this} assigns no combat damage");
switch (duration) {
case EndOfTurn:
    sb.append("this turn");
  break;
case EndOfCombat:
sb.append("this combat");
break;
default :
if (duration.toString().length() > 0) {
sb.append(" ").append(duration.toString());
}
}
return sb.toString();
}
