{
  StackObject source=game.getStack().getStackObject(input.getSourceId());
  if (source != null) {
    for (    Target target : source.getStackAbility().getTargets()) {
      if (target.getTargetTag() > 0 && target.getTargetTag() != targetTag && target.getTargets().contains(input.getObject().getId())) {
        return false;
      }
    }
  }
  return true;
}
