{
  super(ownerId,setInfo,new CardType[]{CardType.CREATURE},"{2}{G}{G}");
  this.subtype.add("Elf");
  this.subtype.add("Shaman");
  this.power=new MageInt(3);
  this.toughness=new MageInt(2);
  this.addAbility(new EntersBattlefieldTriggeredAbility(new DrawCardSourceControllerEffect(1),false));
  OneShotEffect effect=new ReturnToHandSourceEffect();
  effect.setText("return {this} from your graveyard to your hand");
  this.addAbility(new SpellCastControllerTriggeredAbility(Zone.GRAVEYARD,new DoIfCostPaid(effect,new ManaCostsImpl("{G}{G}")),filter,false,false));
}
