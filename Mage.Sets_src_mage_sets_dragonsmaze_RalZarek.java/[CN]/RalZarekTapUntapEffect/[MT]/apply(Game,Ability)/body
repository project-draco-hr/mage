{
  Player player=game.getPlayer(source.getControllerId());
  if (player != null) {
    int i=0;
    for (    UUID targetId : source.getTargets().get(0).getTargets()) {
      i++;
      Permanent permanent=game.getPermanent(targetId);
      if (permanent != null) {
        if (i == 1) {
          permanent.tap(game);
        }
        if (i == 2) {
          permanent.untap(game);
        }
      }
    }
    return true;
  }
  return false;
}
