{
  int oldCount=count();
  if (black > 0) {
    black--;
  }
 else   if (blue > 0) {
    blue--;
  }
 else   if (green > 0) {
    green--;
  }
 else   if (red > 0) {
    red--;
  }
 else   if (white > 0) {
    white--;
  }
 else   if (colorless > 0) {
    colorless--;
  }
  if (stock == oldCount && oldCount > count()) {
    stock--;
  }
}
