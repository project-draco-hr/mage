{
  super(ownerId,94,"Dread Warlock",Rarity.COMMON,new CardType[]{CardType.CREATURE},"{1}{B}{B}");
  this.expansionSetCode="M10";
  this.subtype.add("Human");
  this.subtype.add("Wizard");
