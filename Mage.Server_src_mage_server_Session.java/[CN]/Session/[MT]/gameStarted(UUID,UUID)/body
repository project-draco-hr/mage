{
  executor.submit(new Runnable(){
    @Override public void run(){
      try {
        client.gameStarted(gameId,playerId);
        logger.info("game started for player " + playerId);
      }
 catch (      RemoteException ex) {
        logger.log(Level.WARNING,ex.getMessage());
        kill();
      }
    }
  }
);
}
