{
  super(ownerId,48,"Saruli Gatekeepers",Rarity.COMMON,new CardType[]{CardType.CREATURE},"{3}{G}");
  this.expansionSetCode="DGM";
  this.subtype.add("Elf");
  this.subtype.add("Warrior");
  this.color.setGreen(true);
  this.power=new MageInt(2);
  this.toughness=new MageInt(4);
  this.addAbility(new ConditionalTriggeredAbility(new EntersBattlefieldTriggeredAbility(new GainLifeEffect(7)),new ControlsPermanentCondition(filter,ControlsPermanentCondition.CountType.MORE_THAN,1),"When {this} enters the battlefield, if you control two or more Gates, gain 7 life."));
}
