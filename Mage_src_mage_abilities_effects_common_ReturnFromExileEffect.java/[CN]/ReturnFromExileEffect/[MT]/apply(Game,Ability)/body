{
  ExileZone exile=game.getExile().getExileZone(exileId);
  if (exile != null) {
    exile=exile.copy();
    for (    UUID cardId : exile) {
      Card card=game.getCard(cardId);
      card.moveToZone(zone,source.getId(),game,tapped);
    }
    game.getExile().getExileZone(exileId).clear();
    return true;
  }
  return false;
}
