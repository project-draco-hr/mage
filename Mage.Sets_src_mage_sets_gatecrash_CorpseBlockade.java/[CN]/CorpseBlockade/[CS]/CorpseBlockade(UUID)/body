{
  super(ownerId,60,"Corpse Blockade",Rarity.COMMON,new CardType[]{CardType.CREATURE},"{2}{B}");
  this.expansionSetCode="GTC";
  this.subtype.add("Zombie");
  this.color.setBlack(true);
  this.power=new MageInt(1);
  this.toughness=new MageInt(4);
  this.addAbility(DefenderAbility.getInstance());
  this.addAbility(new SimpleActivatedAbility(Zone.BATTLEFIELD,new GainAbilitySourceEffect(DeathtouchAbility.getInstance(),Constants.Duration.EndOfTurn),new SacrificeTargetCost(new TargetControlledCreaturePermanent(1,1,filter,true))));
}
