{
  Player controller=game.getPlayer(source.getControllerId());
  if (controller != null) {
    Card card=game.getCard(((FixedTarget)getTargetPointer()).getTarget());
    if (card != null) {
      controller.moveCards(card,Zone.STACK,Zone.EXILED,source,game);
      return true;
    }
  }
  return false;
}
