{
  if (event.getType() == EventType.DECLARE_ATTACKER) {
    Permanent permanent=game.getPermanent(event.getSourceId());
    if (permanent != null) {
      if (permanent.getControllerId().equals(source.getControllerId())) {
        if (!permanent.hasSubtype("Eye")) {
          return true;
        }
      }
    }
  }
  return false;
}
