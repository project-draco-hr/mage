{
  for (  UUID playerId : game.getPlayerList()) {
    Player player=game.getPlayer(playerId);
    if (player != null) {
      for (      UUID cid : player.getGraveyard().copy()) {
        Card c=game.getCard(cid);
        if (c != null) {
          c.moveToExile(null,null,source.getId(),game);
        }
      }
    }
  }
  return true;
}
