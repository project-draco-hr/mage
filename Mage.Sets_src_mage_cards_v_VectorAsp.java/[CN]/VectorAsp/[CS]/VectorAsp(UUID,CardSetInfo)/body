  this.toughness=new MageInt(1);
  this.addAbility(new SimpleActivatedAbility(Zone.BATTLEFIELD,new GainAbilitySourceEffect(InfectAbility.getInstance(),Duration.EndOfTurn),new ManaCostsImpl("{B}")));
