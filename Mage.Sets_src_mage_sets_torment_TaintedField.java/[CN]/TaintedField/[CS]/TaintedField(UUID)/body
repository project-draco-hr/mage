{
  super(ownerId,140,"Tainted Field",Rarity.UNCOMMON,new CardType[]{CardType.LAND},"");
  this.expansionSetCode="TOR";
  this.addAbility(new ColorlessManaAbility());
  this.addAbility(new ActivateIfConditionManaAbility(Constants.Zone.BATTLEFIELD,new BasicManaEffect(Mana.WhiteMana),new TapSourceCost(),new ControlsPermanentCondition(filter,ControlsPermanentCondition.CountType.MORE_THAN,0)));
  this.addAbility(new ActivateIfConditionManaAbility(Constants.Zone.BATTLEFIELD,new BasicManaEffect(Mana.BlackMana),new TapSourceCost(),new ControlsPermanentCondition(filter,ControlsPermanentCondition.CountType.MORE_THAN,0)));
}
