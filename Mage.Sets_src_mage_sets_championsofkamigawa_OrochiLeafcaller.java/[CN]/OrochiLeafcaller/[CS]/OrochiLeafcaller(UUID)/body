{
  super(ownerId,234,"Orochi Leafcaller",Rarity.COMMON,new CardType[]{CardType.CREATURE},"{G}");
  this.expansionSetCode="CHK";
  this.subtype.add("Snake");
  this.subtype.add("Shaman");
