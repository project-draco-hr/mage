  this.toughness=new MageInt(1);
  this.addAbility(new SimpleActivatedAbility(Constants.Zone.BATTLEFIELD,new AddManaOfAnyColorEffect(),new ColoredManaCost(Constants.ColoredManaSymbol.G)));
}
