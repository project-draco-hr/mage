{
  for (  MatchPlayer player : this.players) {
    if (!player.hasQuitted()) {
      player.setSideboarding();
      player.getPlayer().sideboard(this,player.getDeck());
    }
  }
synchronized (this) {
    while (!isDoneSideboarding()) {
      try {
        this.wait();
      }
 catch (      InterruptedException ex) {
      }
    }
  }
}
