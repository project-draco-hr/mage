{
  Game game=getGame();
  for (  MatchPlayer player : this.players) {
    Player p=game.getPlayer(player.getPlayer().getId());
    if (p != null) {
      if (game.getPriorityTime() > 0) {
        player.setPriorityTimeLeft(p.getPriorityTimeLeft());
      }
      if (p.hasQuit()) {
        player.setQuit(true);
      }
      if (p.hasWon()) {
        player.addWin();
      }
      if (p.hasLost()) {
        player.addLose();
      }
    }
  }
  game.fireGameEndInfo();
}
