{
  Game game=getGame();
  for (  MatchPlayer player : this.players) {
    Player p=game.getPlayer(player.getPlayer().getId());
    if (p != null) {
      if (p.hasQuitted()) {
        player.setQuitted(true);
      }
      if (p.hasWon()) {
        player.addWin();
      }
      if (p.hasLost()) {
        player.addLose();
      }
    }
  }
}
