{
  super("Beast","3/3 black Beast creature token with deathtouch");
  setOriginalExpansionSetCode("M15");
  setTokenType(1);
  cardType.add(CardType.CREATURE);
  color.setBlack(true);
  subtype.add("Beast");
  power=new MageInt(3);
  toughness=new MageInt(3);
  abilities.add(DeathtouchAbility.getInstance());
}
