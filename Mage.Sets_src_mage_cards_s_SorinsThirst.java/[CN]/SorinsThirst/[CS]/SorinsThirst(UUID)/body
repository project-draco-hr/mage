{
  super(ownerId,110,"Sorin's Thirst",Rarity.COMMON,new CardType[]{CardType.INSTANT},"{B}{B}");
  this.expansionSetCode="M12";
  this.getSpellAbility().addEffect(new DamageTargetEffect(2));
