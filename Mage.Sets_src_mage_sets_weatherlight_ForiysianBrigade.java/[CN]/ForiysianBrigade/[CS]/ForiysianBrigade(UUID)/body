{
  super(ownerId,130,"Foriysian Brigade",Rarity.UNCOMMON,new CardType[]{CardType.CREATURE},"{3}{W}");
  this.expansionSetCode="WTH";
  this.subtype.add("Human");
  this.subtype.add("Soldier");
