{
  super(ownerId,setInfo,new CardType[]{CardType.CREATURE},"{3}{R}{G}");
  this.subtype.add("Beast");
  this.power=new MageInt(7);
  this.toughness=new MageInt(5);
  this.addAbility(new TapForManaAllTriggeredManaAbility(new AddManaOfAnyTypeProducedEffect(),new FilterLandPermanent("a player taps a land"),SetTargetPointer.PERMANENT));
}
