{
  Permanent permanent=game.getPermanent(source.getFirstTarget());
  if (permanent != null) {
    int count=game.getBattlefield().count(filter,source.getControllerId(),game);
    if (count > 0) {
      permanent.addCounters(CounterType.P1P1.createInstance(count),game);
      return true;
    }
  }
  return false;
}
