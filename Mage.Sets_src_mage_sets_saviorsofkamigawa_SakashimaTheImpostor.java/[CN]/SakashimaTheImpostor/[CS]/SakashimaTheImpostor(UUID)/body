{
  super(ownerId,53,"Sakashima the Impostor",Rarity.RARE,new CardType[]{CardType.CREATURE},"{2}{U}{U}");
  this.expansionSetCode="SOK";
  this.supertype.add("Legendary");
  this.subtype.add("Human");
  this.subtype.add("Rogue");
  this.power=new MageInt(3);
  this.toughness=new MageInt(1);
  Ability ability=new SimpleStaticAbility(Zone.BATTLEFIELD,new EntersBattlefieldEffect(new SakashimaTheImpostorCopyEffect(),abilityText,true));
  this.addAbility(ability);
}
