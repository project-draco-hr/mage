{
  StringBuilder sb=new StringBuilder();
  sb.append("Multikicker");
  if (manaCosts.size() > 0) {
    sb.append(manaCosts.getText());
    if (costs.size() > 0)     sb.append(",");
  }
  if (costs.size() > 0)   sb.append(costs.getText());
  sb.append(":").append(modes.getText());
  if (replaces)   sb.append(" instead");
  return sb.toString();
}
