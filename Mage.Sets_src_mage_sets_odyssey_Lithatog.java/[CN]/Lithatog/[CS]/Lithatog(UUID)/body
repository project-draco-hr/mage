{
  super(ownerId,289,"Lithatog",Rarity.UNCOMMON,new CardType[]{CardType.CREATURE},"{1}{R}{G}");
  this.expansionSetCode="ODY";
  this.subtype.add("Atog");
  this.color.setRed(true);
  this.color.setGreen(true);
  this.power=new MageInt(1);
  this.toughness=new MageInt(2);
  this.addAbility(new SimpleActivatedAbility(Zone.BATTLEFIELD,new BoostSourceEffect(1,1,Duration.EndOfTurn),new SacrificeTargetCost(new TargetControlledPermanent(new FilterControlledArtifactPermanent("an artifact")))));
  this.addAbility(new SimpleActivatedAbility(Zone.BATTLEFIELD,new BoostSourceEffect(1,1,Duration.EndOfTurn),new SacrificeTargetCost(new TargetControlledPermanent(new FilterControlledLandPermanent("a land")))));
}
