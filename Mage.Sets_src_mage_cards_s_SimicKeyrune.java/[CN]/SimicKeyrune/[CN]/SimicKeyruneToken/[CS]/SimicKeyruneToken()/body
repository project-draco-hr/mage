{
  super("Crab","2/3 green and blue Crab artifact creature with hexproof");
  cardType.add(CardType.ARTIFACT);
  cardType.add(CardType.CREATURE);
  color.setGreen(true);
  color.setBlue(true);
  subtype.add("Crab");
  power=new MageInt(2);
  toughness=new MageInt(3);
  this.addAbility(HexproofAbility.getInstance());
}
