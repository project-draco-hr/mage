{
  super(ownerId,101,"Marsh Casualties",Rarity.UNCOMMON,new CardType[]{CardType.SORCERY},"{B}{B}");
  this.expansionSetCode="ZEN";
  this.color.setBlack(true);
  this.getSpellAbility().addOptionalCost(new KickerManaCost("{3}"));
  this.getSpellAbility().addEffect(new ConditionalContinousEffect(new MarshCasualtiesEffect(-2,-2),new MarshCasualtiesEffect(-1,-1),KickedCondition.getInstance(),ruleText));
  this.getSpellAbility().addTarget(new TargetPlayer());
}
