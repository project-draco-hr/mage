{
  super(ownerId,128,"Tainted Isle",Rarity.UNCOMMON,new CardType[]{CardType.LAND},"");
  this.expansionSetCode="PC2";
  this.addAbility(new ColorlessManaAbility());
  this.addAbility(new ActivateIfConditionManaAbility(Zone.BATTLEFIELD,new BasicManaEffect(Mana.BlueMana(1)),new TapSourceCost(),new PermanentsOnTheBattlefieldCondition(filter,PermanentsOnTheBattlefieldCondition.CountType.MORE_THAN,0)));
  this.addAbility(new ActivateIfConditionManaAbility(Zone.BATTLEFIELD,new BasicManaEffect(Mana.BlackMana(1)),new TapSourceCost(),new PermanentsOnTheBattlefieldCondition(filter,PermanentsOnTheBattlefieldCondition.CountType.MORE_THAN,0)));
}
