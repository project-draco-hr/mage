{
  super(ownerId,128,"Tainted Isle",Rarity.UNCOMMON,new CardType[]{CardType.LAND},"");
  this.expansionSetCode="PC2";
  this.addAbility(new ColorlessManaAbility());
  this.addAbility(new ActivateIfConditionManaAbility(Zone.BATTLEFIELD,new BasicManaEffect(Mana.BlueMana),new TapSourceCost(),new ControlsPermanentCondition(filter,ControlsPermanentCondition.CountType.MORE_THAN,0)));
  this.addAbility(new ActivateIfConditionManaAbility(Zone.BATTLEFIELD,new BasicManaEffect(Mana.BlackMana),new TapSourceCost(),new ControlsPermanentCondition(filter,ControlsPermanentCondition.CountType.MORE_THAN,0)));
}
