{
  boolean result=false;
  if (card.moveToZone(Zone.LIBRARY,sourceId,game,toTop)) {
    game.informPlayers(new StringBuilder(this.getName()).append(" puts ").append(card.getName()).append(" ").append(fromZone != null ? new StringBuilder("from ").append(fromZone.toString().toLowerCase(Locale.ENGLISH)).append(" ") : "").append("to the ").append(toTop ? "top" : "bottom").append(" of his or her library").toString());
    result=true;
  }
  return result;
}
