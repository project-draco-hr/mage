{
  boolean result=false;
  if (card.moveToZone(Zone.GRAVEYARD,sourceId,game,fromZone != null ? fromZone.equals(Zone.BATTLEFIELD) : false)) {
    game.informPlayers(new StringBuilder(this.getName()).append(" puts ").append(card.getName()).append(" ").append(fromZone != null ? new StringBuilder("from ").append(fromZone.toString().toLowerCase(Locale.ENGLISH)).append(" ") : "").append("into his or her graveyard").toString());
    result=true;
  }
  return result;
}
