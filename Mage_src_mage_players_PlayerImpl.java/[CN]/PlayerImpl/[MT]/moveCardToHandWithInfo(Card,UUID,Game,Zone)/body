{
  boolean result=false;
  if (card.moveToZone(Zone.HAND,sourceId,game,false)) {
    game.informPlayers(new StringBuilder(this.getName()).append(" puts ").append(card.getName()).append(" ").append(fromZone != null ? new StringBuilder("from ").append(fromZone.toString().toLowerCase(Locale.ENGLISH)).append(" ") : "").append("into his or her hand").toString());
  }
  return result;
}
