{
  boolean result=false;
  if (card.putOntoBattlefield(game,fromZone,sourceId,this.getId(),tapped,facedown)) {
    if (!game.isSimulation())     game.informPlayers(new StringBuilder(this.getLogName()).append(" puts ").append(facedown ? "a card face down " : card.getLogName()).append(" from ").append(fromZone.toString().toLowerCase(Locale.ENGLISH)).append(" ").append("onto the Battlefield").toString());
    result=true;
  }
  return result;
}
