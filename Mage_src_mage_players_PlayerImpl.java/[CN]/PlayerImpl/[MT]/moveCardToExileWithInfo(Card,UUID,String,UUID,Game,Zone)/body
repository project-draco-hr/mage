{
  boolean result=false;
  if (card.moveToExile(exileId,exileName,sourceId,game)) {
    game.informPlayers(new StringBuilder(this.getName()).append(" moves ").append(card.getLogName()).append(" ").append(fromZone != null ? new StringBuilder("from ").append(fromZone.toString().toLowerCase(Locale.ENGLISH)).append(" ") : "").append("to exile").toString());
    result=true;
  }
  return result;
}
