{
  boolean result=false;
  if (card.putOntoBattlefield(game,fromZone,sourceId,this.getId())) {
    game.informPlayers(new StringBuilder(this.getName()).append(" puts ").append(card.getName()).append(" from ").append(fromZone.toString().toLowerCase(Locale.ENGLISH)).append(" ").append("onto the Battlefield").toString());
  }
  return result;
}
