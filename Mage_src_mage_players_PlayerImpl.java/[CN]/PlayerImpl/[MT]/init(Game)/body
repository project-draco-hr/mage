{
  this.hand.clear();
  this.graveyard.clear();
  this.abilities.clear();
  this.library.clear();
  this.wins=false;
  this.loses=false;
  this.left=false;
  this.passed=false;
  this.passedTurn=false;
  library.addAll(deck.getCards());
  for (  Card card : deck.getCards().values()) {
    game.getState().getWatchers().addAll(card.getWatchers());
  }
}
