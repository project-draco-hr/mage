{
switch (toZone) {
case GRAVEYARD:
    return moveCardsToGraveyardWithInfo(cards,source,game,fromZone);
case HAND:
  for (  Card card : cards) {
    moveCardToHandWithInfo(card,playerId,game,fromZone);
  }
return true;
default :
throw new UnsupportedOperationException("to Zone not supported yet");
}
}
