{
  super(ownerId,177,"Turf Wound",Rarity.COMMON,new CardType[]{CardType.INSTANT},"{2}{R}");
  this.expansionSetCode="INV";
  this.getSpellAbility().addEffect(new TurfWoundEffect());
  this.getSpellAbility().addTarget(new TargetPlayer());
