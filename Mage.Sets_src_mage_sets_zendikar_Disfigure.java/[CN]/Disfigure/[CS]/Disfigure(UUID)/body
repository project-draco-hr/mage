{
  super(ownerId,87,"Disfigure",Rarity.COMMON,new CardType[]{CardType.INSTANT},"{B}");
  this.expansionSetCode="ZEN";
  this.color.setBlack(true);
  this.getSpellAbility().addEffect(new BoostTargetEffect(-2,-2,Duration.EndOfTurn));
