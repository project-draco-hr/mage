{
  super(ownerId,152,"Krenko's Enforcer",Rarity.COMMON,new CardType[]{CardType.CREATURE},"{1}{R}{R}");
  this.expansionSetCode="M15";
  this.subtype.add("Goblin");
  this.subtype.add("Warrior");
