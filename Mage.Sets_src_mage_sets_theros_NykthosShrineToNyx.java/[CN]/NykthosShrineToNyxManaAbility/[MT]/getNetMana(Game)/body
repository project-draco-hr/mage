{
  if (game == null) {
    return new Mana();
  }
  int amount=0;
  for (  String colorChoice : ChoiceColor.colorChoices) {
    Mana newMana=((NykthosDynamicManaEffect)this.getEffects().get(0)).computeMana(colorChoice,game,this);
    if (newMana.count() > amount) {
      netMana=newMana;
      amount=newMana.count();
    }
  }
  return netMana;
}
