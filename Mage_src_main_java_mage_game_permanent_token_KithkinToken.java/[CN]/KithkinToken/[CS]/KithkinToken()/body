{
  super("Kithkin Soldier","1/1 white Kithkin Soldier creature token");
  cardType.add(CardType.CREATURE);
  color.setWhite(true);
  subtype.add("Kithkin");
  subtype.add("Soldier");
  power=new MageInt(1);
  toughness=new MageInt(1);
  availableImageSetCodes.addAll(Arrays.asList("LRW","SHM","MMA"));
}
