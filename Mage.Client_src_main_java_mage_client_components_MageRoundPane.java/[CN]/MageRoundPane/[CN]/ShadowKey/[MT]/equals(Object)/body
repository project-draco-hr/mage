{
  if (this == obj) {
    return true;
  }
  if (obj == null) {
    return false;
  }
  if (getClass() != obj.getClass()) {
    return false;
  }
  final ShadowKey other=(ShadowKey)obj;
  if (this.width != other.width) {
    return false;
  }
  if (this.height != other.height) {
    return false;
  }
  return true;
}
