{
  if (seats[arg0].getPlayerId() == null) {
    if (arg1 == 0) {
      return Integer.toString(arg0 + 1);
    }
  }
 else {
switch (arg1) {
case 0:
      return Integer.toString(arg0 + 1);
case 1:
    return seats[arg0].getPlayerId().toString();
case 2:
  return seats[arg0].getPlayerName();
case 3:
return seats[arg0].getPlayerType();
}
}
return "";
}
