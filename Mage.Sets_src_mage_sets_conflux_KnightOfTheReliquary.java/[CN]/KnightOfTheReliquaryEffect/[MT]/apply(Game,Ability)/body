{
  int count=game.getPlayer(source.getControllerId()).getGraveyard().count(filter,game);
  if (count > 0) {
    Permanent target=(Permanent)game.getPermanent(source.getSourceId());
    if (target != null) {
      target.addPower(count);
      target.addToughness(count);
      return true;
    }
  }
  return false;
}
