{
  int count=game.getPlayer(this.source.getControllerId()).getGraveyard().count(filter);
  if (count > 0) {
    Permanent target=(Permanent)game.getPermanent(this.source.getSourceId());
    if (target != null) {
      target.addPower(count);
      target.addToughness(count);
      return true;
    }
  }
  return false;
}
