{
  super(ownerId,102,"Boggart Harbinger",Rarity.UNCOMMON,new CardType[]{CardType.CREATURE},"{2}{B}");
  this.expansionSetCode="LRW";
  this.subtype.add("Goblin");
  this.subtype.add("Shaman");
