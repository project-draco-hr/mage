{
  super(ownerId,setInfo,new CardType[]{CardType.ENCHANTMENT},"{2}{B}{B}");
  this.addAbility(new ConditionalTriggeredAbility(new BeginningOfUpkeepTriggeredAbility(new WinGameSourceControllerEffect(),TargetController.YOU,false),new TwentyGraveyardCreatureCondition(),"At the beginning of your upkeep, if twenty or more creature cards are in your graveyard, you win the game."));
}
