{
  super(ownerId,155,"Spinal Parasite",Rarity.UNCOMMON,new CardType[]{CardType.ARTIFACT,CardType.CREATURE},"{5}");
  this.expansionSetCode="5DN";
  this.subtype.add("Insect");
  this.power=new MageInt(-1);
  this.toughness=new MageInt(-1);
  this.addAbility(new SunburstAbility(this));
  Ability ability=new SimpleActivatedAbility(Zone.BATTLEFIELD,new RemoveCounterTargetEffect(),new RemoveCountersSourceCost(CounterType.P1P1.createInstance(2)));
  ability.addTarget(new TargetPermanent());
  this.addAbility(ability);
}
