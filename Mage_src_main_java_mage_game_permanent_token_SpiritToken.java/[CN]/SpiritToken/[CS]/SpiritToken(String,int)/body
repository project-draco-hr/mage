{
  super("Spirit","1/1 colorless Spirit creature token");
  availableImageSetCodes=tokenImageSets;
  setOriginalExpansionSetCode(setCode);
  if (tokenType > 0) {
    setTokenType(tokenType);
  }
  cardType.add(CardType.CREATURE);
  subtype.add("Spirit");
  power=new MageInt(1);
  toughness=new MageInt(1);
}
