{
  super(ownerId,2,"Aura Fracture",Rarity.COMMON,new CardType[]{CardType.ENCHANTMENT},"{2}{W}");
  this.expansionSetCode="PCY";
  Ability ability=new SimpleActivatedAbility(Zone.BATTLEFIELD,new DestroyTargetEffect(),new SacrificeTargetCost(new TargetControlledPermanent(new FilterControlledLandPermanent("land"))));
  ability.addTarget(new TargetPermanent(filter));
  this.addAbility(ability);
}
