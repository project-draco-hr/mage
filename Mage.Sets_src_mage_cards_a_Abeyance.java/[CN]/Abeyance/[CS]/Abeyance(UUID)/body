{
  super(ownerId,117,"Abeyance",Rarity.RARE,new CardType[]{CardType.INSTANT},"{1}{W}");
  this.expansionSetCode="WTH";
  this.getSpellAbility().addEffect(new AbeyanceEffect());
  this.getSpellAbility().addTarget(new TargetPlayer());
