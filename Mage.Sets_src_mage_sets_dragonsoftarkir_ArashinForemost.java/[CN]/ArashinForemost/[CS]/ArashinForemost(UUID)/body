{
  super(ownerId,3,"Arashin Foremost",Rarity.RARE,new CardType[]{CardType.CREATURE},"{1}{W}{W}");
  this.expansionSetCode="DTK";
  this.subtype.add("Human");
  this.subtype.add("Warrior");
