{
  addCard(Constants.Zone.GRAVEYARD,playerA,"Wonder");
  addCard(Constants.Zone.BATTLEFIELD,playerA,"Forest");
  addCard(Constants.Zone.BATTLEFIELD,playerA,"Mountain");
  addCard(Constants.Zone.BATTLEFIELD,playerA,"Plains");
