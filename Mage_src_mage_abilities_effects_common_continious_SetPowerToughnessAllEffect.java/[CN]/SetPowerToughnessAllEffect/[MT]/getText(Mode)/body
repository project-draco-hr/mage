{
  StringBuilder sb=new StringBuilder();
  sb.append(filter.getMessage());
  if (filter.getMessage().startsWith("Each ")) {
    sb.append(" becomes ");
  }
 else {
    sb.append(" become ");
  }
  sb.append(power).append("/").append(toughness);
  if (!duration.toString().isEmpty()) {
    sb.append(" ").append(duration.toString());
  }
  return sb.toString();
}
