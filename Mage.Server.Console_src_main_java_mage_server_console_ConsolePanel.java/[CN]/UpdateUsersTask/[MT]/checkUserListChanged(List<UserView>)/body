{
  if (previousUsers == null || usersToCheck == null) {
    return true;
  }
  if (previousUsers.size() != usersToCheck.size()) {
    return true;
  }
  for (  UserView u1 : previousUsers) {
    boolean found=false;
    for (    UserView u2 : usersToCheck) {
      if (u1.getUserName().equals(u2.getUserName())) {
        found=true;
        break;
      }
    }
    if (!found) {
      return true;
    }
  }
  return false;
}
