{
  if (event.getType() == GameEvent.EventType.ZONE_CHANGE) {
    ZoneChangeEvent zEvent=(ZoneChangeEvent)event;
    if (game.getPermanent(sourceId) == null) {
      if (game.getLastKnownInformation(sourceId,Zone.BATTLEFIELD) == null) {
        return false;
      }
    }
    if (zEvent.getFromZone() == Zone.BATTLEFIELD && zEvent.getToZone() == Zone.GRAVEYARD) {
      Permanent permanent=(Permanent)game.getLastKnownInformation(event.getTargetId(),Zone.BATTLEFIELD);
      if (permanent != null && permanent.getCounters().containsKey(CounterType.M1M1) && game.getOpponents(controllerId).contains(permanent.getControllerId())) {
        for (        Effect effect : this.getEffects()) {
          effect.setTargetPointer(new FixedTarget(event.getTargetId()));
        }
        return true;
      }
    }
  }
  return false;
}
