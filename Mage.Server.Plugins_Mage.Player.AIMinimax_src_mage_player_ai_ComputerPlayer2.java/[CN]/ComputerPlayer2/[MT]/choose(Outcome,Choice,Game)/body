{
  if (choices.isEmpty())   return super.choose(outcome,choice,game);
  if (!choice.isChosen()) {
    for (    String achoice : choices) {
      choice.setChoice(achoice);
      if (choice.isChosen()) {
        choices.clear();
        return true;
      }
    }
    return false;
  }
  return true;
}
