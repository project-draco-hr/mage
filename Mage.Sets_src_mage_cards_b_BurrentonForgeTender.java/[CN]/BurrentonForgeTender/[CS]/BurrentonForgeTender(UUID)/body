{
  super(ownerId,7,"Burrenton Forge-Tender",Rarity.UNCOMMON,new CardType[]{CardType.CREATURE},"{W}");
  this.expansionSetCode="LRW";
  this.subtype.add("Kithkin");
  this.subtype.add("Wizard");
