{
  super(ownerId,46,"Spire Monitor",Rarity.COMMON,new CardType[]{CardType.CREATURE},"{4}{U}");
  this.expansionSetCode="NPH";
  this.subtype.add("Drake");
  this.power=new MageInt(3);
  this.toughness=new MageInt(3);
