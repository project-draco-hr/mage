{
  super(ownerId,185,"Natural End",Rarity.COMMON,new CardType[]{CardType.INSTANT},"{2}{G}");
  this.expansionSetCode="AVR";
  this.color.setGreen(true);
  this.getSpellAbility().addEffect(new DestroyTargetEffect());
