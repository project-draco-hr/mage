{
  if (attacker != null && blocker != null) {
    Permanent sourcePermanent=game.getPermanent(source.getSourceId());
    if (sourcePermanent != null) {
      return attacker.getControllerId().equals(sourcePermanent.getControllerId()) && blocker.getPower().getValue() > attacker.getPower().getValue();
    }
  }
  return true;
}
