{
  if (getMousePosition(true) != null)   return;
  if (popupShowing) {
synchronized (this) {
      if (popupShowing) {
        popupShowing=false;
        data.component=this;
        data.card=this.gameCard;
        data.popupText=popupText;
        callback.mouseExited(e,data);
      }
    }
  }
}
