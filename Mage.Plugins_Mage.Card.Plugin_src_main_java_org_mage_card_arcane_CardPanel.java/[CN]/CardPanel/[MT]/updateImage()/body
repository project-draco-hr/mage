{
  if (!hasImage) {
    Util.threadPool.submit(new Runnable(){
      public void run(){
        BufferedImage srcImage=null;
        tappedAngle=gameCard.isTapped() ? CardPanel.TAPPED_ANGLE : 0;
        if (srcImage != null) {
          hasImage=true;
          setText(gameCard);
          setImage(srcImage,srcImage);
        }
      }
    }
);
  }
}
