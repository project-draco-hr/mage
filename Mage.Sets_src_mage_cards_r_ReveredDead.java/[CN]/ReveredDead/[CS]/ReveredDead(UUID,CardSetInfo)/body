{
  super(ownerId,setInfo,new CardType[]{CardType.CREATURE},"{1}{W}");
  this.subtype.add("Spirit");
  this.subtype.add("Soldier");
  this.power=new MageInt(1);
  this.toughness=new MageInt(1);
