{
  super(ownerId,179,"River of Tears",Rarity.RARE,new CardType[]{CardType.LAND},"");
  this.expansionSetCode="FUT";
  this.addAbility(new SimpleManaAbility(Zone.BATTLEFIELD,new ConditionalManaEffect(new BasicManaEffect(Mana.BlackMana),new BasicManaEffect(Mana.BlueMana),LandfallCondition.getInstance(),"Add {U} to your mana pool. If you played a land this turn, add {B} to your mana pool instead"),new TapSourceCost()));
  this.addWatcher(new LandfallWatcher());
}
