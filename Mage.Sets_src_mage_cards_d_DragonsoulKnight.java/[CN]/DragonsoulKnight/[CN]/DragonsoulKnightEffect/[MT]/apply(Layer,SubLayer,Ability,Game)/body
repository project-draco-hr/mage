{
  Permanent permanent=game.getPermanent(source.getSourceId());
  if (permanent == null) {
    return false;
  }
switch (layer) {
case TypeChangingEffects_4:
    if (sublayer == SubLayer.NA) {
      permanent.getSubtype(game).clear();
      permanent.getSubtype(game).add("Dragon");
    }
  break;
}
return true;
}
