{
  if (!super.resolve(game)) {
    Card card=game.getCard(getSourceId());
    if (card != null) {
      Player owner=game.getPlayer(card.getOwnerId());
      if (owner != null) {
        owner.moveCards(card,Zone.EXILED,Zone.GRAVEYARD,this,game);
      }
    }
    return false;
  }
  return true;
}
