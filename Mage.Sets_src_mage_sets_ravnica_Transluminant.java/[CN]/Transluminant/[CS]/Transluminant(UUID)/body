{
  super(ownerId,186,"Transluminant",Rarity.COMMON,new CardType[]{CardType.CREATURE},"{1}{G}");
  this.expansionSetCode="RAV";
  this.subtype.add("Dryad");
  this.subtype.add("Shaman");
  this.power=new MageInt(2);
  this.toughness=new MageInt(2);
  Ability ability=new SimpleActivatedAbility(Zone.BATTLEFIELD,new CreateDelayedTriggeredAbilityEffect(new AtTheBeginOfNextEndStepDelayedTriggeredAbility(new CreateTokenEffect(new SpiritWhiteToken()))),new ManaCostsImpl("{W}"));
  ability.addCost(new SacrificeSourceCost());
  this.addAbility(ability);
}
