{
  if (filter == null) {
    return;
  }
  if (filter.isBlack()) {
    black=0;
  }
  if (filter.isBlue()) {
    blue=0;
  }
  if (filter.isWhite()) {
    white=0;
  }
  if (filter.isGreen()) {
    green=0;
  }
  if (filter.isRed()) {
    red=0;
  }
  if (filter.isColorless()) {
    colorless=0;
  }
}
