{
  super(ownerId,52,"Salvage Slasher",Rarity.COMMON,new CardType[]{CardType.ARTIFACT,CardType.CREATURE},"{1}{B}");
  this.expansionSetCode="CON";
  this.subtype.add("Human");
  this.subtype.add("Rogue");
  this.power=new MageInt(1);
  this.toughness=new MageInt(1);
  BoostSourceEffect effect=new BoostSourceEffect(new CardsInControllerGraveyardCount(new FilterArtifactCard()),new StaticValue(0),Duration.WhileOnBattlefield);
  this.addAbility(new SimpleStaticAbility(Zone.BATTLEFIELD,effect));
}
