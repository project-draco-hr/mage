{
  super(ownerId,8,"Krovikan Mist",Rarity.COMMON,new CardType[]{CardType.CREATURE},"{1}{U}");
  this.expansionSetCode="DDM";
  this.subtype.add("Illusion");
  this.color.setBlue(true);
  this.power=new MageInt(0);
  this.toughness=new MageInt(0);
  this.addAbility(FlyingAbility.getInstance());
  this.addAbility(new SimpleStaticAbility(Zone.ALL,new SetPowerToughnessSourceEffect(new PermanentsOnBattlefieldCount(controlledIllusionsFilter),Duration.EndOfGame)));
}
