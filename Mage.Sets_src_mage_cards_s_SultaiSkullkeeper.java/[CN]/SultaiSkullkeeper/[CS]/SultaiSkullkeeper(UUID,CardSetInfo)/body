{
  super(ownerId,setInfo,new CardType[]{CardType.CREATURE},"{1}{U}");
  this.subtype.add("Naga");
  this.subtype.add("Shaman");
  this.power=new MageInt(2);
  this.toughness=new MageInt(1);
