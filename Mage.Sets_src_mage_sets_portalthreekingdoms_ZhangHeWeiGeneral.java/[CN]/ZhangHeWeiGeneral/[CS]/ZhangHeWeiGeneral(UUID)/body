{
  super(ownerId,95,"Zhang He, Wei General",Rarity.RARE,new CardType[]{CardType.CREATURE},"{3}{B}{B}");
  this.expansionSetCode="PTK";
  this.supertype.add("Legendary");
  this.subtype.add("Human");
  this.subtype.add("Soldier");
  this.power=new MageInt(4);
  this.toughness=new MageInt(2);
  this.addAbility(HorsemanshipAbility.getInstance());
  this.addAbility(new AttacksTriggeredAbility(new BoostControlledEffect(1,0,Duration.EndOfTurn,new FilterCreaturePermanent(),true),false));
}
