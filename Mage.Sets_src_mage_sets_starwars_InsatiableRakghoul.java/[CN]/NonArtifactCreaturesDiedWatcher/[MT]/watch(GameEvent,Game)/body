{
  if (event.getType() == GameEvent.EventType.ZONE_CHANGE) {
    ZoneChangeEvent zEvent=(ZoneChangeEvent)event;
    if (zEvent.isDiesEvent() && zEvent.getTarget() != null && zEvent.getTarget().getCardType().contains(CardType.CREATURE) && !zEvent.getTarget().getCardType().contains(CardType.ARTIFACT)) {
      condition=true;
    }
  }
}
