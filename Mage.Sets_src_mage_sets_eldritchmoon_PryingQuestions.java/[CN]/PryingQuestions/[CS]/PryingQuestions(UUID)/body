{
  super(ownerId,101,"Prying Questions",Rarity.UNCOMMON,new CardType[]{CardType.SORCERY},"{2}{B}");
  this.expansionSetCode="EMN";
  this.getSpellAbility().addTarget(new TargetOpponent());
