{
  if (infoMessage == null) {
    String message;
    MageObject object=game.getObject(source.getSourceId());
    if (object != null) {
      message=source.getRule(object.getLogName());
    }
 else {
      message=source.getRule();
    }
    return message;
  }
 else {
    return infoMessage;
  }
}
