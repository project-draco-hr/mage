{
  Permanent permanent=game.getPermanent(source.getSourceId());
  if (permanent != null) {
    if (withoutTrigger) {
      if (permanent.canTransform()) {
        permanent.setTransformed(!permanent.isTransformed());
      }
    }
 else {
      permanent.transform(game);
    }
    return true;
  }
  return false;
}
