{
  if (this.getState().equals(TournamentPlayerState.DRAFTING) || this.getState().equals(TournamentPlayerState.CONSTRUCTING) || this.getState().equals(TournamentPlayerState.DUELING)|| this.getState().equals(TournamentPlayerState.SIDEBOARDING)|| this.getState().equals(TournamentPlayerState.WAITING)) {
    this.setState(TournamentPlayerState.FINISHED);
  }
}
