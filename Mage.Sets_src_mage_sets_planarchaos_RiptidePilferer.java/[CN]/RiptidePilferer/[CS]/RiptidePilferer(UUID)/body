{
  super(ownerId,60,"Riptide Pilferer",Rarity.UNCOMMON,new CardType[]{CardType.CREATURE},"{1}{U}");
  this.expansionSetCode="PLC";
  this.subtype.add("Merfolk");
  this.subtype.add("Rogue");
