  this.toughness=new MageInt(1);
  this.addAbility(new DealsCombatDamageToAPlayerTriggeredAbility(new DiscardTargetEffect(1),false,true));
  this.addAbility(new MorphAbility(this,new ManaCostsImpl("{U}")));
}
