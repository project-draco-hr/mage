{
  this.controllerId=controllerId;
  if (watchers != null) {
    for (    Watcher watcher : watchers) {
      watcher.setControllerId(controllerId);
    }
  }
  if (subAbilities != null) {
    for (    Ability subAbility : subAbilities) {
      subAbility.setControllerId(controllerId);
    }
  }
}
