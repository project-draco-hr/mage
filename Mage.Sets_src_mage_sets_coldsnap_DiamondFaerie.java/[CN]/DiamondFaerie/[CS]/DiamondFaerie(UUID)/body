{
  super(ownerId,128,"Diamond Faerie",Rarity.RARE,new CardType[]{CardType.CREATURE},"{2}{G}{W}{U}");
  this.expansionSetCode="CSP";
  this.supertype.add("Snow");
  this.subtype.add("Faerie");
  this.power=new MageInt(3);
  this.toughness=new MageInt(3);
  this.addAbility(FlyingAbility.getInstance());
  this.addAbility(new SimpleActivatedAbility(Zone.BATTLEFIELD,new BoostControlledEffect(1,1,Duration.EndOfTurn,filter),new ManaCostsImpl("{1}{S}")));
}
