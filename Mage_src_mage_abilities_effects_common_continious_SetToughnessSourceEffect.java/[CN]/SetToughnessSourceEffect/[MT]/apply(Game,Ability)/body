{
  MageObject mageObject=game.getObject(source.getSourceId());
  if (mageObject != null) {
    int value=amount.calculate(game,source,this);
    mageObject.getToughness().setValue(value);
    return true;
  }
 else {
    if (Duration.Custom.equals(duration)) {
      discard();
    }
  }
  return false;
}
