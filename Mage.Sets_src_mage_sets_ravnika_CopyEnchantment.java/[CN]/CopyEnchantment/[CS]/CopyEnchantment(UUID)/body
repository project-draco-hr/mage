{
  super(ownerId,42,"Copy Enchantment",Rarity.RARE,new CardType[]{CardType.ENCHANTMENT},"{2}{U}");
  this.expansionSetCode="RAV";
  this.color.setBlue(true);
  Ability ability=new SimpleStaticAbility(Zone.BATTLEFIELD,new EntersBattlefieldEffect(new CopyEnchantmentEffect(new FilterEnchantmentPermanent()),"You may have {this} enter the battlefield as a copy of any enchantment on the battlefield",true));
  this.addAbility(ability);
}
