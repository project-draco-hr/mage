{
  if (dynamicValue.calculate(game,source) >= lessThan) {
    return false;
  }
  Permanent permanent=game.getPermanent(source.getSourceId());
  if (permanent != null) {
switch (layer) {
case TypeChangingEffects_4:
      if (sublayer == SubLayer.NA) {
        permanent.getCardType().remove(CardType.CREATURE);
      }
    break;
}
return true;
}
return false;
}
