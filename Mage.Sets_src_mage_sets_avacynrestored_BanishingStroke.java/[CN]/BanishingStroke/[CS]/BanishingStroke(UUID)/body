{
  super(ownerId,7,"Banishing Stroke",Rarity.UNCOMMON,new CardType[]{CardType.INSTANT},"{5}{W}");
  this.expansionSetCode="AVR";
  this.color.setWhite(true);
  this.getSpellAbility().addEffect(new PutOnLibraryTargetEffect(false));
