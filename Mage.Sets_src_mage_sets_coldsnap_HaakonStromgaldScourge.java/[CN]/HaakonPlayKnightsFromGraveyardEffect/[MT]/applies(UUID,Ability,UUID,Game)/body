{
  if (affectedControllerId.equals(source.getControllerId())) {
    Card knightToCast=game.getCard(objectId);
    if (knightToCast != null && knightToCast.hasSubtype("Knight") && knightToCast.getOwnerId().equals(source.getControllerId()) && game.getState().getZone(objectId) == Zone.GRAVEYARD) {
      return true;
    }
  }
  return false;
}
