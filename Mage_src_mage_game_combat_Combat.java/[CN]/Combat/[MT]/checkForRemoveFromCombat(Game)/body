{
  for (  UUID creatureId : getAttackers()) {
    Permanent creature=game.getPermanent(creatureId);
    if (!creature.getCardType().contains(CardType.CREATURE)) {
      removeFromCombat(creatureId,game,true);
    }
  }
  for (  UUID creatureId : getBlockers()) {
    Permanent creature=game.getPermanent(creatureId);
    if (!creature.getCardType().contains(CardType.CREATURE)) {
      removeFromCombat(creatureId,game,true);
    }
  }
}
