{
  super(ownerId,128,"Harbinger of Spring",Rarity.COMMON,new CardType[]{CardType.CREATURE},"{4}{G}");
  this.expansionSetCode="BOK";
  this.subtype.add("Spirit");
  this.power=new MageInt(2);
  this.toughness=new MageInt(1);
