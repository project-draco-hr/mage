{
  super(ownerId,267,"Seton, Krosan Protector",Rarity.RARE,new CardType[]{CardType.CREATURE},"{G}{G}{G}");
  this.expansionSetCode="ODY";
  this.supertype.add("Legendary");
  this.subtype.add("Centaur");
  this.subtype.add("Druid");
  this.color.setGreen(true);
  this.power=new MageInt(2);
  this.toughness=new MageInt(2);
  this.addAbility(new SimpleActivatedAbility(Zone.BATTLEFIELD,new BasicManaEffect(Mana.GreenMana),new TapTargetCost(new TargetControlledCreaturePermanent(1,1,filter,true))));
}
