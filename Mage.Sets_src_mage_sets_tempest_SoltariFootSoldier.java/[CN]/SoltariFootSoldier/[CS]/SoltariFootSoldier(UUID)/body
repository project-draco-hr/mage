{
  super(ownerId,255,"Soltari Foot Soldier",Rarity.COMMON,new CardType[]{CardType.CREATURE},"{W}");
  this.expansionSetCode="TMP";
  this.subtype.add("Soltari");
  this.subtype.add("Soldier");
