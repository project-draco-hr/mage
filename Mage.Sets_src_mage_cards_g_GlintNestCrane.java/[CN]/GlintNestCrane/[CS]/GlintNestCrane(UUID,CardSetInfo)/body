{
  super(ownerId,setInfo,new CardType[]{CardType.CREATURE},"{1}{U}");
  this.subtype.add("Bird");
  this.power=new MageInt(1);
  this.toughness=new MageInt(3);
  this.addAbility(FlyingAbility.getInstance());
  this.addAbility(new EntersBattlefieldTriggeredAbility(new LookLibraryAndPickControllerEffect(new StaticValue(4),false,new StaticValue(1),filter,Zone.LIBRARY,false,true,false,Zone.HAND,true)));
}
