{
  super(AbilityType.ACTIVATED,zone);
  if (effect != null) {
    this.addEffect(effect);
  }
  if (cost != null) {
    if (cost instanceof PhyrexianManaCost) {
      this.addManaCost((PhyrexianManaCost)cost);
    }
 else     if (cost instanceof ManaCost) {
      this.addManaCost((ManaCost)cost);
    }
 else {
      this.addCost(cost);
    }
  }
}
