{
  while (!isAbort() && boosterNum < numberBoosters) {
    openBooster();
    while (!isAbort() && pickCards()) {
      if (boosterNum % 2 == 1) {
        passLeft();
      }
 else {
        passRight();
      }
      fireUpdatePlayersEvent();
    }
  }
  resetBufferedCards();
  this.fireEndDraftEvent();
}
