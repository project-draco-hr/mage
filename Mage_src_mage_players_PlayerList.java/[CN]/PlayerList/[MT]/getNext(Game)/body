{
  Player player;
  UUID start=this.get();
  while (true) {
    player=game.getPlayer(super.getNext());
    if (!player.hasLeft() && !player.hasLost()) {
      if (!player.hasReachedNextTurnAfterLeaving()) {
        player.setReachedNextTurnAfterLeaving(true);
      }
      break;
    }
    if (player.getId().equals(start)) {
      return null;
    }
  }
  return player;
}
