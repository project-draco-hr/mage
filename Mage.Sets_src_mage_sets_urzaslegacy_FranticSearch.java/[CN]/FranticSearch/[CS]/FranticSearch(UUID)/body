{
  super(ownerId,32,"Frantic Search",Rarity.COMMON,new CardType[]{CardType.INSTANT},"{2}{U}");
  this.expansionSetCode="ULG";
  this.color.setBlue(true);
  this.getSpellAbility().addEffect(new DrawCardControllerEffect(2));
