{
  super(ownerId,28,"Nemesis of Reason",Rarity.RARE,new CardType[]{CardType.CREATURE},"{3}{U}{B}");
  this.expansionSetCode="ARB";
  this.subtype.add("Leviathan");
  this.subtype.add("Horror");
