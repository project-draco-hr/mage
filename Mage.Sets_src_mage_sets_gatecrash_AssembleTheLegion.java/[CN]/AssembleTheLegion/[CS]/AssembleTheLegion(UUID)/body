{
  super(ownerId,142,"Assemble the Legion",Rarity.RARE,new CardType[]{CardType.ENCHANTMENT},"{3}{R}{W}");
  this.expansionSetCode="GTC";
  this.color.setRed(true);
  this.color.setWhite(true);
