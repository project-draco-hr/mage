{
  super(ownerId,setInfo,new CardType[]{CardType.SORCERY},"{1}{G}");
  this.addAbility(new AlternativeCostSourceAbility(new LandGrantReavealCost(),new LandGrantCondition(),"If you have no land cards in hand, you may reveal your hand rather than pay {this}'s mana cost."));
  this.getSpellAbility().addEffect(new SearchLibraryPutInHandEffect(new TargetCardInLibrary(filter),true,true));
}
