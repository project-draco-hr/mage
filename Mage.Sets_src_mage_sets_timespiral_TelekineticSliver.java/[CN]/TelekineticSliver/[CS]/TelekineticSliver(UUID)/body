{
  super(ownerId,84,"Telekinetic Sliver",Rarity.UNCOMMON,new CardType[]{CardType.CREATURE},"{2}{U}{U}");
  this.expansionSetCode="TSP";
  this.subtype.add("Sliver");
  this.power=new MageInt(2);
  this.toughness=new MageInt(2);
  Ability ability=new SimpleActivatedAbility(Zone.BATTLEFIELD,new TapTargetEffect(),new TapSourceCost());
  ability.addTarget(new TargetPermanent());
  this.addAbility(new SimpleStaticAbility(Zone.BATTLEFIELD,new GainAbilityAllEffect(ability,Duration.WhileOnBattlefield,filter,"All Slivers have \"{tap}: Tap target permanent.\"")));
}
