{
  try {
    if (isConnected()) {
      UserDataView userDataView=new UserDataView(avatarId);
      server.setUserData(connection.getUsername(),sessionId,userDataView);
    }
    return true;
  }
 catch (  MageException ex) {
    handleMageException(ex);
  }
catch (  Throwable t) {
    handleThrowable(t);
  }
  return false;
}
