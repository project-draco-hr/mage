{
  try {
    if (JOptionPane.showConfirmDialog(null,"Are you sure you mean to mute userSessionId " + userSessionId + "?","WARNING",JOptionPane.YES_NO_OPTION) == JOptionPane.YES_OPTION) {
      if (isConnected()) {
        server.endUserSession(sessionId,userSessionId);
        return true;
      }
    }
  }
 catch (  MageException ex) {
    handleMageException(ex);
  }
catch (  Throwable t) {
    handleThrowable(t);
  }
  return false;
}
