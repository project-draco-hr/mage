{
  try {
    if (isConnected())     return server.joinTable(sessionId,roomId,tableId,playerName,playerType,skill,deckList);
  }
 catch (  GameException ex) {
    handleGameException(ex);
  }
catch (  MageException ex) {
    handleMageException(ex);
  }
catch (  Throwable t) {
    handleThrowable(t);
  }
  return false;
}
