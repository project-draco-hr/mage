{
  try {
    if (isConnected()) {
      deck.setCardLayout(null);
      deck.setSideboardLayout(null);
      server.updateDeck(sessionId,tableId,deck);
      return true;
    }
  }
 catch (  GameException ex) {
    handleGameException(ex);
  }
catch (  MageException ex) {
    handleMageException(ex);
  }
catch (  Throwable t) {
    handleThrowable(t);
  }
  return false;
}
