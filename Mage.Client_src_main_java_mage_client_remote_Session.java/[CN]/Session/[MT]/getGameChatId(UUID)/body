{
  GetGameChatId method=new GetGameChatId(connection,gameId);
  try {
    q.callMethod(method);
    return method.getReturnVal();
  }
 catch (  ServerUnavailable ex) {
    handleServerUnavailable(ex);
  }
catch (  MageException ex) {
    logger.fatal("GetGameChatId error",ex);
  }
  return null;
}
