{
  SendPlayerString method=new SendPlayerString(connection,sessionId,gameId,data);
  try {
    method.makeCall();
    return true;
  }
 catch (  ServerUnavailable ex) {
    handleServerUnavailable(ex);
  }
catch (  MageException ex) {
    logger.fatal("SendPlayerString error",ex);
  }
  return false;
}
