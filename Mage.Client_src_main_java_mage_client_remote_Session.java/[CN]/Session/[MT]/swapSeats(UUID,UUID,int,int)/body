{
  SwapSeats method=new SwapSeats(connection,sessionId,roomId,tableId,seatNum1,seatNum2);
  try {
    method.makeCall();
    return true;
  }
 catch (  ServerUnavailable ex) {
    handleServerUnavailable(ex);
  }
catch (  MageException ex) {
    logger.fatal("RemoveTable error",ex);
  }
  return false;
}
