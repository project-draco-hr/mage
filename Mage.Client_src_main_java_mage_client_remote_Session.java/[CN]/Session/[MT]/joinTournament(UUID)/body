{
  JoinTournament method=new JoinTournament(connection,sessionId,tournamentId);
  try {
    method.makeCall();
    return true;
  }
 catch (  ServerUnavailable ex) {
    handleServerUnavailable(ex);
  }
catch (  MageException ex) {
    logger.fatal("JoinTournament error",ex);
  }
  return false;
}
