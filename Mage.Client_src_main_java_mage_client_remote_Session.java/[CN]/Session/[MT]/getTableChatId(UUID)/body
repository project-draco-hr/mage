{
  GetTableChatId method=new GetTableChatId(connection,tableId);
  try {
    q.callMethod(method);
    return method.getReturnVal();
  }
 catch (  ServerUnavailable ex) {
    handleServerUnavailable(ex);
  }
catch (  MageException ex) {
    logger.fatal("GetTableChatId error",ex);
  }
  return null;
}
