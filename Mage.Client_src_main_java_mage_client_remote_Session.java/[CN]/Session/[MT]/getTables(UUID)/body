{
  lock.readLock().lock();
  try {
    if (server == null)     return null;
    return server.getTables(roomId);
  }
 catch (  RemoteException ex) {
    handleRemoteException(ex);
    throw new MageRemoteException();
  }
catch (  MageException ex) {
    handleMageException(ex);
    throw new MageRemoteException();
  }
 finally {
    lock.readLock().unlock();
  }
}
