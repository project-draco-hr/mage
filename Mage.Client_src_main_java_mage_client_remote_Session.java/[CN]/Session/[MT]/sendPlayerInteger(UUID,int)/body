{
  SendPlayerInteger method=new SendPlayerInteger(connection,sessionId,gameId,data);
  try {
    method.makeCall();
    return true;
  }
 catch (  ServerUnavailable ex) {
    handleServerUnavailable(ex);
  }
catch (  MageException ex) {
    logger.fatal("SendPlayerInteger error",ex);
  }
  return false;
}
