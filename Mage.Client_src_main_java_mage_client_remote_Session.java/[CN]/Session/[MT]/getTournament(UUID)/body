{
  GetTournament method=new GetTournament(connection,tournamentId);
  try {
    q.callMethod(method);
    return method.getReturnVal();
  }
 catch (  ServerUnavailable ex) {
    handleServerUnavailable(ex);
  }
catch (  MageException ex) {
    logger.fatal("GetTable error",ex);
  }
  return null;
}
