{
  StartTournament method=new StartTournament(connection,sessionId,roomId,tableId);
  try {
    method.makeCall();
    return true;
  }
 catch (  ServerUnavailable ex) {
    handleServerUnavailable(ex);
  }
catch (  MageException ex) {
    logger.fatal("StartTournament error",ex);
  }
  return false;
}
