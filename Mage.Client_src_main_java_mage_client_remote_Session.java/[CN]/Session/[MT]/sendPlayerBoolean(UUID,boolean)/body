{
  SendPlayerBoolean method=new SendPlayerBoolean(connection,sessionId,gameId,data);
  try {
    method.makeCall();
    return true;
  }
 catch (  ServerUnavailable ex) {
    handleServerUnavailable(ex);
  }
catch (  MageException ex) {
    logger.fatal("SendPlayerBoolean error",ex);
  }
  return false;
}
