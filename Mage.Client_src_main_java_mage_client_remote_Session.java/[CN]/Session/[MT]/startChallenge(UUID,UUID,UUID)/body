{
  StartChallenge method=new StartChallenge(connection,sessionId,roomId,tableId,challengeId);
  try {
    method.makeCall();
    return true;
  }
 catch (  ServerUnavailable ex) {
    handleServerUnavailable(ex);
  }
catch (  MageException ex) {
    logger.fatal("StartChallenge error",ex);
  }
  return false;
}
