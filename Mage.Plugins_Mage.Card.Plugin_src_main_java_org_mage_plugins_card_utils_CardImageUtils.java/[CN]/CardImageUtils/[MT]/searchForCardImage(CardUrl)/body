{
  File file=null;
  String path="";
  CardUrl c=new CardUrl(card.name,card.set,0,card.token);
  boolean found=false;
  for (  String set : SettingsManager.getIntance().getTokenLookupOrder()) {
    if (found || card.set.isEmpty()) {
      c.set=set;
      path=getTokenImagePath(c);
      file=new File(path);
      if (file.exists()) {
        pathCache.put(card,path);
        return path;
      }
    }
  }
  return "";
}
