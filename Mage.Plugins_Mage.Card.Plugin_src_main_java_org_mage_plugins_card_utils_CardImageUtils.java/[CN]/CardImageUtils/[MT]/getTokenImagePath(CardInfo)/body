{
  String filename=getImagePath(card,false);
  File file=new File(filename);
  if (!file.exists()) {
    card.setName(card.getName() + " 1");
    filename=getImagePath(card,false);
    file=new File(filename);
    if (!file.exists()) {
      card.setName(card.getName() + " 2");
      filename=getImagePath(card,false);
      file=new File(filename);
    }
  }
  return filename;
}
