{
  String filename=getImagePath(card,false);
  CardUrl c=new CardUrl(card.name,card.set,0,card.token);
  File file=new File(filename);
  if (!file.exists()) {
    c.name=card.name + " 1";
    filename=getImagePath(c,false);
    file=new File(filename);
    if (!file.exists()) {
      c.name=card.name + " 2";
      filename=getImagePath(c,false);
      file=new File(filename);
    }
  }
  return filename;
}
