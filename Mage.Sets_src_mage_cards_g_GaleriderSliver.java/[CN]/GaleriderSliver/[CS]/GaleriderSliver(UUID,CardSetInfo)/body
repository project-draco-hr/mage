{
  super(ownerId,setInfo,new CardType[]{CardType.CREATURE},"{U}");
  this.subtype.add("Sliver");
  this.power=new MageInt(1);
  this.toughness=new MageInt(1);
  this.addAbility(new SimpleStaticAbility(Zone.BATTLEFIELD,new GainAbilityControlledEffect(FlyingAbility.getInstance(),Duration.WhileOnBattlefield,new FilterCreaturePermanent("Sliver","Sliver creatures"))));
}
