{
  super(ownerId,129,"Alloy Myr",Rarity.UNCOMMON,new CardType[]{CardType.ARTIFACT,CardType.CREATURE},"{3}");
  this.expansionSetCode="NPH";
  this.subtype.add("Myr");
  this.power=new MageInt(2);
