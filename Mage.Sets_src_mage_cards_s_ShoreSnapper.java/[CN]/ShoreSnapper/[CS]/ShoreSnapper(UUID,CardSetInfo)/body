{
  super(ownerId,setInfo,new CardType[]{CardType.CREATURE},"{2}{B}");
  this.subtype.add("Beast");
  this.power=new MageInt(2);
  this.toughness=new MageInt(2);
  this.addAbility(new SimpleActivatedAbility(Zone.BATTLEFIELD,new GainAbilitySourceEffect(new IslandwalkAbility(),Duration.EndOfTurn),new ManaCostsImpl("{U}")));
}
