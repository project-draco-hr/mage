{
  Card card=game.getCard(source.getSourceId());
  if (card != null) {
    for (    Ability ability : card.getAbilities()) {
      if (ability instanceof DashAbility) {
        if (((DashAbility)ability).isActivated(source,game)) {
          return true;
        }
      }
    }
  }
  return false;
}
