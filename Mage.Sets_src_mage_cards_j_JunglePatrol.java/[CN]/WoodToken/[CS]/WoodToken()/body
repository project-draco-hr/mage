{
  super("Wood","0/1 green Wall creature token with defender named Wood");
  this.setOriginalExpansionSetCode("MIR");
  cardType.add(CardType.CREATURE);
  color.setGreen(true);
  subtype.add("Wall");
  power=new MageInt(0);
  toughness=new MageInt(1);
  this.addAbility(DefenderAbility.getInstance());
}
