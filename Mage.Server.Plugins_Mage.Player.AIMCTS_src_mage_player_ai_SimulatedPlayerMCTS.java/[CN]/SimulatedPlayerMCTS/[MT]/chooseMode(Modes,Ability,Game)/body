{
  if (this.isHuman()) {
    Iterator<Mode> it=modes.values().iterator();
    Mode mode=it.next();
    if (modes.size() == 1)     return mode;
    int modeNum=rnd.nextInt(modes.values().size());
    for (int i=0; i < modeNum; i++) {
      mode=it.next();
    }
    return mode;
  }
  return super.chooseMode(modes,source,game);
}
