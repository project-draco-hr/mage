{
  super(ownerId,15,"Vedalken Engineer",Rarity.COMMON,new CardType[]{CardType.CREATURE},"{1}{U}");
  this.expansionSetCode="HOP";
  this.subtype.add("Vedalken");
  this.subtype.add("Artificer");
