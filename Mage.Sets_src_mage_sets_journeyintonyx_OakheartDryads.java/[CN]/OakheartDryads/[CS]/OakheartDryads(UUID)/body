{
  super(ownerId,133,"Oakheart Dryads",Rarity.COMMON,new CardType[]{CardType.ENCHANTMENT,CardType.CREATURE},"{2}{G}");
  this.expansionSetCode="JOU";
  this.subtype.add("Nymph");
  this.subtype.add("Dryad");
  this.power=new MageInt(2);
  this.toughness=new MageInt(3);
  Ability ability=new ConstellationAbility(new BoostTargetEffect(1,1,Duration.EndOfTurn),false);
  ability.addTarget(new TargetCreaturePermanent());
  this.addAbility(ability);
}
