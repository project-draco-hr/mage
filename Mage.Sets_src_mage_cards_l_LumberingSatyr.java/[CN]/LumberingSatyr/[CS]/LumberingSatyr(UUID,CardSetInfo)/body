{
  super(ownerId,setInfo,new CardType[]{CardType.CREATURE},"{2}{G}{G}");
  this.subtype.add("Satyr");
  this.subtype.add("Beast");
  this.power=new MageInt(5);
  this.toughness=new MageInt(4);
  this.addAbility(new SimpleStaticAbility(Zone.BATTLEFIELD,new GainAbilityAllEffect(new ForestwalkAbility(),Duration.WhileOnBattlefield,new FilterCreaturePermanent())));
}
