{
  int idx=0;
  if (eachModeMoreThanOnce) {
    for (    UUID modeId : this.selectedModes) {
      idx++;
      if (idx == index) {
        return modeId;
      }
    }
  }
 else {
    for (    Mode mode : this.values()) {
      idx++;
      if (idx == index) {
        return mode.getId();
      }
    }
  }
  return null;
}
