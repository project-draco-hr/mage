{
  super(ownerId,161,"Skeletal Scrying",Rarity.UNCOMMON,new CardType[]{CardType.INSTANT},"{X}{B}");
  this.expansionSetCode="ODY";
  this.color.setBlack(true);
  this.getSpellAbility().addCost(new ExileXFromGraveyardCost());
