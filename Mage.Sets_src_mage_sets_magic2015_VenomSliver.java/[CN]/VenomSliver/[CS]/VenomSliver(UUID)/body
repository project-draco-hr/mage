{
  super(ownerId,205,"Venom Sliver",Rarity.UNCOMMON,new CardType[]{CardType.CREATURE},"{1}{G}");
  this.expansionSetCode="M15";
  this.subtype.add("Sliver");
  this.power=new MageInt(1);
  this.toughness=new MageInt(1);
  this.addAbility(new SimpleStaticAbility(Zone.BATTLEFIELD,new GainAbilityControlledEffect(DeathtouchAbility.getInstance(),Duration.WhileOnBattlefield,new FilterCreaturePermanent("Sliver","Sliver creatures"))));
}
