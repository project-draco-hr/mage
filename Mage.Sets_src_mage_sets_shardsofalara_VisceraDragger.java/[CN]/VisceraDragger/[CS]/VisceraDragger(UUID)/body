  this.power=new MageInt(3);
  this.toughness=new MageInt(3);
  this.addAbility(new CyclingAbility(new ManaCostsImpl("{2}")));
  this.addAbility(new UnearthAbility(new ManaCostsImpl("{1}{B}")));
}
