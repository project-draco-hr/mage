{
  super(ownerId,92,"Viscera Dragger",Rarity.COMMON,new CardType[]{CardType.CREATURE},"{3}{B}");
  this.expansionSetCode="ALA";
  this.subtype.add("Zombie");
  this.subtype.add("Ogre");
