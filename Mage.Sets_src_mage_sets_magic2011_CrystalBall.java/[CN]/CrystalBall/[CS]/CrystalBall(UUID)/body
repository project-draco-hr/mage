{
  super(ownerId,203,"Crystal Ball",Rarity.UNCOMMON,new CardType[]{CardType.ARTIFACT},"{3}");
  this.expansionSetCode="M11";
  Ability ability=new SimpleActivatedAbility(Zone.BATTLEFIELD,new ScryEffect(2),new TapSourceCost());
