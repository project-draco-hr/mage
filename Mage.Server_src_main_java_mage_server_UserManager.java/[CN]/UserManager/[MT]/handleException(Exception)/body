{
  if (ex != null) {
    logger.fatal("User manager exception" + (ex.getMessage() == null ? "null" : ex.getMessage()));
    if (ex.getCause() != null) {
      logger.debug("- Cause: " + (ex.getCause().getMessage() == null ? "null" : ex.getCause().getMessage()));
    }
    ex.printStackTrace();
  }
 else {
    logger.fatal("User manager exception - null");
  }
}
