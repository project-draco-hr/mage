{
  User user=users.get(userId);
  if (user != null) {
    logger.debug(user.getName() + " " + reason.toString()+ "userId: "+ userId);
    user.kill(reason);
    users.remove(userId);
  }
 else {
    logger.warn(new StringBuilder("Trying to remove userId: ").append(userId).append(" but it does not exist."));
  }
}
