{
  super(ownerId,149,"See the Unwritten",Rarity.MYTHIC,new CardType[]{CardType.SORCERY},"{4}{G}{G}");
  this.expansionSetCode="KTK";
  this.color.setGreen(true);
  this.getSpellAbility().addEffect(new ConditionalOneShotEffect(new SeeTheUnwrittenEffect(1),new SeeTheUnwrittenEffect(2),new InvertCondition(FerociousCondition.getInstance()),"Reveal the top eight cards of your library. You may put a creature card from among them onto the battlefield. Put the rest into your graveyard" + "<br/><br/><i>Ferocious</i> - If you control a creature with power 4 or greater, you may put two creature cards onto the battlefield instead of one"));
}
