{
  super(ownerId,117,"Bassara Tower Archer",Rarity.UNCOMMON,new CardType[]{CardType.CREATURE},"{G}{G}");
  this.expansionSetCode="JOU";
  this.subtype.add("Human");
  this.subtype.add("Archer");
