{
  if (StackClause.WHILE_ON_STACK.equals(clause)) {
    player.addAction(turnNum,step,"activate:Cast " + cardName + "$target="+ targetName+ "$spellOnStack="+ spellOnStack);
  }
 else {
    player.addAction(turnNum,step,"activate:Cast " + cardName + "$target="+ targetName+ "$!spellOnStack="+ spellOnStack);
  }
}
