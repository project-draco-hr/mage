{
  super(ownerId,163,"Barbed Sliver",Rarity.UNCOMMON,new CardType[]{CardType.CREATURE},"{2}{R}");
  this.expansionSetCode="TMP";
  this.subtype.add("Sliver");
  this.color.setRed(true);
  this.power=new MageInt(2);
  this.toughness=new MageInt(2);
  this.addAbility(new SimpleStaticAbility(Constants.Zone.BATTLEFIELD,new GainAbilityAllEffect(new SimpleActivatedAbility(Constants.Zone.BATTLEFIELD,new BoostSourceEffect(1,0,Constants.Duration.EndOfTurn),new GenericManaCost(2)),Constants.Duration.WhileOnBattlefield,filter,false)));
}
