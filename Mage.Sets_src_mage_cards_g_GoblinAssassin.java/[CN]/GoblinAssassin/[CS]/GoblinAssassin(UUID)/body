{
  super(ownerId,95,"Goblin Assassin",Rarity.UNCOMMON,new CardType[]{CardType.CREATURE},"{3}{R}{R}");
  this.expansionSetCode="LGN";
  this.subtype.add("Goblin");
  this.subtype.add("Assassin");
