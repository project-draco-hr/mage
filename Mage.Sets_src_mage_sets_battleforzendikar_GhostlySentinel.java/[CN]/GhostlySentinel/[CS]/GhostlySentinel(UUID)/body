{
  super(ownerId,28,"Ghostly Sentinel",Rarity.COMMON,new CardType[]{CardType.CREATURE},"{4}{W}");
  this.expansionSetCode="BFZ";
  this.subtype.add("Kor");
  this.subtype.add("Spirit");
