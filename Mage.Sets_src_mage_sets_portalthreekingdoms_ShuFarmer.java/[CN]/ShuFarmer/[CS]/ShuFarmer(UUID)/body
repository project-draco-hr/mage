{
  super(ownerId,23,"Shu Farmer",Rarity.COMMON,new CardType[]{CardType.CREATURE},"{1}{W}");
  this.expansionSetCode="PTK";
  this.subtype.add("Human");
  this.power=new MageInt(1);
  this.toughness=new MageInt(1);
  Ability ability=new ActivateIfConditionActivatedAbility(Zone.BATTLEFIELD,new GainLifeEffect(1),new TapSourceCost(),MyTurnBeforeAttackersDeclaredCondition.getInstance());
  this.addAbility(ability);
}
