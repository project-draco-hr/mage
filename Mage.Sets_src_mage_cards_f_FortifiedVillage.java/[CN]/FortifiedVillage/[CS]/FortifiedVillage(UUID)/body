{
  super(ownerId,274,"Fortified Village",Rarity.RARE,new CardType[]{CardType.LAND},"");
  this.expansionSetCode="SOI";
  this.addAbility(new AsEntersBattlefieldAbility(new TapSourceUnlessPaysEffect(new RevealTargetFromHandCost(new TargetCardInHand(filter))),"you may reveal a Forest or Plains card from your hand. If you don't, {this} enters the battlefield tapped"));
  this.addAbility(new GreenManaAbility());
  this.addAbility(new WhiteManaAbility());
}
