{
  super(ownerId,setInfo,new CardType[]{CardType.CREATURE},"{2}{W}");
  this.subtype.add("Human");
  this.subtype.add("Warrior");
  this.power=new MageInt(2);
  this.toughness=new MageInt(3);
  this.addAbility(new ConditionalTriggeredAbility(new EntersBattlefieldTriggeredAbility(new CreateTokenEffect(new WarriorToken())),RaidCondition.getInstance(),"<i>Raid</i> -  When {this} enters the battlefield, if you attacked with a creature this turn, put a 1/1 white Warrior creature token onto the battlefield."),new PlayerAttackedWatcher());
}
