{
  Permanent permanent=game.getPermanent(source.getSourceId());
  if (permanent == null) {
    return false;
  }
  Permanent equipped=game.getPermanent(permanent.getAttachedTo());
  if (equipped != null && equipped.regenerate(this.getId(),game)) {
    this.used=true;
    return true;
  }
  return false;
}
