{
  if (event.getType() == EventType.ZONE_CHANGE) {
    ZoneChangeEvent zEvent=(ZoneChangeEvent)event;
    if (zEvent.getToZone() == Zone.BATTLEFIELD && zEvent.getTarget().getCardType().contains(CardType.CREATURE) && zEvent.getTargetId() != this.getSourceId()) {
      return true;
    }
  }
  return false;
}
