{
switch (arg1) {
case 0:
    return matches[arg0].getName();
case 1:
  return matches[arg0].getGameType();
case 2:
return matches[arg0].getDeckType();
case 3:
return matches[arg0].getPlayers();
case 4:
return matches[arg0].getResult();
case 5:
return "Replay";
case 6:
return matches[arg0].getGames();
}
return "";
}
