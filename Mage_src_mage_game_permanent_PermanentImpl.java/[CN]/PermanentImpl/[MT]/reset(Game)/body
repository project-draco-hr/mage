{
  this.beforeResetControllerId=this.controllerId;
  this.controllerId=originalControllerId;
  if (!controllerId.equals(beforeResetControllerId)) {
    controllerChanged=true;
  }
 else {
    controllerChanged=false;
  }
  this.maxBlocks=1;
  this.minBlockedBy=1;
  this.maxBlockedBy=0;
  this.copy=false;
}
