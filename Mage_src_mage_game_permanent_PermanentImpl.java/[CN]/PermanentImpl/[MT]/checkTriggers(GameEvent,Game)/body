{
  if (event.getType() == EventType.BEGINNING_PHASE_PRE && game.getActivePlayerId().equals(controllerId))   this.controlledFromStartOfTurn=true;
  for (  TriggeredAbility ability : abilities.getTriggeredAbilities(Zone.BATTLEFIELD)) {
    if (ability.checkTrigger(event,game)) {
      ability.trigger(game,controllerId);
    }
  }
}
