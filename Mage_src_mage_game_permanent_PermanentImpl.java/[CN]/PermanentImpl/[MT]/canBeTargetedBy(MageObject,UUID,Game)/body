{
  if (source != null) {
    if (abilities.containsKey(ShroudAbility.getInstance().getId()))     return false;
    if (abilities.containsKey(HexproofAbility.getInstance().getId()))     if (game.getOpponents(controllerId).contains(sourceControllerId))     return false;
    if (hasProtectionFrom(source,game))     return false;
  }
  return true;
}
