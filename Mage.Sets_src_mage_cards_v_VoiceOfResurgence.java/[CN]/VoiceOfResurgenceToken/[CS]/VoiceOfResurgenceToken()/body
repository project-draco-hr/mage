{
  super("Elemental","X/X green and white Elemental creature with with \"This creature's power and toughness are each equal to the number of creatures you control.");
  setOriginalExpansionSetCode("DGM");
  cardType.add(CardType.CREATURE);
  color.setGreen(true);
  color.setWhite(true);
  subtype.add("Elemental");
  power=new MageInt(0);
  toughness=new MageInt(0);
  this.addAbility(new SimpleStaticAbility(Zone.BATTLEFIELD,new SetPowerToughnessSourceEffect(new PermanentsOnBattlefieldCount(new FilterControlledCreaturePermanent()),Duration.EndOfGame)));
}
