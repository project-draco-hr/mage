{
  super(ownerId,12,"Magus of the Moat",Rarity.RARE,new CardType[]{CardType.CREATURE},"{2}{W}{W}");
  this.expansionSetCode="FUT";
  this.subtype.add("Human");
  this.subtype.add("Wizard");
