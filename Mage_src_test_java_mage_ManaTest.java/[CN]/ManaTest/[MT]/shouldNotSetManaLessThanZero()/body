{
  Mana mana=new Mana();
  mana.setRed(-4);
  mana.setGreen(-4);
  mana.setBlue(-4);
  mana.setWhite(-4);
  mana.setBlack(-4);
  mana.setColorless(-4);
  mana.setAny(-4);
  assertEquals(0,mana.getRed());
  assertEquals(0,mana.getGreen());
  assertEquals(0,mana.getBlue());
  assertEquals(0,mana.getWhite());
  assertEquals(0,mana.getBlack());
  assertEquals(0,mana.getColorless());
  assertEquals(0,mana.getAny());
}
