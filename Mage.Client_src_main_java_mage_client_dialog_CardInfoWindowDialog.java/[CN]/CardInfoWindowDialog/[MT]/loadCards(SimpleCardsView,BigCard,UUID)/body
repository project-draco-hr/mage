{
  boolean changed=cards.loadCards(showCards,bigCard,gameId);
  if (showCards.size() > 0) {
    show();
    if (changed) {
      try {
        if (!positioned) {
          this.setIcon(false);
          firstWindowPosition();
        }
      }
 catch (      PropertyVetoException ex) {
        Logger.getLogger(CardInfoWindowDialog.class.getName()).log(Level.SEVERE,null,ex);
      }
    }
  }
 else {
    this.hideDialog();
  }
}
