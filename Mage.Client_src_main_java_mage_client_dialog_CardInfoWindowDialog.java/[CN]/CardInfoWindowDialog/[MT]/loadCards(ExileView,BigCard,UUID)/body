{
  boolean changed=cards.loadCards(exile,bigCard,gameId,null);
  String titel=name + " (" + exile.size()+ ")";
  setTitle(titel);
  this.setTitelBarToolTip(titel);
  if (exile.size() > 0) {
    show();
    if (changed) {
      try {
        this.setIcon(false);
      }
 catch (      PropertyVetoException ex) {
        logger.error(null,ex);
      }
    }
  }
 else {
    this.hideDialog();
  }
}
