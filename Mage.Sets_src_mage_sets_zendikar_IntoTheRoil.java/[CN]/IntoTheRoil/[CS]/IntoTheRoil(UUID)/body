{
  super(ownerId,48,"Into the Roil",Rarity.COMMON,new CardType[]{CardType.INSTANT},"{1}{U}");
  this.expansionSetCode="ZEN";
  this.color.setBlue(true);
  this.getSpellAbility().addOptionalCost(new KickerManaCost("{1}{U}"));
  this.getSpellAbility().addEffect(new ReturnToHandTargetEffect());
  this.getSpellAbility().addEffect(new ConditionalOneShotEffect(new DrawCardControllerEffect(1),KickedCondition.getInstance(),"If {this} was kicked, draw a card"));
  this.getSpellAbility().addTarget(new TargetNonlandPermanent());
}
