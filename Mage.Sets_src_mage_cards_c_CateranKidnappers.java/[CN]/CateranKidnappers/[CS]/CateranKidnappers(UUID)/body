{
  super(ownerId,122,"Cateran Kidnappers",Rarity.UNCOMMON,new CardType[]{CardType.CREATURE},"{2}{B}{B}");
  this.expansionSetCode="MMQ";
  this.subtype.add("Human");
  this.subtype.add("Mercenary");
