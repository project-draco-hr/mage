{
  super(ownerId,89,"Snake Cult Initiation",Rarity.UNCOMMON,new CardType[]{CardType.ENCHANTMENT},"{3}{B}");
  this.expansionSetCode="FUT";
  this.subtype.add("Aura");
  TargetPermanent auraTarget=new TargetCreaturePermanent();
  this.getSpellAbility().addTarget(auraTarget);
  this.getSpellAbility().addEffect(new AttachEffect(Outcome.AddAbility));
  Ability ability=new EnchantAbility(auraTarget.getTargetName());
  this.addAbility(ability);
  this.addAbility(new SimpleStaticAbility(Zone.BATTLEFIELD,new GainAbilityAttachedEffect(new PoisonousAbility(3),AttachmentType.AURA)));
}
