{
  super(ownerId,70,"Magus of the Abyss",Rarity.RARE,new CardType[]{CardType.CREATURE},"{3}{B}");
  this.expansionSetCode="FUT";
  this.subtype.add("Human");
  this.subtype.add("Wizard");
