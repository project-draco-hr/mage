{
  Permanent p=(Permanent)game.getLastKnownInformation(targetPointer.getFirst(source),Constants.Zone.BATTLEFIELD);
  if (p != null) {
    Player controller=game.getPlayer(p.getControllerId());
    if (controller != null) {
      controller.loseLife(1,game);
      return true;
    }
  }
  return false;
}
