{
  super(ownerId,131,"Spectral Bears",Rarity.UNCOMMON,new CardType[]{CardType.CREATURE},"{1}{G}");
  this.expansionSetCode="MED";
  this.subtype.add("Bear");
  this.subtype.add("Spirit");
