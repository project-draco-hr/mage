{
  for (  Card card : getAllCards()) {
    String name=card.getName();
    if (name.equals("Baneslayer Angel") || name.equals("Lightning Bolt") || name.equals("Zombie Outlander")|| name.equals("Naturalize")|| name.equals("Kraken's Eye")|| name.equals("Serra Angel")) {
      System.out.println(name + " : " + rateCard(card));
    }
  }
}
