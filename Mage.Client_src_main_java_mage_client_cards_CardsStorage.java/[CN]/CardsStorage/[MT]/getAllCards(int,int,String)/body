{
  List<Card> cards=new ArrayList<Card>();
  List<Card> pool;
  if (set == null) {
    pool=allCards;
  }
 else {
    pool=new ArrayList<Card>();
    for (    Card card : allCards) {
      if (card.getExpansionSetCode().equals(set)) {
        pool.add(card);
      }
    }
  }
  for (int i=start; i < Math.min(end + 1,pool.size()); i++) {
    cards.add(pool.get(i));
  }
  return cards;
}
