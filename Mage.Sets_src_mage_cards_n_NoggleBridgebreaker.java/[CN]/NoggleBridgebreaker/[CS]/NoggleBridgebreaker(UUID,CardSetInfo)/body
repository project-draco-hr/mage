{
  super(ownerId,setInfo,new CardType[]{CardType.CREATURE},"{2}{U/R}{U/R}");
  this.subtype.add("Noggle");
  this.subtype.add("Rogue");
  this.power=new MageInt(4);
  this.toughness=new MageInt(3);
