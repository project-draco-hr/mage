{
  super(ownerId,82,"Bloodchief Ascension",Rarity.RARE,new CardType[]{CardType.ENCHANTMENT},"{B}");
  this.expansionSetCode="ZEN";
  this.color.setBlack(true);
  this.addAbility(new BeginningOfEndStepTriggeredAbility(Zone.BATTLEFIELD,new AddCountersSourceEffect(CounterType.QUEST.createInstance(),false),TargetController.ANY,new OpponentLostLifeCondition(Condition.ComparisonType.GreaterThan,1),true));
  this.addAbility(new BloodchiefAscensionTriggeredAbility());
}
