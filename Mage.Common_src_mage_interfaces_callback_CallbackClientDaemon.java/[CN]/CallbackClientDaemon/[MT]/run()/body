{
  try {
    while (true) {
      final ClientCallback callback=server.callback(id);
      callbackExecutor.submit(new Runnable(){
        @Override public void run(){
          try {
            if (callback != null) {
              client.processCallback(callback);
            }
          }
 catch (          Exception ex) {
            logger.fatal("CallbackClientDaemon error ",ex);
          }
        }
      }
);
    }
  }
 catch (  Exception ex) {
    logger.fatal("CallbackClientDaemon error ",ex);
  }
}
