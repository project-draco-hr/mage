{
  try {
    while (true) {
      final ClientCallback callback=server.callback(id);
      callbackExecutor.submit(new Runnable(){
        @Override public void run(){
          try {
            client.processCallback(callback);
          }
 catch (          Exception ex) {
            logger.log(Level.SEVERE,null,ex);
          }
        }
      }
);
    }
  }
 catch (  Exception ex) {
    logger.log(Level.SEVERE,null,ex);
  }
}
