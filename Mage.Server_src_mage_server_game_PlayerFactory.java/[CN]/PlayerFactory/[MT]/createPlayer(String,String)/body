{
  Player player;
  Constructor<?> con;
  try {
    con=playerTypes.get(playerType).getConstructor(new Class[]{String.class});
    player=(Player)con.newInstance(new Object[]{name});
  }
 catch (  Exception ex) {
    logger.log(Level.SEVERE,null,ex);
    return null;
  }
  logger.info("Player created: " + name + "-"+ player.getId().toString());
  return player;
}
