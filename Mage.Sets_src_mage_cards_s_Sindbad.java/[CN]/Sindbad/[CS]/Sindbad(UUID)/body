{
  super(ownerId,100,"Sindbad",Rarity.UNCOMMON,new CardType[]{CardType.CREATURE},"{1}{U}");
  this.expansionSetCode="4ED";
  this.subtype.add("Human");
  this.power=new MageInt(1);
