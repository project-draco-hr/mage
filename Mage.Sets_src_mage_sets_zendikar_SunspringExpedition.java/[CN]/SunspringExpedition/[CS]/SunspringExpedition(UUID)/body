{
  super(ownerId,"Sunspring Expedition",new CardType[]{CardType.ENCHANTMENT},"{W}");
  this.expansionSetId=Zendikar.getInstance().getId();
  this.color.setWhite(true);
  this.art="123558_typ_reg_sty_010.jpg";
