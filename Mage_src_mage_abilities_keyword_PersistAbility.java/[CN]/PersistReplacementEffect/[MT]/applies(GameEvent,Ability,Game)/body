{
  if (event.getTargetId().equals(source.getSourceId())) {
    Object fixedTarget=game.getState().getValue("persist" + source.getSourceId().toString());
    if (fixedTarget instanceof FixedTarget && ((FixedTarget)fixedTarget).getTarget().equals(source.getSourceId()) && ((FixedTarget)fixedTarget).getZoneChangeCounter() + 1 == game.getState().getZoneChangeCounter(source.getSourceId())) {
      return true;
    }
  }
  return false;
}
