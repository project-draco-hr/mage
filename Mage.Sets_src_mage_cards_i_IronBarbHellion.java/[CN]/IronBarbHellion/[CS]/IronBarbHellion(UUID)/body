{
  super(ownerId,69,"Iron-Barb Hellion",Rarity.UNCOMMON,new CardType[]{CardType.CREATURE},"{5}{R}");
  this.expansionSetCode="5DN";
  this.subtype.add("Hellion");
  this.subtype.add("Beast");
