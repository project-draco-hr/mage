{
  if (f.isDirectory())   return true;
  String ext=null;
  String s=f.getName();
  int i=s.lastIndexOf('.');
  if (i > 0 && i < s.length() - 1) {
    ext=s.substring(i + 1).toLowerCase();
  }
  if (ext != null) {
    if (ext.equals("dec") || ext.equals("mwDeck"))     return true;
  }
  return false;
}
