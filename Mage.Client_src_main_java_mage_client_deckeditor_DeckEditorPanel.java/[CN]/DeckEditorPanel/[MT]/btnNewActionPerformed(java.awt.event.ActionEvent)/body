{
  if (mode == DeckEditorMode.SIDEBOARDING || mode == DeckEditorMode.LIMITED_BUILDING) {
    for (    Card card : deck.getCards()) {
      deck.getSideboard().add(card);
    }
    deck.getCards().clear();
    cardSelector.loadSideboard(new ArrayList<>(deck.getSideboard()),this.bigCard);
  }
 else {
    deck=new Deck();
  }
  refreshDeck();
}
