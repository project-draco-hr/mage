{
  StringBuilder sbRule=new StringBuilder("Flashback ");
  boolean first=true;
  if (manaCosts.size() > 0) {
    sbRule.append(manaCosts.getText());
    first=false;
  }
  if (costs.size() > 0) {
    if (first) {
      sbRule.append(",");
    }
    sbRule.append(costs.getText());
  }
  return sbRule.toString();
}
