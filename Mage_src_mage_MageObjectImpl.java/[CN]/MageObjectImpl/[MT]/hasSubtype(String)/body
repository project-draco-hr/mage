{
  if (value == null) {
    return false;
  }
  if (this.subtype.contains(value)) {
    return true;
  }
 else {
    if (value.equals("Mountain") || value.equals("Island") || value.equals("Plains")|| value.equals("Forest")|| value.equals("Swamp")|| value.equals("Aura")|| value.equals("Equipment")|| value.equals("Fortification")|| value.equals("Shrine")) {
      return false;
    }
    return abilities.contains(ChangelingAbility.getInstance());
  }
}
