{
  if (subtype == null) {
    return false;
  }
  if (subtype.contains(subtype)) {
    return true;
  }
 else {
    if (subtype.equals("Mountain") || subtype.equals("Island") || subtype.equals("Plains")|| subtype.equals("Forest")|| subtype.equals("Swamp")|| subtype.equals("Aura")|| subtype.equals("Equipment")|| subtype.equals("Fortification")) {
      return false;
    }
    return abilities.contains(ChangelingAbility.getInstance());
  }
}
