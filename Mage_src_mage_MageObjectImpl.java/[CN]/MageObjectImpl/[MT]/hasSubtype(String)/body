{
  if (value == null) {
    return false;
  }
  if (this.subtype.contains(value)) {
    return true;
  }
 else {
    if (CardUtil.isNonCreatureSubtype(value)) {
      return false;
    }
    return abilities.contains(ChangelingAbility.getInstance()) || this.subtype.contains(ChangelingAbility.ALL_CREATURE_TYPE);
  }
}
