{
  super(ownerId,113,"Weirded Vampire",Rarity.COMMON,new CardType[]{CardType.CREATURE},"{3}{B}");
  this.expansionSetCode="EMN";
  this.subtype.add("Vampire");
  this.subtype.add("Horror");
