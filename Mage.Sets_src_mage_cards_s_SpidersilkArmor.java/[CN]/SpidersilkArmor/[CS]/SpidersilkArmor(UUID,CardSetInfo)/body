{
  super(ownerId,setInfo,new CardType[]{CardType.ENCHANTMENT},"{2}{G}");
  Ability ability=new SimpleStaticAbility(Zone.BATTLEFIELD,new BoostControlledEffect(0,1,Duration.WhileOnBattlefield));
  Effect effect=new GainAbilityControlledEffect(ReachAbility.getInstance(),Duration.WhileOnBattlefield,new FilterCreaturePermanent());
  effect.setText("and have reach.");
  ability.addEffect(effect);
  this.addAbility(ability);
}
