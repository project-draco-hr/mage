{
  super(ownerId,164,"Trapjaw Kelpie",Rarity.COMMON,new CardType[]{CardType.CREATURE},"{4}{G/U}{G/U}");
  this.expansionSetCode="EVE";
  this.subtype.add("Beast");
  this.power=new MageInt(3);
