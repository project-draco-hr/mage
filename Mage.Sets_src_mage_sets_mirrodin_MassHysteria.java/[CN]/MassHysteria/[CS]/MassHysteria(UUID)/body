  this.addAbility(new SimpleStaticAbility(Constants.Zone.BATTLEFIELD,new GainAbilityAllEffect(HasteAbility.getInstance(),Constants.Duration.WhileOnBattlefield,FilterCreaturePermanent.getDefault(),false)));
