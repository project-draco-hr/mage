{
  super(ownerId,102,"Mutilate",Rarity.RARE,new CardType[]{CardType.SORCERY},"{2}{B}{B}");
  this.expansionSetCode="M13";
  this.color.setBlack(true);
  PermanentsOnBattlefieldCount count=new PermanentsOnBattlefieldCount(filter,-1);
