{
  super(ownerId,setInfo,new CardType[]{CardType.CREATURE},"{1}{W/U}{W/U}");
  this.subtype.add("Kithkin");
  this.subtype.add("Wizard");
  this.power=new MageInt(2);
  this.toughness=new MageInt(2);
