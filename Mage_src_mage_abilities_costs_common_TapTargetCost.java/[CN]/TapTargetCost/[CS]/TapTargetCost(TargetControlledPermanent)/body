{
  this.target=target;
  this.target.setNotTarget(true);
  this.text=new StringBuilder("Tap ").append((target.getTargetName().startsWith("a ") || target.getTargetName().startsWith("an ") || target.getTargetName().startsWith("another")) ? "" : CardUtil.numberToText(target.getMaxNumberOfTargets()) + " ").append(target.getTargetName()).toString();
}
