{
  this.target=target;
  this.target.setNotTarget(true);
  this.text=new StringBuilder("Tap ").append(target.getTargetName().startsWith("a ") || target.getTargetName().startsWith("an ") ? "" : CardUtil.numberToText(target.getMaxNumberOfTargets())).append(" ").append(target.getTargetName()).toString();
}
