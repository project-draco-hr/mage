{
  super(ownerId,setInfo,new CardType[]{CardType.ARTIFACT},"{2}");
  this.subtype.add("Equipment");
  Ability ability=new SimpleStaticAbility(Zone.BATTLEFIELD,new GainAbilityAttachedEffect(FlyingAbility.getInstance(),AttachmentType.AURA,Duration.WhileInGraveyard));
  ability.addEffect(new GainAbilityAttachedEffect(HasteAbility.getInstance(),AttachmentType.AURA,Duration.WhileInGraveyard));
  this.addAbility(ability);
  this.addAbility(new EquipAbility(Outcome.AddAbility,new GenericManaCost(2)));
}
