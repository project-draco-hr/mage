{
  super(ownerId,45,"Soulbound Guardians",Rarity.UNCOMMON,new CardType[]{CardType.CREATURE},"{4}{W}");
  this.expansionSetCode="ROE";
  this.subtype.add("Kor");
  this.subtype.add("Spirit");
