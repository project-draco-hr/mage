{
  super(ownerId,22,"Marchesa's Infiltrator",Rarity.UNCOMMON,new CardType[]{CardType.CREATURE},"{2}{U}");
  this.expansionSetCode="CNS";
  this.subtype.add("Human");
  this.subtype.add("Rogue");
