{
  super(ownerId,34,"Lone Missionary",Rarity.COMMON,new CardType[]{CardType.CREATURE},"{1}{W}");
  this.expansionSetCode="ROE";
  this.subtype.add("Kor");
  this.subtype.add("Monk");
