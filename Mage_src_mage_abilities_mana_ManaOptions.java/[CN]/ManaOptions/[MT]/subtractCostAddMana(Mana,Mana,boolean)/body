{
  if (isEmpty()) {
    this.add(new Mana());
  }
  boolean addAny=false;
  if (addMana.getAny() == 1 && addMana.count() == 1) {
    addAny=true;
  }
  for (  Mana mana : this) {
    while (mana.includesMana(cost)) {
      mana.subtract(cost);
      mana.add(addMana);
      if (!addAny) {
        break;
      }
    }
  }
}
