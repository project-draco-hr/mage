{
  super(ownerId,setInfo,new CardType[]{CardType.CREATURE},"{2}{B}");
  this.subtype.add("Human");
  this.subtype.add("Rogue");
  this.subtype.add("Rigger");
  this.power=new MageInt(2);
  this.toughness=new MageInt(2);
  Ability ability=new ZoneChangeAllTriggeredAbility(Zone.BATTLEFIELD,Zone.BATTLEFIELD,Zone.GRAVEYARD,new AddCountersSourceEffect(CounterType.P1P1.createInstance(1)),new FilterArtifactPermanent(),"Whenever an artifact is put into a graveyard from the battlefield, ",true);
  this.addAbility(ability);
}
