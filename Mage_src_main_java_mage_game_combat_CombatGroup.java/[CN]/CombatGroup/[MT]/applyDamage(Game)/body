{
  for (  UUID uuid : attackers) {
    Permanent permanent=game.getPermanent(uuid);
    if (permanent != null) {
      permanent.applyDamage(game);
    }
  }
  for (  UUID uuid : blockers) {
    Permanent permanent=game.getPermanent(uuid);
    if (permanent != null) {
      permanent.applyDamage(game);
    }
  }
}
