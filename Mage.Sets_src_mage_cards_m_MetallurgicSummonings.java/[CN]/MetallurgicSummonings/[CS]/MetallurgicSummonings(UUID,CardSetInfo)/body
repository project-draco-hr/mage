{
  super(ownerId,setInfo,new CardType[]{CardType.ENCHANTMENT},"{3}{U}{U}");
  this.addAbility(new SpellCastControllerTriggeredAbility(new MetallurgicSummoningsTokenEffect(),filter,false,true));
  Ability ability=new ConditionalActivatedAbility(Zone.BATTLEFIELD,new MetallurgicSummoningsReturnEffect(),new ManaCostsImpl("{3}{U}{U}"),new PermanentsOnTheBattlefieldCondition(new FilterControlledArtifactPermanent(),PermanentsOnTheBattlefieldCondition.CountType.MORE_THAN,5),"{3}{U}{U}, Exile {this}: Return all instant and sorcery cards from your graveyard to your hand." + " Activate this ability only if you control six or more artifacts.");
  ability.addCost(new ExileSourceCost());
  this.addAbility(ability);
}
