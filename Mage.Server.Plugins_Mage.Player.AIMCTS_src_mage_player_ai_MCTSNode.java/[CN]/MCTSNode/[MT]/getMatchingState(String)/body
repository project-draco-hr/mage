{
  ArrayDeque<MCTSNode> queue=new ArrayDeque<MCTSNode>();
  queue.add(this);
  while (!queue.isEmpty()) {
    MCTSNode current=queue.remove();
    if (current.stateValue.equals(state))     return current;
    for (    MCTSNode child : current.children) {
      queue.add(child);
    }
  }
  return null;
}
