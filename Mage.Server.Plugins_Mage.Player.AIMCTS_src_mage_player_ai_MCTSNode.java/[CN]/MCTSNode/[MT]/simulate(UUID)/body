{
  long startTime=System.nanoTime();
  Game sim=createSimulation(game);
  sim.resume();
  long duration=System.nanoTime() - startTime;
  int retVal=0;
  for (  Player simPlayer : sim.getPlayers().values()) {
    if (simPlayer.getId().equals(playerId) && simPlayer.hasWon()) {
      retVal=1;
    }
  }
  return retVal;
}
