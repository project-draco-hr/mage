{
  super(ownerId,setInfo,new CardType[]{CardType.SORCERY},"{2}{G}");
  this.getSpellAbility().addEffect(new SearchLibraryPutInHandEffect(new TargetCardInLibrary(0,2,new FilterBasicLandCard())));
  Mode mode=new Mode();
  mode.getEffects().add(new PlayAdditionalLandsControllerEffect(2,Duration.EndOfTurn));
  this.getSpellAbility().getModes().addMode(mode);
  this.addAbility(new EntwineAbility("{2}{G}"));
}
