{
  if (sourceId != null) {
    MageObject mageObject=game.getPermanent(sourceId);
    if (mageObject == null) {
      mageObject=game.getCard(sourceId);
    }
    if (mageObject != null) {
      message+="<div style='font-size:11pt'>" + mageObject.getLogName() + "</div>";
    }
  }
  return message;
}
