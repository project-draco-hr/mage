{
  if (this.getUserData().isShowAbilityPickerForced()) {
    if (ability instanceof PlayLandAbility) {
      return true;
    }
    for (    Cost cost : ability.getCosts()) {
      if (!(cost instanceof TapSourceCost) || !((cost instanceof ManaCosts) && ((ManaCosts)cost).convertedManaCost() > 0)) {
        return true;
      }
    }
    return false;
  }
  return true;
}
