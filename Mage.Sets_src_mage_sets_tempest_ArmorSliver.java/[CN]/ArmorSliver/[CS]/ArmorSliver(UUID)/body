{
  super(ownerId,216,"Armor Sliver",Rarity.UNCOMMON,new CardType[]{CardType.CREATURE},"{2}{W}");
  this.expansionSetCode="TMP";
  this.subtype.add("Sliver");
  this.color.setWhite(true);
  this.power=new MageInt(2);
  this.toughness=new MageInt(2);
  this.addAbility(new SimpleStaticAbility(Constants.Zone.BATTLEFIELD,new GainAbilityAllEffect(new SimpleActivatedAbility(Constants.Zone.BATTLEFIELD,new BoostSourceEffect(0,1,Constants.Duration.EndOfTurn),new GenericManaCost(2)),Constants.Duration.WhileOnBattlefield,filter,false)));
}
