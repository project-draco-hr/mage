{
  super(ownerId,132,"Sudden Strength",Rarity.COMMON,new CardType[]{CardType.INSTANT},"{3}{G}");
  this.expansionSetCode="JUD";
  this.getSpellAbility().addEffect(new BoostTargetEffect(3,3,Duration.EndOfTurn));
