{
  ObjectColor color=new ObjectColor();
  if (choice.equals("Black"))   color.setBlack(true);
 else   if (choice.equals("Blue"))   color.setBlue(true);
 else   if (choice.equals("Green"))   color.setGreen(true);
 else   if (choice.equals("Red"))   color.setRed(true);
 else   if (choice.equals("White"))   color.setWhite(true);
  return color;
}
