{
  super(ownerId,12,"Kjeldoran Outrider",Rarity.COMMON,new CardType[]{CardType.CREATURE},"{1}{W}");
  this.expansionSetCode="CSP";
  this.subtype.add("Human");
  this.subtype.add("Soldier");
