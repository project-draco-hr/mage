{
  super(ownerId,94,"Peek",Rarity.COMMON,new CardType[]{CardType.INSTANT},"{U}");
  this.expansionSetCode="10E";
  this.getSpellAbility().addEffect(new LookAtTargetPlayerHandEffect());
