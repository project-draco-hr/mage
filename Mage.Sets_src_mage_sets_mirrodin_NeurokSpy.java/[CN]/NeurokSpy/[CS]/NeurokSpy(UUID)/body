{
  super(ownerId,44,"Neurok Spy",Rarity.COMMON,new CardType[]{CardType.CREATURE},"{2}{U}");
  this.expansionSetCode="MRD";
  this.subtype.add("Human");
  this.subtype.add("Rogue");
  this.color.setBlue(true);
  this.power=new MageInt(2);
  this.toughness=new MageInt(2);
  Effect effect=new ConditionalContinousEffect(new UnblockableSourceEffect(),new DefendingPlayerControlsArtifact(),"Neurok Spy is unblockable as long as defending player controls an artifact");
  this.addAbility(new SimpleStaticAbility(Zone.BATTLEFIELD,effect));
}
