{
  super(ownerId,134,"Falkenrath Exterminator",Rarity.UNCOMMON,new CardType[]{CardType.CREATURE},"{1}{R}");
  this.expansionSetCode="AVR";
  this.subtype.add("Vampire");
  this.subtype.add("Archer");
  this.color.setRed(true);
  this.power=new MageInt(1);
  this.toughness=new MageInt(1);
  this.addAbility(new DealsCombatDamageToAPlayerTriggeredAbility(new AddCountersSourceEffect(CounterType.P1P1.createInstance()),false));
  Ability ability=new SimpleActivatedAbility(Constants.Zone.BATTLEFIELD,new DamageTargetEffect(new CountersCount(CounterType.P1P1)),new ManaCostsImpl("{2}{R}"));
  ability.addTarget(new TargetCreaturePermanent());
  this.addAbility(ability);
}
