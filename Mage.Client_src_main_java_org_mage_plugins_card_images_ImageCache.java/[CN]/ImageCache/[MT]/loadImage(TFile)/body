{
  BufferedImage image=null;
  if (!file.exists()) {
    log.debug("File does not exist: " + file.toString());
    return null;
  }
  try {
    TFileInputStream inputStream=new TFileInputStream(file);
    image=ImageIO.read(inputStream);
    inputStream.close();
  }
 catch (  Exception e) {
    log.error(e,e);
  }
  return image;
}
