  this.toughness=new MageInt(0);
  this.addAbility(new SimpleStaticAbility(Zone.BATTLEFIELD,new SetPowerToughnessSourceEffect(new LordOfExtinctionDynamicCount(),Duration.WhileOnBattlefield)));
}
