{
  super(ownerId,91,"Lord of Extinction",Rarity.MYTHIC,new CardType[]{CardType.CREATURE},"{3}{B}{G}");
  this.expansionSetCode="ARB";
  this.subtype.add("Elemental");
  this.color.setGreen(true);
  this.color.setBlack(true);
