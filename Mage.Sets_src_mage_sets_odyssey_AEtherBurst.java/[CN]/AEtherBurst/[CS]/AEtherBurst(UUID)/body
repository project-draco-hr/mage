{
  super(ownerId,60,"AEther Burst",Rarity.COMMON,new CardType[]{CardType.INSTANT},"{1}{U}");
  this.expansionSetCode="ODY";
  this.color.setBlue(true);
  this.getSpellAbility().addEffect(new DynamicReturnToHandTargetEffect());
