{
  if (!isLogging()) {
    return;
  }
  if (currentBooster != null) {
    String lastPick=getCardName(getLastPick(pickView.getPicks().values()));
    if (lastPick != null) {
      logPick(lastPick);
    }
    currentBooster=null;
  }
  setCurrentBoosterForLog(pickView.getBooster());
  if (currentBooster.length == 1) {
    logPick(currentBooster[0]);
  }
}
