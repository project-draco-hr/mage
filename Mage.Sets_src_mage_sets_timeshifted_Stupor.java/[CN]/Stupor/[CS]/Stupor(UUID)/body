{
  super(ownerId,48,"Stupor",Rarity.COMMON,new CardType[]{CardType.SORCERY},"{2}{B}");
  this.expansionSetCode="TSB";
  this.color.setBlack(true);
  this.getSpellAbility().addEffect(new DiscardTargetEffect(1,true));
