{
  super(ownerId,141,"Shreds of Sanity",Rarity.UNCOMMON,new CardType[]{CardType.SORCERY},"{2}{R}");
  this.expansionSetCode="EMN";
  this.getSpellAbility().addEffect(new ShredsOfSanityEffect());
