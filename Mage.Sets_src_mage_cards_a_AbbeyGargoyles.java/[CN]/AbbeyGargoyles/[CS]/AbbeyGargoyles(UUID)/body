{
  super(ownerId,277,"Abbey Gargoyles",Rarity.UNCOMMON,new CardType[]{CardType.CREATURE},"{2}{W}{W}{W}");
  this.expansionSetCode="5ED";
  this.subtype.add("Gargoyle");
  this.power=new MageInt(3);
