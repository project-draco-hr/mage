{
  super(ownerId,71,"Send to Sleep",Rarity.COMMON,new CardType[]{CardType.INSTANT},"{1}{U}");
  this.expansionSetCode="ORI";
  this.getSpellAbility().addEffect(new TapTargetEffect());
  this.getSpellAbility().addTarget(new TargetCreaturePermanent(0,2));
  Effect effect=new ConditionalContinuousRuleModifyingEffect(new DontUntapInControllersNextUntapStepTargetEffect(),SpellMasteryCondition.getInstance());
  this.getSpellAbility().addEffect(effect);
}
