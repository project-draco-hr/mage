{
  super(ownerId,setInfo,new CardType[]{CardType.CREATURE},"{2}{G}{G}");
  this.subtype.add("Faerie");
  this.subtype.add("Druid");
  this.power=new MageInt(2);
  this.toughness=new MageInt(2);
  this.addAbility(new SimpleActivatedAbility(Zone.BATTLEFIELD,new PutPermanentOnBattlefieldEffect(filter),new TapSourceCost()));
  Ability ability=new SimpleActivatedAbility(Zone.BATTLEFIELD,new GainAbilityTargetEffect(ProtectionAbility.from(ObjectColor.BLACK),Duration.EndOfTurn),new ManaCostsImpl("{2}{G}"));
  Target target=new TargetPermanent(greenCreature);
  ability.addTarget(target);
  this.addAbility(ability);
}
