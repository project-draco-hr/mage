{
  StringBuilder sb=new StringBuilder();
  String msg=message != null && message.length() > 5 ? message.substring(0,5) : message;
  sb.append("Feedback [username=").append(username).append(", host=").append(host).append(", title=").append(title).append(", type=").append(type).append(", message=").append(msg).append("...]");
  return sb.toString();
}
