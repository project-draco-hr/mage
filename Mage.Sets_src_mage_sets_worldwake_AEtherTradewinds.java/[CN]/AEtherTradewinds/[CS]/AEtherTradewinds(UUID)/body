{
  super(ownerId,24,"AEther Tradewinds",Rarity.COMMON,new CardType[]{CardType.INSTANT},"{2}{U}");
  this.expansionSetCode="WWK";
  this.color.setBlue(true);
  this.getSpellAbility().addTarget(new TargetControlledPermanent());
