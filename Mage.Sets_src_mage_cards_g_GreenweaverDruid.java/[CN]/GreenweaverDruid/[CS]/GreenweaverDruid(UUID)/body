{
  super(ownerId,164,"Greenweaver Druid",Rarity.UNCOMMON,new CardType[]{CardType.CREATURE},"{2}{G}");
  this.expansionSetCode="ZEN";
  this.subtype.add("Elf");
  this.subtype.add("Druid");
