{
  super(ownerId,200,"Sandsteppe Scavenger",Rarity.COMMON,new CardType[]{CardType.CREATURE},"{4}{G}");
  this.expansionSetCode="DTK";
  this.subtype.add("Hound");
  this.subtype.add("Scount");
