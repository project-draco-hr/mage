{
  Target targetCards=source.getTargets().get(1);
  if (targetCards != null) {
    for (    UUID targetID : targetCards.getTargets()) {
      Card card=game.getCard(targetID);
      if (card != null) {
        card.moveToExile(null,"",source.getId(),game);
      }
    }
    return true;
  }
  return false;
}
