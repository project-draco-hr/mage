{
  super(ownerId,90,"Quirion Explorer",Rarity.COMMON,new CardType[]{CardType.CREATURE},"{1}{G}");
  this.expansionSetCode="PLS";
  this.subtype.add("Elf");
  this.subtype.add("Druid");
  this.subtype.add("Scout");
