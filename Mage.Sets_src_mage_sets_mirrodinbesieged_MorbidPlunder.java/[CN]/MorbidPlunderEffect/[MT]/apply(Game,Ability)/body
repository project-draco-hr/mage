{
  boolean result=false;
  List<UUID> targets=source.getTargets().get(0).getTargets();
  if (targets.size() > 0) {
    for (    UUID target : targets) {
      Card card=game.getCard(target);
      if (card != null) {
        result|=card.moveToZone(Zone.HAND,source.getId(),game,true);
      }
    }
  }
  return result;
}
