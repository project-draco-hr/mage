{
  super(ownerId,211,"Sliver Hivelord",Rarity.MYTHIC,new CardType[]{CardType.CREATURE},"{W}{U}{B}{R}{G}");
  this.expansionSetCode="M15";
  this.supertype.add("Legendary");
  this.subtype.add("Sliver");
  this.power=new MageInt(5);
  this.toughness=new MageInt(5);
  this.addAbility(new SimpleStaticAbility(Zone.BATTLEFIELD,new GainAbilityControlledEffect(IndestructibleAbility.getInstance(),Duration.WhileOnBattlefield,new FilterCreaturePermanent("Sliver","Sliver creatures"))));
}
