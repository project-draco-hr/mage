{
  super(ownerId,229,"Kitchen Finks",Rarity.UNCOMMON,new CardType[]{CardType.CREATURE},"{1}{G/W}{G/W}");
  this.expansionSetCode="SHM";
  this.subtype.add("Ouphe");
