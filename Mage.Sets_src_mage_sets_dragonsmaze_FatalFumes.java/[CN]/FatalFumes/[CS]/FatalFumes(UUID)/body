{
  super(ownerId,24,"Fatal Fumes",Rarity.COMMON,new CardType[]{CardType.INSTANT},"{3}{B}");
  this.expansionSetCode="DGM";
  this.color.setBlack(true);
  this.getSpellAbility().addTarget(new TargetCreaturePermanent());
