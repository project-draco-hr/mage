{
  if (staticText != null && !staticText.isEmpty()) {
    return staticText;
  }
  StringBuilder sb=new StringBuilder();
  sb.append("Return ");
  if (!mode.getTargets().get(0).getTargetName().startsWith("another")) {
    sb.append("target ");
  }
  sb.append(mode.getTargets().get(0).getTargetName()).append(" to your hand");
  return sb.toString();
}
