{
  super(ownerId,154,"Scout Trooper",Rarity.COMMON,new CardType[]{CardType.CREATURE},"{1}{G}");
  this.expansionSetCode="SWS";
  this.subtype.add("Human");
  this.subtype.add("Trooper");
  this.subtype.add("Scout");
