{
  super(ownerId,39,"Veteran Swordsmith",Rarity.COMMON,new CardType[]{CardType.CREATURE},"{2}{W}");
  this.expansionSetCode="M10";
  this.subtype.add("Human");
  this.subtype.add("Soldier");
