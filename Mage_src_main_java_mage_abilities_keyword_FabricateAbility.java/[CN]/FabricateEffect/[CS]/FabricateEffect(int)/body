{
  super(Outcome.Benefit);
  this.value=value;
  this.staticText="Fabricate " + value + " <i>(When this creature enters the battlefield, put "+ CardUtil.numberToText(value,"a")+ " +1/+1 counter"+ (value > 1 ? "s" : "")+ " on it or create "+ CardUtil.numberToText(value,"a")+ " 1/1 colorless Servo artifact creature token"+ (value > 1 ? "s" : "")+ ".)</i>";
}
