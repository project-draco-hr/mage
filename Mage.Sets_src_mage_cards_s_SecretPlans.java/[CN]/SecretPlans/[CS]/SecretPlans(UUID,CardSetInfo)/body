{
  super(ownerId,setInfo,new CardType[]{CardType.ENCHANTMENT},"{G}{U}");
  this.addAbility(new SimpleStaticAbility(Zone.BATTLEFIELD,new BoostControlledEffect(0,1,Duration.WhileOnBattlefield,filter)));
