{
  if (type == null || type.isEmpty()) {
    return "";
  }
  if (type.toLowerCase().startsWith("bug")) {
    return "bug";
  }
  if (type.toLowerCase().startsWith("feature")) {
    return "feature";
  }
  if (type.toLowerCase().startsWith("thank")) {
    return "thank";
  }
  if (type.toLowerCase().startsWith("question")) {
    return "question";
  }
  return "";
}
