{
  super(ownerId,125,"Goldenhide Ox",Rarity.UNCOMMON,new CardType[]{CardType.ENCHANTMENT,CardType.CREATURE},"{5}{G}");
  this.expansionSetCode="JOU";
  this.subtype.add("Ox");
  this.color.setGreen(true);
  this.power=new MageInt(5);
  this.toughness=new MageInt(4);
  Ability ability=new ConstellationAbility(new MustBeBlockedByAtLeastOneTargetEffect(Duration.EndOfTurn),false);
  ability.addTarget(new TargetCreaturePermanent(true));
  this.addAbility(ability);
}
