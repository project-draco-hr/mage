{
  String text="";
  if (partOfOptionalEffect) {
    text="If you do, ";
  }
  text+="{this} assigns no combat damage";
switch (duration) {
case EndOfTurn:
    text+=" this turn";
  break;
case EndOfCombat:
text+=" this combat";
break;
default :
if (!duration.toString().isEmpty()) {
text+=" " + duration.toString();
}
}
return text;
}
