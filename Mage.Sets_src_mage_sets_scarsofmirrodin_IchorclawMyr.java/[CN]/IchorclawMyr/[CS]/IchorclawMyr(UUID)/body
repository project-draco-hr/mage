{
  super(ownerId,166,"Ichorclaw Myr",Rarity.COMMON,new CardType[]{CardType.ARTIFACT,CardType.CREATURE},"{2}");
  this.expansionSetCode="SOM";
  this.subtype.add("Myr");
  this.power=new MageInt(1);
  this.toughness=new MageInt(1);
  this.addAbility(InfectAbility.getInstance());
  this.addAbility(new GenericTriggeredAbility(BecomesBlockedTrigger.getInstance(),new BoostSourceEffect(2,2,Duration.EndOfTurn),false));
}
