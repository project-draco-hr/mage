{
  super(ownerId,66,"Doomwake Giant",Rarity.RARE,new CardType[]{CardType.ENCHANTMENT,CardType.CREATURE},"{4}{B}");
  this.expansionSetCode="JOU";
  this.subtype.add("Giant");
