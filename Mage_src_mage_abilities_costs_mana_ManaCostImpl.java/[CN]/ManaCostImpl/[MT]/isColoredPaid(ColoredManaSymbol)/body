{
switch (mana) {
case B:
    if (this.payment.getBlack() > 0) {
      return true;
    }
case U:
  if (this.payment.getBlue() > 0) {
    return true;
  }
case W:
if (this.payment.getWhite() > 0) {
  return true;
}
case G:
if (this.payment.getGreen() > 0) {
return true;
}
case R:
if (this.payment.getRed() > 0) {
return true;
}
}
return false;
}
