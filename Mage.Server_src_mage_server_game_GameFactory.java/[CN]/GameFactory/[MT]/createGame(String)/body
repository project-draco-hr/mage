{
  Game game;
  Constructor<?> con;
  try {
    con=gameTypes.get(gameType).getConstructor(new Class[]{});
    game=(Game)con.newInstance(new Object[]{});
  }
 catch (  Exception ex) {
    logger.log(Level.SEVERE,null,ex);
    return null;
  }
  logger.info("Game created: " + game.getId().toString());
  return game;
}
