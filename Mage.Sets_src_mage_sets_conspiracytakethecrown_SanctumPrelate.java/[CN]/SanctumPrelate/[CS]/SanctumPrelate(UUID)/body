  this.toughness=new MageInt(2);
  this.addAbility(new AsEntersBattlefieldAbility(new ChooseNumberEffect()));
  this.addAbility(new SimpleStaticAbility(Zone.BATTLEFIELD,new SanctumPrelateReplacementEffect()));
}
