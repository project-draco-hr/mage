{
  super(ownerId,23,"Sanctum Prelate",Rarity.MYTHIC,new CardType[]{CardType.CREATURE},"{1}{W}{W}");
  this.expansionSetCode="CN2";
  this.subtype.add("Human");
  this.subtype.add("Cleric");
