{
  super(ownerId,setInfo,new CardType[]{CardType.ENCHANTMENT},"{1}{U}");
  this.addAbility(new EntersBattlefieldAllTriggeredAbility(Zone.BATTLEFIELD,new ReturnToHandChosenPermanentEffect(RETURN_FILTER),ENTERS_BATTLEFIELD_FILTER,false,SetTargetPointer.PLAYER,"Whenever a player puts a nontoken creature onto the battlefield," + " that player returns a land he or she controls to its owner's hand."));
}
