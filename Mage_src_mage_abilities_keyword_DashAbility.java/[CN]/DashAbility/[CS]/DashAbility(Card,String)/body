{
  super(Zone.ALL,null);
  name=KEYWORD;
  this.addDashCost(manaString);
  Ability ability=new EntersBattlefieldAbility(new GainAbilitySourceEffect(HasteAbility.getInstance(),Duration.Custom,false),DashedCondition.getInstance(),false,"","");
  ability.addEffect(new CreateDelayedTriggeredAbilityEffect(new AtTheBeginOfNextEndStepDelayedTriggeredAbility(new ReturnToHandSourceEffect(true)),false));
  card.addAbility(ability);
}
