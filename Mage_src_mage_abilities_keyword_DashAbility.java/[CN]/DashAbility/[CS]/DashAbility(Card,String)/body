{
  super(Zone.ALL,null);
  name=KEYWORD;
  this.addDashCost(manaString);
  Ability ability=new EntersBattlefieldAbility(new GainAbilitySourceEffect(HasteAbility.getInstance(),Duration.Custom,false),DashedCondition.getInstance(),false,"","");
  Effect effect=new ReturnToHandTargetEffect();
  effect.setTargetPointer(new FixedTarget(card.getId()));
  ability.addEffect(new CreateDelayedTriggeredAbilityEffect(new AtTheBeginOfNextEndStepDelayedTriggeredAbility(effect),false));
  card.addAbility(ability);
}
