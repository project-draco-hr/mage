{
switch (roundingPolicy) {
case DO_NOTHING:
    break;
case FIRST:
  adjustFirst(relativeSpace,spaceRemaining);
break;
case LAST:
adjustLast(relativeSpace,spaceRemaining);
break;
case LARGEST:
adjustLargest(relativeSpace,spaceRemaining);
break;
case EQUAL:
adjustEqual(relativeSpace,spaceRemaining);
break;
default :
adjustLargest(relativeSpace,spaceRemaining);
}
}
