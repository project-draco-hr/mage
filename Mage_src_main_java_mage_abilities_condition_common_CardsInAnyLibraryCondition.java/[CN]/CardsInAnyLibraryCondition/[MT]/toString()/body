{
  StringBuilder sb=new StringBuilder("a library has ");
switch (type) {
case GreaterThan:
    sb.append(value + 1).append(" or more cards in it ");
  break;
case Equal:
sb.append(value).append(" cards in it ");
break;
case LessThan:
sb.append(value - 1).append(" or fewer cards in it ");
break;
}
return sb.toString();
}
