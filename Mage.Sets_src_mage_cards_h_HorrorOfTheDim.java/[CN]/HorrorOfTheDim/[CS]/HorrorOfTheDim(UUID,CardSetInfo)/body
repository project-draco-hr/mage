{
  super(ownerId,setInfo,new CardType[]{CardType.CREATURE},"{4}{B}");
  this.subtype.add("Horror");
  this.power=new MageInt(3);
  this.toughness=new MageInt(4);
  this.addAbility(new SimpleActivatedAbility(Zone.BATTLEFIELD,new GainAbilitySourceEffect(HexproofAbility.getInstance(),Duration.EndOfTurn),new ColoredManaCost(ColoredManaSymbol.U)));
}
