{
  super(ownerId,134,"Pheres-Band Tromper",Rarity.COMMON,new CardType[]{CardType.CREATURE},"{3}{G}");
  this.expansionSetCode="BNG";
  this.subtype.add("Centaur");
  this.subtype.add("Warrior");
