{
  if (gameSessions.containsKey(playerId)) {
    if (cards != null)     gameSessions.get(playerId).target(question,new CardsView(cards.getCards(game)),targets,required,getGameView(playerId),options);
 else     gameSessions.get(playerId).target(question,new CardsView(),targets,required,getGameView(playerId),options);
  }
  informOthers(playerId);
}
