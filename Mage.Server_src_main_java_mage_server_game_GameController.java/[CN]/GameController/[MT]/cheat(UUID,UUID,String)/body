{
  String clazz=Sets.findCard(cardName);
  if (clazz != null) {
    Card card=CardImpl.createCard(clazz);
    Set<Card> cards=new HashSet<Card>();
    cards.add(card);
    game.loadCards(cards,playerId);
    card.moveToZone(Zone.HAND,null,game,false);
    return true;
  }
 else {
    return false;
  }
}
