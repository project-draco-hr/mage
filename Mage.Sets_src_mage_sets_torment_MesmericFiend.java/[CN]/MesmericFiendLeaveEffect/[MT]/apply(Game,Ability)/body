{
  Player controller=game.getPlayer(source.getControllerId());
  if (controller != null) {
    ExileZone exZone=game.getExile().getExileZone(CardUtil.getCardExileZoneId(game,source.getSourceId(),true));
    if (exZone != null) {
      for (      Card card : exZone.getCards(game)) {
        if (card != null) {
          controller.moveCardToHandWithInfo(card,source.getSourceId(),game,Zone.EXILED);
        }
      }
      return true;
    }
  }
  return false;
}
