{
  super(ownerId,54,"Meletis Charlatan",Rarity.RARE,new CardType[]{CardType.CREATURE},"{2}{U}");
  this.expansionSetCode="THS";
  this.subtype.add("Human");
  this.subtype.add("Wizard");
