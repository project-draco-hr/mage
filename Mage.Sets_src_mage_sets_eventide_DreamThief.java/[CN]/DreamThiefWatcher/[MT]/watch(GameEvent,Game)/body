{
  if (condition == true) {
    return;
  }
  if (event.getType() == EventType.SPELL_CAST && controllerId == event.getPlayerId()) {
    Spell spell=game.getStack().getSpell(event.getTargetId());
    if (!spell.getSourceId().equals(cardId) && filter.match(spell,game)) {
      condition=true;
    }
  }
}
