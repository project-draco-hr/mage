{
  if (event.getType() == GameEvent.EventType.LAND_PLAYED) {
    Permanent land=game.getPermanent(event.getTargetId());
    if (land.getCardType().contains(CardType.LAND) && land.getControllerId().equals(this.controllerId) && event.getTargetId() != this.getSourceId()) {
      return true;
    }
  }
  return false;
}
