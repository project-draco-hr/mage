{
  if (!currentStep.skipStep(game,activePlayerId)) {
    prePriority(game,activePlayerId);
    if (!game.isPaused() && !game.isGameOver()) {
      currentStep.priority(game,activePlayerId,false);
    }
    if (!game.isPaused() && !game.isGameOver()) {
      postPriority(game,activePlayerId);
    }
  }
}
