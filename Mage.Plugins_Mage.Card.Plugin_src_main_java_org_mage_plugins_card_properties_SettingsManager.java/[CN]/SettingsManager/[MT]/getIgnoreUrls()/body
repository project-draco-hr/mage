{
  HashSet<String> ignoreUrls=new HashSet<String>();
  if (imageUrlProperties != null) {
    String result=imageUrlProperties.getProperty("ignore.urls");
    if (result != null) {
      String[] ignore=result.split(",");
      for (      String i : ignore) {
        ignoreUrls.add(i);
      }
    }
  }
  return ignoreUrls;
}
