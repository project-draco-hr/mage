{
  super(ownerId,145,"Sasaya, Orochi Ascendant",Rarity.RARE,new CardType[]{CardType.CREATURE},"{1}{G}{G}");
  this.expansionSetCode="SOK";
  this.supertype.add("Legendary");
  this.subtype.add("Snake");
