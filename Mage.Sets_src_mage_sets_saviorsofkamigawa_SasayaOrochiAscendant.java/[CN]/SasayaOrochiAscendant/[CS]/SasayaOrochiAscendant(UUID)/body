  this.toughness=new MageInt(3);
  this.flipCard=true;
  this.flipCardName="Sasaya's Essence";
  this.addAbility(new SimpleActivatedAbility(Zone.BATTLEFIELD,new SasayaOrochiAscendantFlipEffect(),new RevealHandSourceControllerCost()));
}
