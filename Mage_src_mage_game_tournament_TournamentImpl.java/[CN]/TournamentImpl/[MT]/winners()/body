{
  for (  TournamentPlayer winner : this.getActivePlayers()) {
    winner.setState(TournamentPlayerState.FINISHED);
    if (options.getNumberRounds() == 0) {
      if (isAbort()) {
        winner.setStateInfo("Tournament canceled");
      }
 else {
        winner.setStateInfo("Winner");
      }
    }
  }
}
