{
  Player controller=game.getPlayer(source.getControllerId());
  if (controller != null) {
    Card card=game.getCard(event.getTargetId());
    if (card != null) {
      return controller.moveCards(card,Zone.EXILED,source,game,false,false,false,event.getAppliedEffects());
    }
  }
  return false;
}
