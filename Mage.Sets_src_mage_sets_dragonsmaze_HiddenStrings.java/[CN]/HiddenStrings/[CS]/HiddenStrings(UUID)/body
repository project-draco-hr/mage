{
  super(ownerId,12,"Hidden Strings",Rarity.COMMON,new CardType[]{CardType.SORCERY},"{1}{U}");
  this.expansionSetCode="DGM";
  this.color.setBlue(true);
  this.getSpellAbility().addEffect(new HiddenStringsEffect());
