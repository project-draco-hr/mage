{
  super(ownerId,setInfo,new CardType[]{CardType.CREATURE},"{2}{U}{R}{W}");
  this.subtype.add("Djinn");
  this.subtype.add("Wizard");
  this.power=new MageInt(3);
  this.toughness=new MageInt(4);
  this.addAbility(FlyingAbility.getInstance());
  this.addAbility(new SpellCastControllerTriggeredAbility(new GainAbilityControlledEffect(LifelinkAbility.getInstance(),Duration.EndOfTurn,new FilterCreaturePermanent(),false),filterNonCreature,false));
}
