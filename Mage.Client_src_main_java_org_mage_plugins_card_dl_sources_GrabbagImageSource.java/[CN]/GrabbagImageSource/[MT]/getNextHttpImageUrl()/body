{
  if (copyUrlToImage == null) {
    setupLinks();
  }
  for (  String key : copyUrlToImageDone.keySet()) {
    if (copyUrlToImageDone.get(key) < maxTimes) {
      copyUrlToImageDone.put(key,maxTimes);
      doPause(key);
      return getSourceName(key) + key;
    }
  }
  if (maxTimes < 2) {
    maxTimes++;
  }
  for (  String key : copyUrlToImageDone.keySet()) {
    if (copyUrlToImageDone.get(key) < maxTimes) {
      copyUrlToImageDone.put(key,maxTimes);
      doPause(key);
      return getSourceName(key) + key;
    }
  }
  return null;
}
