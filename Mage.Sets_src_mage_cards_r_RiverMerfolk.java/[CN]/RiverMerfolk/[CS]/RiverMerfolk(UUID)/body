{
  super(ownerId,47,"River Merfolk",Rarity.COMMON,new CardType[]{CardType.CREATURE},"{U}{U}");
  this.expansionSetCode="MED";
  this.subtype.add("Merfolk");
  this.power=new MageInt(2);
  this.toughness=new MageInt(1);
  this.addAbility(new SimpleActivatedAbility(Zone.BATTLEFIELD,new GainAbilitySourceEffect(new MountainwalkAbility(false),Duration.EndOfTurn),new ManaCostsImpl("{U}")));
}
