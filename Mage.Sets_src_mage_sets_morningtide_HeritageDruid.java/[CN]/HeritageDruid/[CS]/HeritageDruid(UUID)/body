{
  super(ownerId,126,"Heritage Druid",Rarity.UNCOMMON,new CardType[]{CardType.CREATURE},"{G}");
  this.expansionSetCode="MOR";
  this.subtype.add("Elf");
  this.subtype.add("Druid");
