  this.toughness=new MageInt(1);
  this.addAbility(new SimpleManaAbility(Constants.Zone.BATTLEFIELD,new Mana(0,3,0,0,0,0,0),new TapTargetCost(new TargetControlledCreaturePermanent(3,3,filter,false))));
}
