{
  String[] symbols=new String[]{"0","1","10","11","12","15","16","2","3","4","5","6","7","8","9","B","BG","BR","G","GU","GW","R","RG","RW","S","T","U","UB","UR","W","WB","WU","X","Y","Z","slash"};
  for (  String symbol : symbols)   manaImages.put(symbol,UI.getImageIcon(Constants.RESOURCE_PATH_MANA + "/" + symbol+ ".png").getImage());
}
