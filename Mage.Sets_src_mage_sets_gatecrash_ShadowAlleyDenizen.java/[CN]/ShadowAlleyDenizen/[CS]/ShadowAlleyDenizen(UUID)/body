{
  super(ownerId,76,"Shadow Alley Denizen",Rarity.COMMON,new CardType[]{CardType.CREATURE},"{B}");
  this.expansionSetCode="GTC";
  this.subtype.add("Vampire");
  this.subtype.add("Rogue");
  this.color.setBlack(true);
  this.power=new MageInt(1);
  this.toughness=new MageInt(1);
  Ability ability=new EntersBattlefieldAllTriggeredAbility(Zone.BATTLEFIELD,new GainAbilityTargetEffect(IntimidateAbility.getInstance(),Constants.Duration.EndOfTurn),filter,false);
  ability.addTarget(new TargetCreaturePermanent(true));
  this.addAbility(ability);
}
