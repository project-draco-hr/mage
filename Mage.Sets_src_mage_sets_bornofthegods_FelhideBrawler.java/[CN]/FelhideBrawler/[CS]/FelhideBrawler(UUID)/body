{
  super(ownerId,70,"Felhide Brawler",Rarity.COMMON,new CardType[]{CardType.CREATURE},"{1}{B}");
  this.expansionSetCode="BNG";
  this.subtype.add("Minotaur");
  this.power=new MageInt(2);
  this.toughness=new MageInt(2);
