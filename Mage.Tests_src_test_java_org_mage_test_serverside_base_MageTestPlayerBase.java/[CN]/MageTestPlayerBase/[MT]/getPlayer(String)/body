{
  if (name.equals("ComputerA")) {
    return playerA;
  }
 else   if (name.equals("ComputerB")) {
    return playerB;
  }
 else   if (name.equals("ComputerC")) {
    return playerC;
  }
 else   if (name.equals("ComputerD")) {
    return playerD;
  }
  throw new IllegalArgumentException("Couldn't find player for name=" + name);
}
