{
  returnVal=null;
  logger.debug("Calling: " + name);
  try {
    returnVal=super.makeCall();
  }
 catch (  ServerUnavailable e) {
    this.exception=true;
  }
catch (  MageException e) {
    this.exception=true;
    this.ex=e;
  }
 finally {
    ServerCache.noLongerUsingServer(connection);
  }
  return returnVal;
}
