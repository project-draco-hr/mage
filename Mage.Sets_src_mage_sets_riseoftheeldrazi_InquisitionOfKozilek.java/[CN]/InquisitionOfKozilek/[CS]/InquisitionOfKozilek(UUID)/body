{
  super(ownerId,115,"Inquisition of Kozilek",Rarity.UNCOMMON,new CardType[]{CardType.SORCERY},"{B}");
  this.expansionSetCode="ROE";
  this.color.setBlack(true);
  this.getSpellAbility().addTarget(new TargetPlayer(true));
