{
  Player controller=game.getPlayer(source.getControllerId());
  if (controller == null) {
    return false;
  }
  Permanent permanent=game.getPermanent(this.getTargetPointer().getFirst(game,source));
  if (permanent != null) {
    controller.moveCards(permanent,null,Zone.HAND,source,game);
  }
  return true;
}
