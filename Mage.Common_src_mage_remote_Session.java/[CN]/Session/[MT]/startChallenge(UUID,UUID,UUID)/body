{
  try {
    if (sessionState == SessionState.CONNECTED) {
      server.startChallenge(sessionId,roomId,tableId,challengeId);
      return true;
    }
  }
 catch (  RemoteException ex) {
    handleRemoteException(ex);
  }
catch (  MageException ex) {
    handleMageException(ex);
  }
  return false;
}
