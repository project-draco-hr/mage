{
  try {
    if (sessionState == SessionState.CONNECTED)     return server.joinTable(sessionId,roomId,tableId,playerName,playerType,skill,deckList);
  }
 catch (  RemoteException ex) {
    handleRemoteException(ex);
  }
catch (  GameException ex) {
    handleGameException(ex);
  }
catch (  MageException ex) {
    handleMageException(ex);
  }
  return false;
}
