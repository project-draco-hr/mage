{
  super(ownerId,setInfo,new CardType[]{CardType.ARTIFACT,CardType.CREATURE},"{1}{U}");
  this.subtype.add("Vedalken");
  this.subtype.add("Artificer");
  this.power=new MageInt(1);
  this.toughness=new MageInt(2);
