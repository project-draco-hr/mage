{
  if ((event.getType() == GameEvent.EventType.CAST_SPELL) && event.getPlayerId() == source.getControllerId()) {
    MageObject spellObject=game.getObject(event.getSourceId());
    if (spellObject != null) {
      return spellObject.getCardType().contains(CardType.CREATURE);
    }
  }
  return false;
}
