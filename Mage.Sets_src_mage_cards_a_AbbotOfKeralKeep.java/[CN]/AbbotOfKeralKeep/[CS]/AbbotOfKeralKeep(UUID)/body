{
  super(ownerId,127,"Abbot of Keral Keep",Rarity.RARE,new CardType[]{CardType.CREATURE},"{1}{R}");
  this.expansionSetCode="ORI";
  this.subtype.add("Human");
  this.subtype.add("Monk");
