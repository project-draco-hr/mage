{
  super(ownerId,17,"Peace and Quiet",Rarity.UNCOMMON,new CardType[]{CardType.INSTANT},"{1}{W}");
  this.expansionSetCode="ULG";
  this.color.setWhite(true);
  this.getSpellAbility().addEffect(new DestroyTargetEffect());
