{
  JSONParser parser=new JSONParser();
  parser.parseJSON("{}");
  parser.parseJSON("{\"test\" : 1}");
  parser.parseJSON("{\"test\" : \"test\"}");
  parser.parseJSON("{\"list\" : [\"1\", \"2\", \"3\"]}");
  parser.parseJSON("{test:test}");
  testError(parser,"{");
  testError(parser,"}");
  testError(parser,"{{}");
  testError(parser,"{\"test\" : [}}");
}
