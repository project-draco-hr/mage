{
  super("Faerie Rogue","1/1 blue and black Faerie Rogue creature token with flying");
  cardType.add(CardType.CREATURE);
  color.setBlue(true);
  color.setBlack(true);
  subtype.add("Faerie");
  subtype.add("Rogue");
  power=new MageInt(1);
  toughness=new MageInt(1);
  this.addAbility(FlyingAbility.getInstance());
}
