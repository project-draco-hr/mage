{
  super(ownerId,21,"Fetid Horror",Rarity.COMMON,new CardType[]{CardType.CREATURE},"{3}{B}");
  this.expansionSetCode="MIR";
  this.subtype.add("Shade");
  this.subtype.add("Horror");
