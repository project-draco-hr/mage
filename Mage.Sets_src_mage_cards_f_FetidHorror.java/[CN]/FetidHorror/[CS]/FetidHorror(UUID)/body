  this.toughness=new MageInt(2);
  this.addAbility(new SimpleActivatedAbility(Zone.BATTLEFIELD,new BoostSourceEffect(1,1,Duration.EndOfTurn),new ColoredManaCost(ColoredManaSymbol.B)));
}
