{
  super(ownerId,33,"Covert Operative",Rarity.COMMON,new CardType[]{CardType.CREATURE},"{4}{U}");
  this.expansionSetCode="LGN";
  this.subtype.add("Human");
  this.subtype.add("Wizard");
