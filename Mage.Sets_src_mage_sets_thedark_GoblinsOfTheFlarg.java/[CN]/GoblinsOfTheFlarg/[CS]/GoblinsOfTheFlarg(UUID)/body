{
  super(ownerId,69,"Goblins of the Flarg",Rarity.COMMON,new CardType[]{CardType.CREATURE},"{R}");
  this.expansionSetCode="DRK";
  this.subtype.add("Goblin");
  this.subtype.add("Warrior");
  this.power=new MageInt(1);
