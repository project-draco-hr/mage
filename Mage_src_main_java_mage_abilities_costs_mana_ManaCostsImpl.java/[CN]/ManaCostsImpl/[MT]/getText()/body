{
  if (text != null) {
    return text;
  }
  if (this.size() == 0) {
    return "";
  }
  StringBuilder sbText=new StringBuilder();
  for (  ManaCost cost : this) {
    if (cost instanceof GenericManaCost) {
      sbText.append(cost.getText());
    }
  }
  for (  ManaCost cost : this) {
    if (!(cost instanceof GenericManaCost)) {
      sbText.append(cost.getText());
    }
  }
  return sbText.toString();
}
