{
  super(ownerId,setInfo,new CardType[]{CardType.ENCHANTMENT},"{2}{G}");
  this.addAbility(new SimpleStaticAbility(Zone.BATTLEFIELD,new GainAbilityControlledEffect(new BecomesBlockedByCreatureTriggeredAbility(new BoostSourceEffect(1,1,Duration.EndOfTurn),false),Duration.WhileOnBattlefield,new FilterCreaturePermanent("Creatures"))));
}
