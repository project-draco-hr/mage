{
  if (event.getType() == GameEvent.EventType.DAMAGED_CREATURE && event.getTargetId().equals(targetObject.getSourceId()) && targetObject.getPermanentOrLKIBattlefield(game) != null) {
    for (    Effect effect : this.getEffects()) {
      effect.setValue("damage",event.getAmount());
    }
    return true;
  }
  return false;
}
