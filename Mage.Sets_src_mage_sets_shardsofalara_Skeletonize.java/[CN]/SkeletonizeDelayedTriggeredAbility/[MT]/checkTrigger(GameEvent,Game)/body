{
  ZoneChangeEvent zce=(ZoneChangeEvent)event;
  if (zce.isDiesEvent()) {
    DamagedByWatcher watcher=(DamagedByWatcher)game.getState().getWatchers().get("DamagedByWatcher",this.getSourceId());
    if (watcher != null) {
      return watcher.wasDamaged(zce.getTarget(),game);
    }
  }
  return false;
}
