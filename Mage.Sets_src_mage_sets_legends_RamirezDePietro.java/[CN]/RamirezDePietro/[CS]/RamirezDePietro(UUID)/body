{
  super(ownerId,291,"Ramirez DePietro",Rarity.UNCOMMON,new CardType[]{CardType.CREATURE},"{3}{U}{B}{B}");
  this.expansionSetCode="LEG";
  this.supertype.add("Legendary");
  this.subtype.add("Human");
