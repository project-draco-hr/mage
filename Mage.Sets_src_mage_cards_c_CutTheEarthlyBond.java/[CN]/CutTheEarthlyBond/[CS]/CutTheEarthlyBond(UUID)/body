{
  super(ownerId,32,"Cut the Earthly Bond",Rarity.COMMON,new CardType[]{CardType.INSTANT},"{U}");
  this.expansionSetCode="SOK";
  this.subtype.add("Arcane");
  this.getSpellAbility().addEffect(new ReturnToHandTargetEffect());
