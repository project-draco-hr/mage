{
  super("Kenzo the Hardhearted","");
  supertype.add("Legendary");
  cardType.add(CardType.CREATURE);
  color.setWhite(true);
  subtype.add("Human");
  subtype.add("Samurai");
  power=new MageInt(3);
  toughness=new MageInt(4);
  this.addAbility(DoubleStrikeAbility.getInstance());
  this.addAbility(new BushidoAbility(2));
}
