{
  super(ownerId,setInfo,new CardType[]{CardType.ENCHANTMENT},"{W}");
  this.supertype.add("Snow");
  this.subtype.add("Aura");
  TargetPermanent auraTarget=new TargetCreaturePermanent();
  this.getSpellAbility().addTarget(auraTarget);
  this.getSpellAbility().addEffect(new AttachEffect(Outcome.Detriment));
  Ability ability=new EnchantAbility(auraTarget.getTargetName());
  this.addAbility(ability);
  this.addAbility(new SimpleStaticAbility(Zone.BATTLEFIELD,new CantBlockActivateAttachedEffect()));
  this.addAbility(new SimpleActivatedAbility(Zone.BATTLEFIELD,new GainAbilityAttachedEffect(DefenderAbility.getInstance(),AttachmentType.AURA,Duration.EndOfTurn),new ManaCostsImpl("{S}")));
}
