{
  super(ownerId,setInfo,new CardType[]{CardType.ENCHANTMENT},"{1}{W}");
  this.addAbility(new PutIntoGraveFromBattlefieldAllTriggeredAbility(new DoIfCostPaid(new ReturnToBattlefieldUnderOwnerControlTargetEffect(),new SacrificeSourceCost()),false,new FilterCreaturePermanent("a creature"),true,true));
}
