{
  super(ownerId,setInfo,new CardType[]{CardType.CREATURE},"");
  this.subtype.add("Eldrazi");
  this.subtype.add("Fish");
  this.power=new MageInt(4);
  this.toughness=new MageInt(5);
  this.nightCard=true;
