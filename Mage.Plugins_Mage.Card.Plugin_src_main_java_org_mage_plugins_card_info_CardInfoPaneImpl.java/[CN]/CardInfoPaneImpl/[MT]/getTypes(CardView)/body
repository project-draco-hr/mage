{
  String types="";
  for (  String superType : card.getSuperTypes()) {
    types+=superType + " ";
  }
  for (  Constants.CardType cardType : card.getCardTypes()) {
    types+=cardType.toString() + " ";
  }
  if (card.getSubTypes().size() > 0) {
    types+="- ";
  }
  for (  String subType : card.getSubTypes()) {
    types+=subType + " ";
  }
  return types.trim();
}
