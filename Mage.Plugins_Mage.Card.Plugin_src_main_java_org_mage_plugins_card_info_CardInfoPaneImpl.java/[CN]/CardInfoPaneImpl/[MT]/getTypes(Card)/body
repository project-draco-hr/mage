{
  String types="";
  for (  String superType : card.getSupertype()) {
    types+=superType + " ";
  }
  for (  Constants.CardType cardType : card.getCardType()) {
    types+=cardType.toString() + " ";
  }
  if (card.getSubtype().size() > 0) {
    types+="- ";
  }
  for (  String subType : card.getSubtype()) {
    types+=subType + " ";
  }
  return types.trim();
}
