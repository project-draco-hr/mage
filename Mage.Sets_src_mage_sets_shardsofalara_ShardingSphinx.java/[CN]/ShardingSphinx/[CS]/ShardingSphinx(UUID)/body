{
  super(ownerId,55,"Sharding Sphinx",Rarity.RARE,new CardType[]{CardType.ARTIFACT,CardType.CREATURE},"{4}{U}{U}");
  this.expansionSetCode="ALA";
  this.subtype.add("Sphinx");
  this.power=new MageInt(4);
  this.toughness=new MageInt(4);
  this.addAbility(FlyingAbility.getInstance());
  this.addAbility(new DealsDamageToAPlayerAllTriggeredAbility(new CreateTokenEffect(new ThopterToken()),filter,true,SetTargetPointer.NONE,true));
}
