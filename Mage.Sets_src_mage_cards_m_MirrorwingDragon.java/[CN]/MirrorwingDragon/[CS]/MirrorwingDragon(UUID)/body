{
  super(ownerId,136,"Mirrorwing Dragon",Rarity.MYTHIC,new CardType[]{CardType.CREATURE},"{3}{R}{R}");
  this.expansionSetCode="EMN";
  this.subtype.add("Dragon");
  this.power=new MageInt(4);
