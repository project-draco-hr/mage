{
  super(ownerId,51,"Stonybrook Banneret",Rarity.COMMON,new CardType[]{CardType.CREATURE},"{1}{U}");
  this.expansionSetCode="MOR";
  this.subtype.add("Merfolk");
  this.subtype.add("Wizard");
