{
  Permanent creature=game.getPermanent(source.getFirstTarget());
  if (creature != null) {
    if (creature.hasSubtype("Zombie")) {
      creature.tap(game);
      creature.moveToExile(source.getSourceId(),creature.getName(),source.getSourceId(),game);
    }
 else {
      creature.tap(game);
    }
    return true;
  }
  return false;
}
