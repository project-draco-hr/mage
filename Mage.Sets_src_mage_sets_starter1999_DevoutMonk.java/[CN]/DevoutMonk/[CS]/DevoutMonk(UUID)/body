{
  super(ownerId,14,"Devout Monk",Rarity.COMMON,new CardType[]{CardType.CREATURE},"{W}");
  this.expansionSetCode="S99";
  this.subtype.add("Human");
  this.subtype.add("Monk");
  this.subtype.add("Cleric");
