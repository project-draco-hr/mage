{
  addCard(Zone.BATTLEFIELD,playerA,"Homicidal Seclusion",2);
  addCard(Zone.BATTLEFIELD,playerA,"Horned Turtle",1);
  setStopAt(1,PhaseStep.BEGIN_COMBAT);
  execute();
