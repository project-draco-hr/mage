{
  ZoneChangeEvent zEvent=(ZoneChangeEvent)event;
  Permanent permanent=zEvent.getTarget();
  if (permanent != null && permanent.getControllerId().equals(this.controllerId) && zEvent.getToZone() == Zone.GRAVEYARD && zEvent.getFromZone() == Zone.BATTLEFIELD && !(permanent instanceof PermanentToken) && permanent.getCardType().contains(CardType.CREATURE)) {
    getEffects().get(0).setTargetPointer(new FixedTarget(permanent.getId()));
    return true;
  }
  return false;
}
