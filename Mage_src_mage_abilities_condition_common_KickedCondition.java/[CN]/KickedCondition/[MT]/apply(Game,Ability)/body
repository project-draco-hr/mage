{
  Card p=game.getCard(source.getSourceId());
  boolean kicked=false;
  if (p != null) {
    for (    Object cost : p.getSpellAbility().getOptionalCosts()) {
      if (cost instanceof KickerManaCost) {
        if (((KickerManaCost)cost).isPaid()) {
          kicked=true;
        }
      }
    }
  }
  return kicked;
}
