{
  if (table.getState() == TableState.SIDEBOARDING) {
    match.updateDeck(playerId,deck);
  }
 else {
    if (tournament != null) {
      TournamentManager.getInstance().updateDeck(tournament.getId(),playerId,deck);
    }
 else {
      logger.fatal("Tournament == null  table: " + table.getId());
    }
  }
}
