{
  table.addTableEventListener(new Listener<TableEvent>(){
    @Override public void event(    TableEvent event){
switch (event.getEventType()) {
case SIDEBOARD:
        sideboard(event.getPlayerId(),event.getDeck());
      break;
case SUBMIT_DECK:
    submitDeck(event.getPlayerId(),event.getDeck());
  break;
}
}
}
);
}
