{
  LOGGER.info("End drag (" + target + ")");
  if (target == this) {
  }
 else {
    for (    ArrayList<ArrayList<CardView>> gridRow : cardGrid) {
      for (      ArrayList<CardView> stack : gridRow) {
        for (int i=0; i < stack.size(); ++i) {
          CardView view=stack.get(i);
          if (view.isSelected()) {
            stack.set(i,null);
          }
        }
      }
    }
    trimGrid();
    layoutGrid();
    cardScroll.repaint();
  }
}
