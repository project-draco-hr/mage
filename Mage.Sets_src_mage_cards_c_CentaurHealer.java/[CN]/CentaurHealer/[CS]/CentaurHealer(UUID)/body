{
  super(ownerId,148,"Centaur Healer",Rarity.COMMON,new CardType[]{CardType.CREATURE},"{1}{G}{W}");
  this.expansionSetCode="RTR";
  this.subtype.add("Centaur");
  this.subtype.add("Cleric");
