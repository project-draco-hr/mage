{
  super(ownerId,329,"Subterranean Hangar",Rarity.UNCOMMON,new CardType[]{CardType.LAND},"");
  this.expansionSetCode="MMQ";
  this.addAbility(new EntersBattlefieldTappedAbility());
  this.addAbility(new SimpleActivatedAbility(Zone.BATTLEFIELD,new AddCountersSourceEffect(CounterType.STORAGE.createInstance()),new TapSourceCost()));
  Ability ability=new DynamicManaAbility(Mana.BlackMana(1),new RemovedCountersForCostValue(),new TapSourceCost(),"Add {B} to your mana pool for each storage counter removed this way",true,new CountersSourceCount(CounterType.STORAGE));
  ability.addCost(new RemoveVariableCountersSourceCost(CounterType.STORAGE.createInstance(),"Remove any number of storage counters from {this}"));
  this.addAbility(ability);
}
