{
  super(ownerId,173,"Hindering Light",Rarity.COMMON,new CardType[]{CardType.INSTANT},"{W}{U}");
  this.expansionSetCode="ALA";
  this.getSpellAbility().addEffect(new CounterTargetEffect());
