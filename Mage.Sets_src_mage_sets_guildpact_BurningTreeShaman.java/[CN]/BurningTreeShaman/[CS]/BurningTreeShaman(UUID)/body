{
  super(ownerId,105,"Burning-Tree Shaman",Rarity.RARE,new CardType[]{CardType.CREATURE},"{1}{R}{G}");
  this.expansionSetCode="GPT";
  this.subtype.add("Centaur");
  this.subtype.add("Shaman");
