{
  super(ownerId,141,"Well of Life",Rarity.UNCOMMON,new CardType[]{CardType.ARTIFACT},"{4}");
  this.expansionSetCode="PCY";
  this.addAbility(new ConditionalTriggeredAbility(new BeginningOfEndStepTriggeredAbility(new GainLifeEffect(2),TargetController.YOU,false),new InvertCondition(new PermanentsOnTheBattlefieldCondition(filter)),"At the beginning of your end step, if you control no untapped lands, you gain 2 life."));
}
