{
  for (  StackObject stackObject : game.getStack()) {
    if (stackObject.getStackAbility() != null && stackObject.getStackAbility().getAbilityType().equals(AbilityType.ACTIVATED) && game.getPlayer(sourceControllerId).getInRange().contains(stackObject.getStackAbility().getControllerId())) {
      return true;
    }
  }
  return false;
}
