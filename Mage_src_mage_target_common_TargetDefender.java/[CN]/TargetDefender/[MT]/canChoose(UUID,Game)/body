{
  int count=0;
  for (  Player player : game.getPlayers().values()) {
    if (player.canTarget(game.getObject(this.source.getSourceId())) && filter.match(player))     count++;
  }
  if (count >= this.minNumberOfTargets)   return true;
  for (  Permanent permanent : game.getBattlefield().getActivePermanents(CardType.PLANESWALKER)) {
    if (permanent.canTarget(game.getObject(this.source.getSourceId())) && filter.match(permanent))     count++;
  }
  return count >= this.minNumberOfTargets;
}
