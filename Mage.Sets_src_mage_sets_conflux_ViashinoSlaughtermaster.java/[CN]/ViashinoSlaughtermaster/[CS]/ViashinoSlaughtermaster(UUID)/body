{
  super(ownerId,73,"Viashino Slaughtermaster",Rarity.UNCOMMON,new CardType[]{CardType.CREATURE},"{1}{R}");
  this.expansionSetCode="CON";
  this.subtype.add("Viashino");
  this.subtype.add("Warrior");
  this.color.setRed(true);
  this.power=new MageInt(1);
  this.toughness=new MageInt(1);
  this.addAbility(DoubleStrikeAbility.getInstance());
  this.addAbility(new ActivateOncePerTurnActivatedAbility(Constants.Zone.BATTLEFIELD,new BoostSourceEffect(1,1,Constants.Duration.EndOfTurn),new ManaCostsImpl("{B}{G}")));
}
