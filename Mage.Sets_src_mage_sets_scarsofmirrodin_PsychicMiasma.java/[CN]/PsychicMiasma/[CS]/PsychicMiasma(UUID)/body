{
  super(ownerId,76,"Psychic Miasma",Rarity.COMMON,new CardType[]{CardType.SORCERY},"{1}{B}");
  this.expansionSetCode="SOM";
  this.color.setBlack(true);
  this.getSpellAbility().addTarget(new TargetPlayer(true));
