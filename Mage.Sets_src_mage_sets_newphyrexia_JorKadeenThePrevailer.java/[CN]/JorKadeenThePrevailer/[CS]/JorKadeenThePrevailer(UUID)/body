{
  super(ownerId,128,"Jor Kadeen, the Prevailer",Rarity.RARE,new CardType[]{CardType.CREATURE},"{3}{R}{W}");
  this.expansionSetCode="NPH";
  this.subtype.add("Legendary");
  this.subtype.add("Human");
  this.subtype.add("Warrior");
  this.color.setRed(true);
  this.color.setWhite(true);
  this.power=new MageInt(5);
  this.toughness=new MageInt(4);
  this.addAbility(FirstStrikeAbility.getInstance());
  ConditionalContinousEffect effect=new ConditionalContinousEffect(new BoostControlledEffect(3,0,Duration.WhileOnBattlefield,FilterCreaturePermanent.getDefault(),true),Metalcraft.getInstance(),effectText);
  this.addAbility(new SimpleStaticAbility(Zone.BATTLEFIELD,effect));
}
