{
  super(ownerId,116,"Ghostly Changeling",Rarity.UNCOMMON,new CardType[]{CardType.CREATURE},"{2}{B}");
  this.expansionSetCode="LRW";
  this.subtype.add("Shapeshifter");
  this.color.setBlack(true);
  this.power=new MageInt(2);
  this.toughness=new MageInt(2);
  this.addAbility(ChangelingAbility.getInstance());
  this.addAbility(new SimpleActivatedAbility(Constants.Zone.BATTLEFIELD,new BoostSourceEffect(1,1,Constants.Duration.EndOfTurn),new ManaCostsImpl("{1}{B}")));
}
