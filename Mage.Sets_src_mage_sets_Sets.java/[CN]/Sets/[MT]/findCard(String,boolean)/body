{
  if (!random) {
    return findCard(name);
  }
 else {
    List<String> cards=new ArrayList<String>();
    for (    ExpansionSet set : fINSTANCE.values()) {
      String cardName=set.findCard(name,true);
      if (cardName != null) {
        cards.add(cardName);
      }
    }
    if (cards.size() > 0) {
      return cards.get(rnd.nextInt(cards.size()));
    }
  }
  return null;
}
