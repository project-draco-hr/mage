{
  return blocker.getAbilities().containsKey(FlyingAbility.getInstance().getId()) || blocker.getAbilities().containsKey(ReachAbility.getInstance().getId()) || (game.getContinuousEffects().asThough(blocker.getId(),AsThoughEffectType.BLOCK_DRAGON,source,blocker.getControllerId(),game) && attacker.hasSubtype("Dragon"));
}
