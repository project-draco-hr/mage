{
  super(ownerId,155,"Virulent Sliver",Rarity.COMMON,new CardType[]{CardType.CREATURE},"{G}");
  this.expansionSetCode="FUT";
  this.subtype.add("Sliver");
  this.power=new MageInt(1);
  this.toughness=new MageInt(1);
  this.addAbility(new SimpleStaticAbility(Zone.BATTLEFIELD,new GainAbilityAllEffect(new PoisonousAbility(1),Duration.WhileOnBattlefield,filter,"All Sliver creatures have poisonous 1. <i>(Whenever a Sliver deals combat damage to a player, that player gets a poison counter.)</i>")));
}
