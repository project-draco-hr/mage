{
  super(ownerId,266,"Stonewing Antagonizer",Rarity.UNCOMMON,new CardType[]{CardType.ARTIFACT,CardType.CREATURE},"");
  this.expansionSetCode="SOI";
  this.subtype.add("Gargoyle");
