{
  super(ownerId,35,"Whirlpool Rider",Rarity.COMMON,new CardType[]{CardType.CREATURE},"{1}{U}");
  this.expansionSetCode="APC";
  this.subtype.add("Merfolk");
  this.power=new MageInt(1);
  this.toughness=new MageInt(1);
