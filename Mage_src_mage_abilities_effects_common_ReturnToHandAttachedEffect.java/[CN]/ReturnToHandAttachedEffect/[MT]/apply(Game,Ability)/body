{
  Object object=getValue("attachedTo");
  if (object != null && object instanceof Permanent) {
    Card card=game.getCard(((Permanent)object).getId());
    if (card != null) {
      if (card.moveToZone(Zone.HAND,source.getId(),game,false)) {
        return true;
      }
    }
  }
  return false;
}
