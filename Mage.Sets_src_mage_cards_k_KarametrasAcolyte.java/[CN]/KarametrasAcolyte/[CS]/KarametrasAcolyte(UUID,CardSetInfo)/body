{
  super(ownerId,setInfo,new CardType[]{CardType.CREATURE},"{3}{G}");
  this.subtype.add("Human");
  this.subtype.add("Druid");
  this.power=new MageInt(1);
  this.toughness=new MageInt(4);
  this.addAbility(new DynamicManaAbility(Mana.GreenMana(1),new DevotionCount(ColoredManaSymbol.G),"Add an amount of {G} to your mana pool equal to your devotion to green. (Each {G} in the mana costs of permanents you control counts towards your devotion to green.)"));
}
