{
  super(ownerId,94,"Sandbar Merfolk",Rarity.COMMON,new CardType[]{CardType.CREATURE},"{U}");
  this.expansionSetCode="USG";
  this.subtype.add("Merfolk");
  this.color.setBlue(true);
  this.power=new MageInt(1);
