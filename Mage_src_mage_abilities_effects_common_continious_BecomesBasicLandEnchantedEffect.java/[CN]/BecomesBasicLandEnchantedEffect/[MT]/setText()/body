{
  StringBuilder sb=new StringBuilder("Enchanted land is a ");
  int i=1;
  for (  String landType : landTypes) {
    if (i > 1) {
      if (i == landTypes.size()) {
        sb.append(" and ");
      }
 else {
        sb.append(", ");
      }
    }
    i++;
    sb.append(landType);
  }
  return sb.toString();
}
