{
  if (mode == DeckEditorMode.Sideboard || mode == DeckEditorMode.Limited)   this.setTitle("Deck Editor - " + tableId.toString());
 else {
    if (deck != null)     this.setTitle("Deck Editor - " + deck.getName());
 else     this.setTitle("Deck Editor");
  }
  this.deckEditorPanel1.showDeckEditor(mode,deck,tableId,time);
  this.repaint();
}
