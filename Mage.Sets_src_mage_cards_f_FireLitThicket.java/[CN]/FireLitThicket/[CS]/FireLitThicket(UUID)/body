  ability.addCost(new TapSourceCost());
  this.addAbility(ability);
  ability=new SimpleManaAbility(Zone.BATTLEFIELD,Mana.GreenMana(2),new ManaCostsImpl("{R/G}"));
  ability.addCost(new TapSourceCost());
  this.addAbility(ability);
}
