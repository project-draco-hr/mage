{
  for (  Component comp : pile1.getComponents()) {
    if (comp instanceof CardPanel) {
      ((CardPanel)comp).cleanUp();
      pile1.remove(comp);
    }
  }
  for (  Component comp : pile2.getComponents()) {
    if (comp instanceof CardPanel) {
      ((CardPanel)comp).cleanUp();
      pile2.remove(comp);
    }
  }
}
