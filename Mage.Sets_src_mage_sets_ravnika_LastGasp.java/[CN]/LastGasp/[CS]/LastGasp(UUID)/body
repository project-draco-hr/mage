{
  super(ownerId,93,"Last Gasp",Rarity.COMMON,new CardType[]{CardType.INSTANT},"{1}{B}");
  this.expansionSetCode="RAV";
  this.color.setBlack(true);
  this.getSpellAbility().addEffect(new BoostTargetEffect(-3,-3,Duration.EndOfTurn));
