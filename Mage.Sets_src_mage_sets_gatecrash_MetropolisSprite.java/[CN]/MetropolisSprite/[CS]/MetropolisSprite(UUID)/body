{
  super(ownerId,42,"Metropolis Sprite",Rarity.COMMON,new CardType[]{CardType.CREATURE},"{1}{U}");
  this.expansionSetCode="GTC";
  this.subtype.add("Faerie");
  this.subtype.add("Rogue");
  this.color.setBlue(true);
  this.power=new MageInt(1);
  this.toughness=new MageInt(2);
  this.addAbility(FlyingAbility.getInstance());
  this.addAbility(new SimpleActivatedAbility(Constants.Zone.BATTLEFIELD,new BoostSourceEffect(1,-1,Constants.Duration.EndOfTurn),new ManaCostsImpl("{U}")));
}
