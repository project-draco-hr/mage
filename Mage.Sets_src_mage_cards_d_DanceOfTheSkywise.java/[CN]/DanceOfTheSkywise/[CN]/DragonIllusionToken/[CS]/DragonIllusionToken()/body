{
  super("Dragon","blue Dragon Illusion with base power and toughness 4/4 and with flying");
  cardType.add(CardType.CREATURE);
  color.setBlue(true);
  subtype.add("Dragon");
  subtype.add("Illusion");
  power=new MageInt(4);
  toughness=new MageInt(4);
  this.addAbility(FlyingAbility.getInstance());
}
