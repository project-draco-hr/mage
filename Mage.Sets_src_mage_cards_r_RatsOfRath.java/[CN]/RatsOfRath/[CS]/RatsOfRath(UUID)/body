{
  super(ownerId,44,"Rats of Rath",Rarity.COMMON,new CardType[]{CardType.CREATURE},"{1}{B}");
  this.expansionSetCode="TMP";
  this.subtype.add("Rat");
  this.power=new MageInt(2);
  this.toughness=new MageInt(1);
  Effect effect=new DestroyTargetEffect();
  effect.setOutcome(Outcome.AIDontUseIt);
  Ability ability=new SimpleActivatedAbility(Zone.BATTLEFIELD,effect,new ColoredManaCost(ColoredManaSymbol.B));
  ability.addTarget(new TargetControlledPermanent(filter));
  this.addAbility(ability);
}
