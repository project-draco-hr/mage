{
  if (isConnected()) {
    try {
      server.deregisterClient(sessionId);
      server=null;
      logger.info("Disconnected ... ");
    }
 catch (    RemoteException ex) {
      logger.log(Level.SEVERE,"Error disconnecting ...",ex);
    }
catch (    MageException ex) {
      logger.log(Level.SEVERE,"Error disconnecting ...",ex);
    }
  }
}
