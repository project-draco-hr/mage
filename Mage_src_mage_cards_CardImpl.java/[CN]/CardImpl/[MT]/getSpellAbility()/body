{
  if (spellAbility == null) {
    for (    Ability ability : abilities.getActivatedAbilities(Zone.HAND)) {
      if (ability instanceof SpellAbility && !((SpellAbility)ability).getSpellAbilityType().equals(SpellAbilityType.BASE_ALTERNATE)) {
        return spellAbility=(SpellAbility)ability;
      }
    }
  }
  return spellAbility;
}
