{
  try {
    List<String> rules=getRules();
    CardState state=game.getState().getCardState(objectId);
    for (    String data : state.getInfo().values()) {
      rules.add(data);
    }
    return rules;
  }
 catch (  Exception e) {
    logger.error("Exception in rules generation for card: " + this.getName(),e);
  }
  return rulesError;
}
