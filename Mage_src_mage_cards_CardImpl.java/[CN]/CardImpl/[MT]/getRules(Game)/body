{
  try {
    List<String> rules=getRules();
    if (game != null) {
      CardState cardState=game.getState().getCardState(objectId);
      if (cardState != null) {
        for (        String data : cardState.getInfo().values()) {
          rules.add(data);
        }
      }
    }
    return rules;
  }
 catch (  Exception e) {
    logger.error("Exception in rules generation for card: " + this.getName(),e);
  }
  return rulesError;
}
