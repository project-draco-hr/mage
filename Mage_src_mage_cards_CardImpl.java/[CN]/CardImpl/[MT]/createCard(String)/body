{
  try {
    Class<?> theClass=Class.forName(name);
    Constructor<?> con=theClass.getConstructor(new Class[]{UUID.class});
    Card card=(Card)con.newInstance(new Object[]{null});
    card.setZone(Zone.OUTSIDE);
    return card;
  }
 catch (  Exception e) {
    e.printStackTrace();
    return null;
  }
}
