{
  this(ownerId,name);
  this.rarity=rarity;
  this.cardNumber=cardNumber;
  for (  CardType newCardType : cardTypes)   this.cardType.add(newCardType);
  this.manaCost.load(costs);
  if (cardType.contains(CardType.LAND))   addAbility(new PlayLandAbility(name));
 else   addAbility(new SpellAbility(manaCost,name));
}
