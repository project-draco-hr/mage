{
  Permanent permanent=game.getPermanent(event.getTargetId());
  if (permanent.getCardType().contains(CardType.CREATURE) && permanent.getControllerId().equals(this.controllerId) && event.getTargetId() != this.getSourceId()) {
    Effect effect=this.getEffects().get(0);
    effect.setValue("lifeSource",event.getTargetId());
    effect.setValue("zoneChangeCounter",permanent.getZoneChangeCounter(game));
    return true;
  }
  return false;
}
