{
  this.reloaded=true;
  cardArea.removeAll();
  if (showCards != null && showCards.size() < 10) {
    yTextOffset=10;
    loadCardsFew(showCards,bigCard,gameId,listener);
  }
 else {
    yTextOffset=0;
    loadCardsMany(showCards,bigCard,gameId,listener,dimension);
  }
  cardArea.revalidate();
  this.revalidate();
  this.repaint();
}
