{
  super(ownerId,214,"Iroas's Champion",Rarity.UNCOMMON,new CardType[]{CardType.CREATURE},"{1}{R}{W}");
  this.expansionSetCode="ORI";
  this.subtype.add("Human");
  this.subtype.add("Soldier");
