{
  super(ownerId,setInfo,new CardType[]{CardType.ARTIFACT},"{4}");
  SimpleActivatedAbility ability=new SimpleActivatedAbility(Zone.BATTLEFIELD,new UntapTargetEffect(),new TapTargetCost(new TargetControlledPermanent(2,2,filter,true)));
  ability.addTarget(new TargetArtifactPermanent());
  this.addAbility(ability);
}
