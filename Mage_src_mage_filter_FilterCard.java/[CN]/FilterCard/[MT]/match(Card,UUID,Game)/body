{
  if (!this.match(card))   return notFilter;
  if (owner != TargetController.ANY && playerId != null) {
switch (owner) {
case YOU:
      if (!card.getOwnerId().equals(playerId))       return notFilter;
    break;
case OPPONENT:
  if (!game.getOpponents(playerId).contains(card.getOwnerId()))   return notFilter;
break;
case NOT_YOU:
if (card.getOwnerId().equals(playerId)) return notFilter;
break;
}
}
return !notFilter;
}
