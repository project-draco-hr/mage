{
  super(ownerId,setInfo,new CardType[]{CardType.ARTIFACT,CardType.CREATURE},"{2}");
  this.subtype.add("snake");
  this.power=new MageInt(2);
  this.toughness=new MageInt(1);
  this.addAbility(new SimpleActivatedAbility(Zone.BATTLEFIELD,new GainAbilitySourceEffect(DeathtouchAbility.getInstance(),Duration.EndOfTurn),new ColoredManaCost(ColoredManaSymbol.G)));
}
