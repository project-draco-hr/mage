{
  super(ownerId,69,"Molten Frame",Rarity.COMMON,new CardType[]{CardType.INSTANT},"{1}{R}");
  this.expansionSetCode="CON";
  this.color.setRed(true);
  this.getSpellAbility().addEffect(new DestroyTargetEffect());
