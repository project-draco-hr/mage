{
  super(ownerId,245,"Wilt-Leaf Liege",Rarity.RARE,new CardType[]{CardType.CREATURE},"{1}{G/W}{G/W}{G/W}");
  this.expansionSetCode="SHM";
  this.subtype.add("Elf");
