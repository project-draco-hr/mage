{
  super(ownerId,setInfo,new CardType[]{CardType.ENCHANTMENT},"{W}");
  this.addAbility(new SimpleStaticAbility(Zone.BATTLEFIELD,new AuthorityOfTheConsulsTapEffect()));
  this.addAbility(new EntersBattlefieldAllTriggeredAbility(new GainLifeEffect(1),filter,"Whenever a creature enters the battlefield under an opponent's control, you gain 1 life."));
}
