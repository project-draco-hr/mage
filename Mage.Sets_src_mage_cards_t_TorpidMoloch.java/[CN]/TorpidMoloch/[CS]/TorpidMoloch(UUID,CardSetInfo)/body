{
  super(ownerId,setInfo,new CardType[]{CardType.CREATURE},"{R}");
  this.subtype.add("Lizard");
  this.power=new MageInt(3);
  this.toughness=new MageInt(2);
  this.addAbility(DefenderAbility.getInstance());
  this.addAbility(new SimpleActivatedAbility(Zone.BATTLEFIELD,new LoseAbilitySourceEffect(DefenderAbility.getInstance(),Duration.EndOfTurn),new SacrificeTargetCost(new TargetControlledPermanent(3,3,new FilterControlledLandPermanent("three lands"),true))));
}
