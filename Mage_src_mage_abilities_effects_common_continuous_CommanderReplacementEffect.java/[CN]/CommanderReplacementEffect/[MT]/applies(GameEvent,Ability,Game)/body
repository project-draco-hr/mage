{
switch (((ZoneChangeEvent)event).getToZone()) {
case HAND:
    if (!alsoHand) {
      return false;
    }
case GRAVEYARD:
case EXILED:
case LIBRARY:
  if (commanderId.equals(event.getTargetId())) {
    return true;
  }
break;
case STACK:
Spell spell=game.getStack().getSpell(event.getTargetId());
if (spell != null) {
if (commanderId.equals(spell.getSourceId())) {
return true;
}
}
break;
}
return false;
}
