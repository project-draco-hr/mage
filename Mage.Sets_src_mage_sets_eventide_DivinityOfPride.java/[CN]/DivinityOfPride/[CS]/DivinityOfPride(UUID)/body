{
  super(ownerId,86,"Divinity of Pride",Rarity.RARE,new CardType[]{CardType.CREATURE},"{W/B}{W/B}{W/B}{W/B}{W/B}");
  this.expansionSetCode="EVE";
  this.subtype.add("Spirit");
  this.subtype.add("Avatar");
  this.color.setBlack(true);
  this.color.setWhite(true);
  this.power=new MageInt(4);
  this.toughness=new MageInt(4);
  this.addAbility(FlyingAbility.getInstance());
  this.addAbility(LifelinkAbility.getInstance());
  this.addAbility(new SimpleStaticAbility(Zone.BATTLEFIELD,new ConditionalContinuousEffect(new BoostSourceEffect(4,4,Duration.WhileOnBattlefield),new DivinityOfPrideCondition(),"Divinity of Pride gets +4/+4 as long as you have 25 or more life")));
}
