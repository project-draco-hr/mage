{
  Set<UUID> dependentTo=null;
  for (  ContinuousEffect effect : allEffectsInLayer) {
    if (MagusOfTheMoon.class.equals(effect.getClass().getEnclosingClass())) {
      if (dependentTo == null) {
        dependentTo=new HashSet<>();
      }
      dependentTo.add(effect.getId());
    }
  }
  return dependentTo;
}
