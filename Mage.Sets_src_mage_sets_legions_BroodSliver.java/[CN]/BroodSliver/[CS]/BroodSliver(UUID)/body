{
  super(ownerId,120,"Brood Sliver",Rarity.RARE,new CardType[]{CardType.CREATURE},"{4}{G}");
  this.expansionSetCode="LGN";
  this.subtype.add("Sliver");
  this.color.setGreen(true);
  this.power=new MageInt(3);
  this.toughness=new MageInt(3);
  Effect effect=new CreateTokenTargetEffect(new SliverToken());
  effect.setText("its controller may put a 1/1 colorless Sliver creature token onto the battlefield");
  this.addAbility(new DealsDamageToAPlayerAllTriggeredAbility(effect,new FilterCreaturePermanent("Sliver","a Sliver"),true,SetTargetPointer.PLAYER,true));
}
