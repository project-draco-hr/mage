{
  if (game.getTurn().getStepType() == Constants.PhaseStep.UNTAP && event.getType() == GameEvent.EventType.UNTAP) {
    Player player=game.getPlayer(event.getPlayerId());
    Permanent permanent=game.getPermanent(event.getTargetId());
    if (player != null && game.getActivePlayerId().equals(event.getPlayerId()) && permanent != null && permanent.getCardType().contains(CardType.CREATURE)) {
      return true;
    }
  }
  return false;
}
