{
  if (rule != null && !rule.isEmpty()) {
    return rule;
  }
  StringBuilder sb=new StringBuilder("Whenever ").append(filter.getMessage());
  sb.append(" enters the battlefield");
  if (controlled) {
    sb.append("under your control,");
  }
 else {
    sb.append(",");
  }
  sb.append(super.getRule());
  return sb.toString();
}
