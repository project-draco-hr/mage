{
  if (gameCard.isFaceDown()) {
    return;
  }
  if (!popupShowing) {
synchronized (this) {
      if (!popupShowing) {
        TransferData transferData=getTransferDataForMouseEntered();
        if (this.isShowing()) {
          popupShowing=true;
          callback.mouseEntered(e,transferData);
        }
      }
    }
  }
}
