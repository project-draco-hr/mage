{
  super(ownerId,56,"Deep-Sea Kraken",Rarity.RARE,new CardType[]{CardType.CREATURE},"{7}{U}{U}{U}");
  this.expansionSetCode="TSP";
  this.subtype.add("Kraken");
  this.color.setBlue(true);
  this.power=new MageInt(6);
  this.toughness=new MageInt(6);
  this.addAbility(new UnblockableAbility());
  this.addAbility(new SuspendAbility(9,new ManaCostsImpl("{2}{U}"),this));
  this.addAbility(new ConditionalTriggeredAbility(new SpellCastAllTriggeredAbility(new RemoveCounterSourceEffect(CounterType.TIME.createInstance()),filter,false),SuspendedCondition.getInstance(),"Whenever an opponent casts a spell, if Deep-Sea Kraken is suspended, remove a time counter from it.",false));
}
