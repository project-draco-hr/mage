{
  super(ownerId,204,"Shocker",Rarity.RARE,new CardType[]{CardType.CREATURE},"{1}{R}");
  this.expansionSetCode="TMP";
  this.subtype.add("Insect");
  this.power=new MageInt(1);
  this.toughness=new MageInt(1);
