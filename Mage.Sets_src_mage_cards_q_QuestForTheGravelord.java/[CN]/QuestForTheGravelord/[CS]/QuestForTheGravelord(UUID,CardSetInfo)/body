{
  super(ownerId,setInfo,new CardType[]{CardType.ENCHANTMENT},"{B}");
  this.addAbility(new DiesCreatureTriggeredAbility(new AddCountersSourceEffect(CounterType.QUEST.createInstance()),true));
  SimpleActivatedAbility ability=new SimpleActivatedAbility(Zone.BATTLEFIELD,new CreateTokenEffect(new ZombieToken()),new RemoveCountersSourceCost(CounterType.QUEST.createInstance(3)));
  ability.addCost(new SacrificeSourceCost());
  this.addAbility(ability);
}
