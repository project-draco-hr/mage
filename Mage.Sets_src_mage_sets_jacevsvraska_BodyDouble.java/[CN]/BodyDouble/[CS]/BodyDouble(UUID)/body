{
  super(ownerId,15,"Body Double",Rarity.RARE,new CardType[]{CardType.CREATURE},"{4}{U}");
  this.expansionSetCode="DDM";
  this.subtype.add("Shapeshifter");
  this.color.setBlue(true);
  this.power=new MageInt(0);
  this.toughness=new MageInt(0);
  Ability ability=new SimpleStaticAbility(Zone.BATTLEFIELD,new EntersBattlefieldEffect(new BodyDoubleCopyEffect(),"You may have {this} enter the battlefield as a copy of any creature card in a graveyard",true));
  this.addAbility(ability);
}
