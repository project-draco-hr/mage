{
  super(ownerId,222,"Quilled Wolf",Rarity.COMMON,new CardType[]{CardType.CREATURE},"{1}{G}");
  this.expansionSetCode="SOI";
  this.subtype.add("Wolf");
  this.power=new MageInt(2);
  this.toughness=new MageInt(2);
