{
  super(ownerId,118,"Goblin War Drums",Rarity.COMMON,new CardType[]{CardType.ENCHANTMENT},"{2}{R}");
  this.expansionSetCode="FEM";
  this.color.setRed(true);
  this.addAbility(new SimpleStaticAbility(Constants.Zone.BATTLEFIELD,new GainAbilityAllEffect(new SimpleStaticAbility(Constants.Zone.BATTLEFIELD,new CantBeBlockedByOneEffect(2)),Constants.Duration.WhileOnBattlefield,new FilterControlledCreaturePermanent(),"Each creature you control can't be blocked except by two or more creatures")));
}
