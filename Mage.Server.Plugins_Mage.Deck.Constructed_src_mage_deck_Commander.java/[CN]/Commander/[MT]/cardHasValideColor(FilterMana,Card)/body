{
  FilterMana cardColor=CardUtil.getColorIdentity(card);
  if (cardColor.isBlack() && !commander.isBlack() || cardColor.isBlue() && !commander.isBlue() || cardColor.isGreen() && !commander.isGreen() || cardColor.isRed() && !commander.isRed() || cardColor.isWhite() && !commander.isWhite()) {
    return false;
  }
  return true;
}
