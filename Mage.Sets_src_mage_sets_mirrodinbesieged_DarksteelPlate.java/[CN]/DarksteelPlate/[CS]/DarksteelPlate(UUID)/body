{
  super(ownerId,104,"Darksteel Plate",Rarity.RARE,new CardType[]{CardType.ARTIFACT},"{3}");
  this.expansionSetCode="MBS";
  this.subtype.add("Equipment");
  this.addAbility(IndestructibleAbility.getInstance());
  this.addAbility(new SimpleStaticAbility(Zone.BATTLEFIELD,new GainAbilityAttachedEffect(IndestructibleAbility.getInstance(),AttachmentType.EQUIPMENT,Constants.Duration.WhileOnBattlefield)));
  this.addAbility(new EquipAbility(Outcome.BoostCreature,new GenericManaCost(2)));
}
