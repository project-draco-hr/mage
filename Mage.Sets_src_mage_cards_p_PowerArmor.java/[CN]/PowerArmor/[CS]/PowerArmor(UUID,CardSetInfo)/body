{
  super(ownerId,setInfo,new CardType[]{CardType.ARTIFACT},"{4}");
  Ability ability=new SimpleActivatedAbility(Zone.BATTLEFIELD,new BoostTargetEffect(new DomainValue(),new DomainValue(),Duration.EndOfTurn),new TapSourceCost());
