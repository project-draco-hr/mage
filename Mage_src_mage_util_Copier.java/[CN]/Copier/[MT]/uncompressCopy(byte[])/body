{
  T copy=null;
  try {
    ObjectInputStream in=new CopierObjectInputStream(loader,new GZIPInputStream(new ByteArrayInputStream(buffer)));
    copy=(T)in.readObject();
  }
 catch (  IOException e) {
    e.printStackTrace();
  }
catch (  ClassNotFoundException cnfe) {
    cnfe.printStackTrace();
  }
  return copy;
}
