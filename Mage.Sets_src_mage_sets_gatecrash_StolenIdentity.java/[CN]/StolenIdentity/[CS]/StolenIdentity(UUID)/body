{
  super(ownerId,53,"Stolen Identity",Rarity.RARE,new CardType[]{CardType.SORCERY},"{4}{U}{U}");
  this.expansionSetCode="GTC";
  this.color.setBlue(true);
  this.getSpellAbility().addEffect(new StolenIdentityEffect());
