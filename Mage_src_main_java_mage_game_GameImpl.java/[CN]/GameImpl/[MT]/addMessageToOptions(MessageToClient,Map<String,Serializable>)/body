{
  if (message.getSecondMessage() != null) {
    if (options == null) {
      options=new HashMap<>();
    }
    options.put("secondMessage",message.getSecondMessage());
  }
  if (message.getHintText() != null) {
    if (options == null) {
      options=new HashMap<>();
    }
    options.put("hintText",message.getHintText());
  }
  return options;
}
