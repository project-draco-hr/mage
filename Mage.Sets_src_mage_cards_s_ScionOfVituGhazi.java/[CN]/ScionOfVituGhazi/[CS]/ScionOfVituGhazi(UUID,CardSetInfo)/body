{
  super(ownerId,setInfo,new CardType[]{CardType.CREATURE},"{3}{W}{W}");
  this.subtype.add("Elemental");
  this.power=new MageInt(4);
  this.toughness=new MageInt(4);
  TriggeredAbility ability=new EntersBattlefieldTriggeredAbility(new CreateTokenEffect(new BirdToken()),false);
  ability.addEffect(new PopulateEffect("then"));
  this.addAbility(new ConditionalTriggeredAbility(ability,new CastFromHandSourceCondition(),"When {this} enters the battlefield, if you cast it from your hand, put a 1/1 white Bird creature token with flying onto the battlefield, then populate."),new CastFromHandWatcher());
}
