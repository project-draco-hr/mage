{
  super(ownerId,203,"Overgrown Battlement",Rarity.COMMON,new CardType[]{CardType.CREATURE},"{1}{G}");
  this.expansionSetCode="ROE";
  this.subtype.add("Wall");
  this.power=new MageInt(0);
  this.toughness=new MageInt(4);
