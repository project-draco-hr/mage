{
  super(ownerId,141,"Viscid Lemures",Rarity.COMMON,new CardType[]{CardType.CREATURE},"{4}{B}");
  this.expansionSetCode="TSP";
  this.subtype.add("Spirit");
  this.power=new MageInt(4);
  this.toughness=new MageInt(3);
  Ability ability=new SimpleActivatedAbility(Zone.BATTLEFIELD,new GainAbilitySourceEffect(new SwampwalkAbility(),Duration.EndOfTurn),new GenericManaCost(0));
  ability.addEffect(new BoostSourceEffect(-1,0,Duration.EndOfTurn));
  this.addAbility(ability);
}
