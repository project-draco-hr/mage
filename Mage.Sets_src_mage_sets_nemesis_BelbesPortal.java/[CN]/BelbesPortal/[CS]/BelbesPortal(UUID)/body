{
  super(ownerId,127,"Belbe's Portal",Rarity.RARE,new CardType[]{CardType.ARTIFACT},"{5}");
  this.expansionSetCode="NMS";
  this.addAbility(new AsEntersBattlefieldAbility(new BelbesPortalEnterBattlefieldEffect()));
  SimpleActivatedAbility ability=new SimpleActivatedAbility(Zone.BATTLEFIELD,new BelbesPortalPutCreatureOnBattlefieldEffect(),new ManaCostsImpl("{3}"));
  ability.addCost(new TapSourceCost());
  this.addAbility(ability);
}
