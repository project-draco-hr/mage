{
  super(ownerId,68,"Helix Pinnacle",Rarity.RARE,new CardType[]{CardType.ENCHANTMENT},"{G}");
  this.expansionSetCode="EVE";
  this.color.setGreen(true);
  this.addAbility(ShroudAbility.getInstance());
  this.addAbility(new SimpleActivatedAbility(Zone.BATTLEFIELD,new AddCountersSourceEffect(CounterType.TOWER.createInstance(),new ManacostVariableValue(),true),new ManaCostsImpl("{X}")));
  this.addAbility(new ConditionalTriggeredAbility(new BeginningOfUpkeepTriggeredAbility(Zone.BATTLEFIELD,new WinGameEffect(),TargetController.YOU,false),new SourceHasCounterCondition(CounterType.TOWER,100),rule,false));
}
