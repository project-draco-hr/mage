{
  Player controller=game.getPlayer(source.getControllerId());
  if (controller != null) {
    Card spellCard=game.getStack().getSpell(source.getSourceId()).getCard();
    if (spellCard != null) {
      controller.moveCards(spellCard,null,Zone.EXILED,source,game);
    }
    return true;
  }
  return false;
}
