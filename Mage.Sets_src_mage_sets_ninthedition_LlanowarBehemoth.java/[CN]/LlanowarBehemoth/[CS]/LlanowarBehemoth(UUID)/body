{
  super(ownerId,252,"Llanowar Behemoth",Rarity.UNCOMMON,new CardType[]{CardType.CREATURE},"{3}{G}{G}");
  this.expansionSetCode="9ED";
  this.subtype.add("Elemental");
  this.color.setGreen(true);
  this.power=new MageInt(4);
  this.toughness=new MageInt(4);
  this.addAbility(new SimpleActivatedAbility(Constants.Zone.BATTLEFIELD,new BoostSourceEffect(1,1,Constants.Duration.EndOfTurn),new TapTargetCost(new TargetControlledCreaturePermanent(1,1,filter,false))));
}
