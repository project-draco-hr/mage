{
  super(ownerId,setInfo,new CardType[]{CardType.CREATURE},"{2}{W}");
  this.subtype.add("Human");
  this.subtype.add("Trooper");
  this.power=new MageInt(2);
  this.toughness=new MageInt(2);
  this.addAbility(new SimpleStaticAbility(Zone.BATTLEFIELD,new GainAbilityControlledEffect(FirstStrikeAbility.getInstance(),Duration.WhileOnBattlefield,new FilterCreaturePermanent("Trooper","Trooper creatures"),false)));
}
