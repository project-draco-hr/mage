{
  super(ownerId,112,"Feral Animist",Rarity.UNCOMMON,new CardType[]{CardType.CREATURE},"{1}{R}{G}");
  this.expansionSetCode="GPT";
  this.subtype.add("Goblin");
  this.subtype.add("Shaman");
