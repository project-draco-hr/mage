{
  super(ownerId,219,"Spinneret Sliver",Rarity.COMMON,new CardType[]{CardType.CREATURE},"{1}{G}");
  this.expansionSetCode="TSP";
  this.subtype.add("Sliver");
  this.power=new MageInt(2);
  this.toughness=new MageInt(2);
  this.addAbility(new SimpleStaticAbility(Zone.BATTLEFIELD,new GainAbilityAllEffect(ReachAbility.getInstance(),Duration.WhileOnBattlefield,filter,"All Sliver creatures have reach.")));
}
