{
  super(ownerId,18,"Spelltithe Enforcer",Rarity.RARE,new CardType[]{CardType.CREATURE},"{3}{W}{W}");
  this.expansionSetCode="GPT";
  this.subtype.add("Elephant");
  this.subtype.add("Wizard");
