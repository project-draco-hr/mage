  this.toughness=new MageInt(3);
  this.addAbility(new SpellCastOpponentTriggeredAbility(Zone.BATTLEFIELD,new SpelltitheEnforcerEffect(),new FilterSpell(),false,SetTargetPointer.PLAYER));
}
