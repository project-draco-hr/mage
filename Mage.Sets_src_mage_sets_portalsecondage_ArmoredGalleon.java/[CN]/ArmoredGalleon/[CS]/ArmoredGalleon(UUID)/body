{
  super(ownerId,33,"Armored Galleon",Rarity.UNCOMMON,new CardType[]{CardType.CREATURE},"{4}{U}");
  this.expansionSetCode="PO2";
  this.subtype.add("Human");
  this.subtype.add("Pirate");
