{
  super(ownerId,203,"Valeron Wardens",Rarity.UNCOMMON,new CardType[]{CardType.CREATURE},"{2}{G}");
  this.expansionSetCode="ORI";
  this.subtype.add("Human");
  this.subtype.add("Monk");
