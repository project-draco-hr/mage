{
  if (this.size() > 0) {
    Iterator<DelayedTriggeredAbility> it=this.iterator();
    while (it.hasNext()) {
      DelayedTriggeredAbility ability=it.next();
      if (ability.checkTrigger(event,game)) {
        ability.trigger(game,ability.controllerId);
        it.remove();
      }
    }
  }
}
