{
  StringBuilder sb=new StringBuilder();
  if (attachmentType == AttachmentType.AURA)   sb.append("Enchanted");
 else   if (attachmentType == AttachmentType.EQUIPMENT)   sb.append("Equipped");
  sb.append(" creature becomes ").append(addedCardType.toString()).append(" in addition to its other types");
  staticText=sb.toString();
}
