  this.color.setGreen(true);
  this.color.setBlack(true);
  this.power=new MageInt(2);
  this.toughness=new MageInt(5);
  this.addAbility(new EntersBattlefieldTriggeredAbility(new SultaiSoothsayerEffect(),false));
}
