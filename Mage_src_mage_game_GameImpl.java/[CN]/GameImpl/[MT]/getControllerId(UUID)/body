{
  if (objectId == null) {
    return null;
  }
  MageObject object=getObject(objectId);
  if (object != null) {
    if (object instanceof Permanent) {
      return ((Permanent)object).getControllerId();
    }
    if (object instanceof Card) {
      return ((Card)object).getOwnerId();
    }
  }
  return null;
}
