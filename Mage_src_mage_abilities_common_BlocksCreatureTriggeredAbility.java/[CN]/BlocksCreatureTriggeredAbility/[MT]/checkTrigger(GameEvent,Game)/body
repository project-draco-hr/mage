{
  if (event.getType() == EventType.BLOCKER_DECLARED && event.getSourceId().equals(this.getSourceId())) {
    if (setTargetPointer) {
      for (      Effect effect : this.getEffects()) {
        effect.setTargetPointer(new FixedTarget(event.getTargetId()));
      }
    }
    return true;
  }
  return false;
}
