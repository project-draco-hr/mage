{
  int notches=e.getWheelRotation();
  if (!enlargedWindowState.equals(EnlargedWindowState.CLOSED)) {
    if (enlargeMode.equals(EnlargeMode.NORMAL)) {
      if (notches > 0) {
        hideEnlargedCard();
      }
    }
 else {
      if (notches < 0) {
        hideEnlargedCard();
      }
    }
    return;
  }
  if (notches < 0) {
    enlargeCard(EnlargeMode.NORMAL);
  }
 else {
    enlargeCard(EnlargeMode.ALTERNATE);
  }
}
