{
  this.popupCard=null;
  if (popup != null) {
    popup.hide();
  }
  if (jPopupMenu != null) {
    jPopupMenu.setVisible(false);
  }
  try {
    if (session == null) {
      return;
    }
    Component popupContainer=session.getUI().getComponent(MageComponents.POPUP_CONTAINER);
    popupContainer.setVisible(false);
  }
 catch (  Exception e2) {
    e2.printStackTrace();
  }
}
