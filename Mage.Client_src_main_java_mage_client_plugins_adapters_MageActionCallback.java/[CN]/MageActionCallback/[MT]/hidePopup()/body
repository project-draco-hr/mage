{
  this.popupCard=null;
  if (popup != null) {
    popup.hide();
  }
  if (jPopupMenu != null) {
    jPopupMenu.setVisible(false);
  }
  try {
    Component popup2=session.getUI().getComponent(MageComponents.CARD_INFO_PANE);
    popup2.setVisible(false);
  }
 catch (  Exception e2) {
    e2.printStackTrace();
