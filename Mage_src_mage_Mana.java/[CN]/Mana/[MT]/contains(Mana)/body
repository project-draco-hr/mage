{
  if (mana.black > 0 && black > 0) {
    return true;
  }
  if (mana.blue > 0 && blue > 0) {
    return true;
  }
  if (mana.red > 0 && red > 0) {
    return true;
  }
  if (mana.white > 0 && white > 0) {
    return true;
  }
  if (mana.green > 0 && green > 0) {
    return true;
  }
  if (mana.colorless > 0 && count() > 0) {
    return true;
  }
  return false;
}
