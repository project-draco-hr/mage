{
  int notches=e.getWheelRotation();
  int index=rows.getSelectedIndex();
  if (notches < 0) {
    if (index > 0) {
      rows.setSelectedIndex(index - 1);
      rows.repaint();
    }
  }
 else {
    if (index < choices.size() - 1) {
      rows.setSelectedIndex(index + 1);
      rows.repaint();
    }
  }
}
