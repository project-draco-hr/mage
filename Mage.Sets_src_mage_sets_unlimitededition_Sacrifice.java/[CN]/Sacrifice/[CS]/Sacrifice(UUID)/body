{
  super(ownerId,33,"Sacrifice",Rarity.UNCOMMON,new CardType[]{CardType.INSTANT},"{B}");
  this.expansionSetCode="2ED";
  this.getSpellAbility().addCost(new SacrificeTargetCost(new TargetControlledCreaturePermanent()));
  this.getSpellAbility().addEffect(new DynamicManaEffect(Mana.BlackMana,new SacrificeCostConvertedMana("creature"),"add to your mana pool an amount of {B} equal to the sacrificed creature's converted mana cost"));
}
