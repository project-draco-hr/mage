{
  MageObject object=game.getObject(targetId);
  String targetNames=null;
  if (object == null) {
    Player targetPlayer=game.getPlayer(targetId);
    if (targetPlayer != null) {
      targetNames=targetPlayer.getLogName();
    }
  }
 else {
    targetNames=object.getName();
  }
  return targetNames;
}
