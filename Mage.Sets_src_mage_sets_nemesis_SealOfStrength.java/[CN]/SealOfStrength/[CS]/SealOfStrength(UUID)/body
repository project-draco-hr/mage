{
  super(ownerId,115,"Seal of Strength",Rarity.COMMON,new CardType[]{CardType.ENCHANTMENT},"{G}");
  this.expansionSetCode="NMS";
  this.color.setGreen(true);
  Ability ability=new SimpleActivatedAbility(Constants.Zone.BATTLEFIELD,new BoostTargetEffect(3,3,Constants.Duration.EndOfTurn),new SacrificeSourceCost());
  ability.addTarget(new TargetCreaturePermanent());
  this.addAbility(ability);
}
