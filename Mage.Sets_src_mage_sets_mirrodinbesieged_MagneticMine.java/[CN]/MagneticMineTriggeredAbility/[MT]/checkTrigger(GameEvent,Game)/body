{
  if (event.getType() == EventType.ZONE_CHANGE) {
    ZoneChangeEvent zEvent=(ZoneChangeEvent)event;
    if (zEvent.getFromZone() == Zone.BATTLEFIELD && zEvent.getToZone() == Zone.GRAVEYARD && zEvent.getTarget().getCardType().contains(CardType.ARTIFACT) && zEvent.getTarget().getId() != this.getSourceId()) {
      this.getTargets().get(0).add(zEvent.getTarget().getControllerId(),game);
      return true;
    }
  }
  return false;
}
