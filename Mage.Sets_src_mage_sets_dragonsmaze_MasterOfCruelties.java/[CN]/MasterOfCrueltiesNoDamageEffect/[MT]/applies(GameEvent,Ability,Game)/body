{
switch (event.getType()) {
case DAMAGE_CREATURE:
case DAMAGE_PLAYER:
case DAMAGE_PLANESWALKER:
    DamageEvent damageEvent=(DamageEvent)event;
  if (event.getSourceId().equals(source.getSourceId()) && damageEvent.isCombatDamage()) {
    return true;
  }
return event.getFlag();
default :
return false;
}
}
