{
  try {
    call.setMessageId(messageId++);
    callbackHandler.handleCallbackOneway(new Callback(call));
  }
 catch (  HandleCallbackException ex) {
    logger.info("CALLBACK EXCEPTION - userId " + userId,ex);
    if (logger.isDebugEnabled()) {
      ex.printStackTrace();
    }
    userLostConnection();
  }
}
