{
  if (artImage != null) {
    int imgWidth=artImage.getWidth();
    int imgHeight=artImage.getHeight();
    BufferedImage subImg=artImage.getSubimage((int)(.079 * imgWidth),(int)(.11 * imgHeight),(int)(.84 * imgWidth),(int)(.42 * imgHeight));
    g.drawImage(subImg,totalContentInset + 1,totalContentInset + boxHeight,contentWidth - 2,typeLineY - totalContentInset - boxHeight,null);
  }
}
