{
  Rectangle2D rect;
  if (useInventionFrame()) {
    rect=new Rectangle2D.Float(0,0,1,1);
  }
 else   if (cardView.getFrameStyle().isFullArt() || cardView.isToken()) {
    rect=new Rectangle2D.Float(.079f,.11f,.84f,.63f);
  }
 else {
    rect=new Rectangle2D.Float(.079f,.11f,.84f,.42f);
  }
  return rect;
}
