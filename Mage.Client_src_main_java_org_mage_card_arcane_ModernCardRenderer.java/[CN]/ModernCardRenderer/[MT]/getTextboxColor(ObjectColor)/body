{
  if (color.isWhite()) {
    return TEXTBOX_WHITE;
  }
 else   if (color.isBlue()) {
    return TEXTBOX_BLUE;
  }
 else   if (color.isBlack()) {
    return TEXTBOX_BLACK;
  }
 else   if (color.isRed()) {
    return TEXTBOX_RED;
  }
 else   if (color.isGreen()) {
    return TEXTBOX_GREEN;
  }
 else {
    return ERROR_COLOR;
  }
}
