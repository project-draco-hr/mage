  this.toughness=new MageInt(1);
  this.flipCard=true;
  this.flipCardName="Erayo's Essence";
  this.addAbility(FlyingAbility.getInstance());
  this.addAbility(new ErayoSoratamiAscendantTriggeredAbility());
}
