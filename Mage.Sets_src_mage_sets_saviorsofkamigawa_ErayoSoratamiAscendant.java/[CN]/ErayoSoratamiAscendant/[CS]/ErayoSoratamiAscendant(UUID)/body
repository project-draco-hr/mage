{
  super(ownerId,35,"Erayo, Soratami Ascendant",Rarity.RARE,new CardType[]{CardType.CREATURE},"{1}{U}");
  this.expansionSetCode="SOK";
  this.supertype.add("Legendary");
  this.subtype.add("Moonfolk");
  this.subtype.add("Monk");
